                  <description>
                    <text>AES-GCM 128-bit encryption algorithm</text>
                  </description>
                </enum>
                <enum name="aes-256-gcm">
                  <description>
                    <text>AES-GCM 256-bit encryption algorithm</text>
                  </description>
