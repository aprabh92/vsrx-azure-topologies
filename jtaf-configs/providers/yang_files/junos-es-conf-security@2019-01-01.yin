  <grouping name="custom-attack-type">
    <description>
      <text>Define custom attack</text>
    </description>
    <leaf name="name">
      <description>
        <text>Custom attack name</text>
      </description>
      <type name="string"/>
    </leaf>
    <uses name="apply-advanced"/>
    <leaf-list name="attack-description">
      <ordered-by value="user"/>
      <description>
        <text>Attack description</text>
      </description>
      <type name="string"/>
    </leaf-list>
    <leaf name="recommended-action">
      <description>
        <text>Recommended Action</text>
      </description>
      <type name="enumeration">
        <enum name="none">
          <description>
            <text>No action</text>
          </description>
        </enum>
        <enum name="ignore">
          <description>
            <text>No further inspection on the flow</text>
          </description>
        </enum>
        <enum name="drop-packet">
          <description>
            <text>Drop the packet</text>
          </description>
        </enum>
        <enum name="drop">
          <description>
            <text>Drop the packet and subsequent packets of the flow</text>
          </description>
        </enum>
        <enum name="close-client">
          <description>
            <text>Reset the client side</text>
          </description>
        </enum>
        <enum name="close-server">
          <description>
            <text>Reset the server side</text>
          </description>
        </enum>
        <enum name="close">
          <description>
            <text>Reset both client and server</text>
          </description>
        </enum>
      </type>
    </leaf>
    <leaf name="severity">
      <description>
        <text>Select the severity that matches the lethality of this attack on your network</text>
      </description>
      <type name="enumeration">
        <enum name="info">
          <description>
            <text>Provide information of attack when it matches.</text>
          </description>
        </enum>
        <enum name="warning">
          <description>
            <text>Issue a warning when attack matches.</text>
          </description>
        </enum>
        <enum name="minor">
          <description>
            <text>The attack is a  minor one.</text>
          </description>
        </enum>
        <enum name="major">
          <description>
            <text>The attack is a major one.</text>
          </description>
        </enum>
        <enum name="critical">
          <description>
            <text>The attack is a critical one.</text>
          </description>
        </enum>
      </type>
    </leaf>
    <container name="time-binding">
      <description>
        <text>Time binding params</text>
      </description>
      <leaf name="count">
        <description>
          <text>Number of times this attack is to be triggered</text>
        </description>
        <type name="union">
          <type name="uint32"/>
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
        </type>
      </leaf>
      <leaf name="scope">
        <description>
          <text>Scope within which the count occurs</text>
        </description>
        <type name="enumeration">
          <enum name="peer">
            <description>
              <text>If the attack is between a single source and single destination</text>
            </description>
          </enum>
          <enum name="source">
            <description>
              <text>If the attack is from one source and multiple destination</text>
            </description>
          </enum>
          <enum name="destination">
            <description>
              <text>If the attack is from multiple sources to one destination</text>
            </description>
          </enum>
        </type>
      </leaf>
      <leaf name="interval">
        <description>
          <text>Maximum time-gap between two instances of the attack. Format : MMm-SSs</text>
        </description>
        <default value="01m-00s"/>
        <type name="string"/>
      </leaf>
    </container>
    <container name="detection-filter">
      <description>
        <text>Detection filter params</text>
      </description>
      <leaf name="count">
        <description>
          <text>Number of matches for this attack to be triggered. Must be greater than 0</text>
        </description>
        <type name="union">
          <type name="uint32"/>
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
        </type>
      </leaf>
      <leaf name="scope">
        <description>
          <text>Scope within which the count occurs</text>
        </description>
        <type name="enumeration">
          <enum name="session">
            <description>
              <text>If the attack is between a single source and single destination</text>
            </description>
          </enum>
          <enum name="source">
            <description>
              <text>If the attack is from one source and multiple destination</text>
            </description>
          </enum>
          <enum name="destination">
            <description>
              <text>If the attack is from multiple sources to one destination</text>
            </description>
          </enum>
        </type>
      </leaf>
      <leaf name="interval">
        <description>
          <text>Time period over which count is accrued. Format : MMm-SSs. Minimum value is 1 second</text>
        </description>
        <type name="string"/>
      </leaf>
    </container>
    <container name="attack-type">
      <description>
        <text>Type of attack</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="signature">
        <description>
          <text>Signature based attack</text>
        </description>
        <uses name="apply-advanced"/>
        <container name="protocol-binding">
          <description>
            <text>Protocol binding over which attack will be detected</text>
          </description>
          <uses name="apply-advanced"/>
          <choice name="protocol">
            <case name="case_1">
              <container name="tcp">
                <presence value="enable tcp"/>
                <description>
                  <text>Attack is for TCP packets only</text>
                </description>
                <uses name="apply-advanced"/>
                <list name="minimum-port">
                  <key value="name"/>
                  <ordered-by value="user"/>
                  <description>
                    <text>Multiple sets of (single port/port ranges) can be specified</text>
                  </description>
                  <uses name="port-range"/>
                </list>
              </container>
            </case>
            <case name="case_2">
              <container name="udp">
                <presence value="enable udp"/>
                <description>
                  <text>Attack is for UDP packets only</text>
                </description>
                <uses name="apply-advanced"/>
                <list name="minimum-port">
                  <key value="name"/>
                  <ordered-by value="user"/>
                  <description>
                    <text>Either single port or port ranges can be specified</text>
                  </description>
                  <uses name="port-range"/>
                </list>
              </container>
            </case>
            <case name="case_3">
              <container name="rpc">
                <presence value="enable rpc"/>
                <description>
                  <text>Attack is for RPC packets only</text>
                </description>
                <uses name="apply-advanced"/>
                <leaf name="program-number">
                  <description>
                    <text>RPC Program Number</text>
                  </description>
                  <type name="union">
                    <type name="uint32"/>
                    <type name="string">
                      <pattern value="&lt;.*&gt;|$.*"/>
                    </type>
                  </type>
                </leaf>
              </container>
            </case>
            <case name="case_4">
              <leaf name="icmp">
                <description>
                  <text>Attack is for ICMP packets only</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
            <case name="case_5">
              <leaf name="icmpv6">
                <description>
                  <text>Attack is for ICMPv6 packets only</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
            <case name="case_6">
              <container name="ip">
                <presence value="enable ip"/>
                <description>
                  <text>Attack is for all IP based packets</text>
                </description>
                <uses name="apply-advanced"/>
                <leaf name="protocol-number">
                  <description>
                    <text>Transport layer protocol number</text>
                  </description>
                  <type name="union">
                    <type name="string">
                      <pattern value="&lt;.*&gt;|$.*"/>
                    </type>
                    <type name="uint32">
                      <range value="0 .. 139"/>
                    </type>
                  </type>
                </leaf>
              </container>
            </case>
            <case name="case_7">
              <container name="ipv6">
                <presence value="enable ipv6"/>
                <description>
                  <text>Attack is for all IPv6 based packets</text>
                </description>
                <uses name="apply-advanced"/>
                <leaf name="protocol-number">
                  <description>
                    <text>Transport layer protocol number</text>
                  </description>
                  <type name="union">
                    <type name="string">
                      <pattern value="&lt;.*&gt;|$.*"/>
                    </type>
                    <type name="uint32">
                      <range value="0 .. 139"/>
                    </type>
                  </type>
                </leaf>
              </container>
            </case>
            <case name="case_8">
              <leaf name="application">
                <description>
                  <text>Application name</text>
                </description>
                <type name="string"/>
              </leaf>
            </case>
            <case name="case_9">
              <leaf name="nested-application">
                <description>
                  <text>Nested application name</text>
                </description>
                <status value="deprecated"/>
                <type name="string"/>
              </leaf>
            </case>
          </choice>
        </container>
        <leaf name="context">
          <description>
            <text>Context</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="pattern">
          <description>
            <text>Pattern is the signature of the attack you want to detect</text>
          </description>
          <type name="string">
            <length value="0 .. 511"/>
          </type>
        </leaf>
        <leaf name="pattern-pcre">
          <description>
            <text>Attack signature pattern in PCRE format</text>
          </description>
          <type name="string">
            <length value="0 .. 511"/>
          </type>
        </leaf>
        <container name="content">
          <description>
            <text>Mention the match-modifire parameters to enhance pattern matching</text>
          </description>
          <leaf name="pattern">
            <description>
              <text>Specify match-modifier pattern</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="pcre">
            <description>
              <text>PCRE expression</text>
            </description>
            <type name="string">
              <length value="0 .. 511"/>
            </type>
          </leaf>
          <container name="depth">
            <description>
              <text>Maximum depth to search pattern within a packet. Depth is not relative</text>
            </description>
            <leaf name="depth-value">
              <description>
                <text>Specify the value of 'depth'</text>
              </description>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint16">
                  <range value="1 .. 65535"/>
                </type>
              </type>
            </leaf>
            <leaf name="depth-variable">
              <description>
                <text>Specify the variable name from which 'depth' should be extracted</text>
              </description>
              <type name="string"/>
            </leaf>
          </container>
          <container name="offset">
            <description>
              <text>Where to start searching for a pattern within a packet. Offset value is not relative</text>
            </description>
            <leaf name="offset-value">
              <description>
                <text>Specify the value of 'offset'</text>
              </description>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint16">
                  <range value="0 .. 65535"/>
                </type>
              </type>
            </leaf>
            <leaf name="offset-variable">
              <description>
                <text>Specify the variable name from which 'offset' should be extracted</text>
              </description>
              <type name="string"/>
            </leaf>
          </container>
          <container name="within">
            <description>
              <text>Maximum Number of bytes present between two conjugative pattern match. within is  relative</text>
            </description>
            <leaf name="within-value">
              <description>
                <text>Specify the value of 'within'</text>
              </description>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint16">
                  <range value="1 .. 65535"/>
                </type>
              </type>
            </leaf>
            <leaf name="within-variable">
              <description>
                <text>Specify the variable name from which 'within' should be extracted</text>
              </description>
              <type name="string"/>
            </leaf>
          </container>
          <container name="distance">
            <description>
              <text>Maximum Length to ignore before searching next pattern match. Distance is  relative</text>
            </description>
            <leaf name="distance-value">
              <description>
                <text>Specify the value of 'distance'</text>
              </description>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="int32">
                  <range value="-65535 .. 65535"/>
                </type>
              </type>
            </leaf>
            <leaf name="distance-variable">
              <description>
                <text>Specify the variable name from which 'distance' should be extracted</text>
              </description>
              <type name="string"/>
            </leaf>
          </container>
          <container name="byte-extract">
            <description>
              <text>Mention the byte-extract parameters for signature in length encoded protocols</text>
            </description>
            <leaf name="bytes">
              <description>
                <text>Specify the number of bytes to extract from packet</text>
              </description>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="1 .. 10"/>
                </type>
              </type>
            </leaf>
            <leaf name="offset">
              <description>
                <text>Specify the number of bytes in to payload to start processing</text>
              </description>
              <type name="union">
                <type name="int32"/>
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
              </type>
            </leaf>
            <leaf name="var-name">
              <description>
                <text>Specify the name of the variable to reference in other rule options</text>
              </description>
              <type name="string"/>
            </leaf>
            <leaf name="relative">
              <description>
                <text>Specify whether to use an offset relative to last pattern match or not</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="multiplier">
              <description>
                <text>Specify the value to be multiplied against the bytes read</text>
              </description>
              <type name="union">
                <type name="uint16"/>
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
              </type>
            </leaf>
            <leaf name="endianness">
              <description>
                <text>Specify the endianness with which bytes read should be processed</text>
              </description>
              <type name="enumeration">
                <enum name="Little">
                  <description>
                    <text>Process data in little endian</text>
                  </description>
                </enum>
                <enum name="Big">
                  <description>
                    <text>Process data in big endian</text>
                  </description>
                </enum>
              </type>
            </leaf>
            <leaf name="align">
              <description>
                <text>Specify the byte alignment</text>
              </description>
              <type name="enumeration">
                <enum name="2-byte">
                  <description>
                    <text>Round the number of converted bytes to 2-byte boundary</text>
                  </description>
                </enum>
                <enum name="4-byte">
                  <description>
                    <text>Round the number of converted bytes to 4-byte boundary</text>
                  </description>
                </enum>
              </type>
            </leaf>
            <leaf name="string">
              <description>
                <text>Specify the data type in which string data should be parsed</text>
              </description>
              <type name="enumeration">
                <enum name="hex">
                  <description>
                    <text>Process data in hexadecimal format</text>
                  </description>
                </enum>
                <enum name="dec">
                  <description>
                    <text>Process data in decimal format</text>
                  </description>
                </enum>
                <enum name="oct">
                  <description>
                    <text>Process data in octal format</text>
                  </description>
                </enum>
              </type>
            </leaf>
            <leaf name="bitmask">
              <description>
                <text>Specify the bitmask (1-4 bytes) for AND operation in hexadecimal format</text>
              </description>
              <type name="string"/>
            </leaf>
          </container>
          <container name="byte-test">
            <description>
              <text>Mention the byte-test parameters for signature in length encoded protocols</text>
            </description>
            <leaf name="bytes">
              <description>
                <text>Specify the number of bytes to extract from packet</text>
              </description>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="1 .. 10"/>
                </type>
              </type>
            </leaf>
            <container name="offset">
              <description>
                <text>Mention the offset variable name or offset value to be used</text>
              </description>
              <leaf name="offset-value">
                <description>
                  <text>Specify the number of bytes in to payload to start processing</text>
                </description>
                <type name="union">
                  <type name="int32"/>
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                </type>
              </leaf>
