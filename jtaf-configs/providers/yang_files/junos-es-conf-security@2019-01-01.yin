            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="1 .. 65535"/>
              </type>
            </type>
          </leaf>
        </container>
        <leaf name="direction">
          <description>
