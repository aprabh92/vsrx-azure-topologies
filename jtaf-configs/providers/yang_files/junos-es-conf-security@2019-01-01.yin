    <leaf name="authentication-algorithm">
      <description>
        <text>Define authentication algorithm</text>
      </description>
      <type name="enumeration">
        <enum name="hmac-sha-256-128">
          <description>
            <text>HMAC-SHA-256-128 authentication algorithm</text>
          </description>
        </enum>
      </type>
    </leaf>
    <leaf name="encryption-algorithm">
      <description>
        <text>Define encryption algorithm</text>
      </description>
      <type name="enumeration">
        <enum name="aes-128-cbc">
          <description>
            <text>AES-CBC 128-bit encryption algorithm</text>
          </description>
        </enum>
        <enum name="aes-192-cbc">
          <description>
            <text>AES-CBC 192-bit encryption algorithm</text>
          </description>
        </enum>
        <enum name="aes-256-cbc">
          <description>
            <text>AES-CBC 256-bit encryption algorithm</text>
          </description>
        </enum>
      </type>
    </leaf>
    <leaf name="lifetime-seconds">
      <description>
        <text>Lifetime, in seconds</text>
      </description>
      <units name="seconds"/>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="uint32">
          <range value="180 .. 86400"/>
        </type>
      </type>
    </leaf>
  </grouping>
  <grouping name="gvpn-server-member-communication">
    <description>
      <text>Group VPN Server to Member communication</text>
    </description>
    <uses name="apply-advanced"/>
    <leaf name="communication-type">
      <description>
        <text>Define type of server member communication</text>
      </description>
      <type name="enumeration">
        <enum name="unicast">
          <description>
            <text>Unicast</text>
          </description>
        </enum>
      </type>
    </leaf>
    <leaf name="lifetime-seconds">
      <description>
        <text>Configure lifetime in seconds</text>
      </description>
      <default value="3600"/>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="uint32">
          <range value="300 .. 86400"/>
        </type>
      </type>
    </leaf>
    <leaf name="retransmission-period">
      <description>
        <text>Configure retransmission period in seconds</text>
      </description>
      <default value="10"/>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="uint32">
          <range value="2 .. 60"/>
        </type>
      </type>
    </leaf>
    <leaf name="number-of-retransmission">
      <description>
        <text>Configure maximum number of retransmission attempts</text>
      </description>
      <default value="2"/>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="uint32">
          <range value="0 .. 60"/>
        </type>
      </type>
    </leaf>
    <leaf name="heartbeat">
      <description>
        <text>Configure heartbeat period in seconds</text>
      </description>
      <status value="deprecated"/>
      <default value="300"/>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="uint32">
          <range value="60 .. 3600"/>
        </type>
      </type>
    </leaf>
    <leaf name="encryption-algorithm">
      <description>
        <text>Define encryption algorithm</text>
      </description>
      <type name="enumeration">
        <enum name="aes-128-cbc">
          <description>
            <text>AES-CBC 128-bit encryption algorithm</text>
          </description>
        </enum>
        <enum name="aes-192-cbc">
          <description>
            <text>AES-CBC 192-bit encryption algorithm</text>
          </description>
        </enum>
        <enum name="aes-256-cbc">
          <description>
            <text>AES-CBC 256-bit encryption algorithm</text>
          </description>
        </enum>
      </type>
    </leaf>
    <leaf name="sig-hash-algorithm">
      <description>
        <text>Define sig-hash algorithm</text>
      </description>
      <type name="enumeration">
        <enum name="sha-256">
          <description>
            <text>SHA 256-bit sig-hash algorithm</text>
          </description>
        </enum>
        <enum name="sha-384">
          <description>
            <text>SHA 384-bit sig-hash algorithm</text>
          </description>
        </enum>
      </type>
    </leaf>
    <leaf name="certificate">
      <description>
        <text>Certificate identifier</text>
      </description>
      <type name="string"/>
    </leaf>
  </grouping>
  <grouping name="gvpn-server-traceoptions">
    <description>
      <text>Group VPN server tracing options</text>
    </description>
    <uses name="apply-advanced"/>
    <leaf name="no-remote-trace">
      <description>
        <text>Disable remote tracing</text>
      </description>
      <type name="empty"/>
    </leaf>
    <container name="file">
      <description>
        <text>Trace file information</text>
      </description>
      <leaf name="filename">
        <description>
          <text>Name of file in which to write trace information</text>
        </description>
        <type name="string">
          <length value="1 .. 1024"/>
        </type>
      </leaf>
      <leaf name="size">
        <description>
          <text>Maximum trace file size</text>
        </description>
        <type name="string"/>
      </leaf>
      <leaf name="files">
        <description>
          <text>Maximum number of trace files</text>
        </description>
        <default value="3"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="2 .. 1000"/>
          </type>
        </type>
      </leaf>
      <choice name="world-readable-choice">
        <leaf name="world-readable">
          <description>
            <text>Allow any user to read the log file</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-world-readable">
          <description>
            <text>Don't allow any user to read the log file</text>
          </description>
          <type name="empty"/>
        </leaf>
      </choice>
      <leaf name="match">
        <description>
          <text>Regular expression for lines to be logged</text>
        </description>
        <type name="jt:regular-expression"/>
      </leaf>
    </container>
    <leaf name="level">
      <description>
        <text>Level of debugging output</text>
      </description>
      <default value="error"/>
      <type name="enumeration">
        <enum name="error">
          <description>
            <text>Match error conditions</text>
          </description>
        </enum>
        <enum name="warning">
          <description>
            <text>Match warning messages</text>
          </description>
        </enum>
        <enum name="notice">
          <description>
            <text>Match conditions that should be handled specially</text>
          </description>
        </enum>
        <enum name="info">
          <description>
            <text>Match informational messages</text>
          </description>
        </enum>
        <enum name="verbose">
          <description>
            <text>Match verbose messages</text>
          </description>
        </enum>
        <enum name="all">
          <description>
            <text>Match all levels</text>
          </description>
        </enum>
      </type>
    </leaf>
    <list name="flag">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Tracing parameters for GKSD</text>
      </description>
      <leaf name="name">
        <type name="enumeration">
          <enum name="timer">
            <description>
              <text>Trace internal timer events</text>
            </description>
          </enum>
          <enum name="routing-socket">
            <description>
              <text>Trace routing socket messages</text>
            </description>
          </enum>
          <enum name="parse">
            <description>
              <text>Trace configuration processing</text>
            </description>
          </enum>
          <enum name="config">
            <description>
              <text>Trace configuration download processing</text>
            </description>
          </enum>
          <enum name="ike">
            <description>
              <text>Trace IKE module processing</text>
            </description>
          </enum>
          <enum name="policy-manager">
            <description>
              <text>Trace policy manager processing</text>
            </description>
          </enum>
          <enum name="general">
            <description>
              <text>Trace general events</text>
            </description>
          </enum>
          <enum name="database">
            <description>
              <text>Trace security associations database events</text>
            </description>
          </enum>
          <enum name="certificates">
            <description>
              <text>Trace certificate events</text>
            </description>
          </enum>
          <enum name="snmp">
            <description>
              <text>Trace SNMP operations</text>
            </description>
          </enum>
          <enum name="thread">
            <description>
              <text>Trace thread processing</text>
            </description>
          </enum>
          <enum name="high-availability">
            <description>
              <text>Trace high-availability operations</text>
            </description>
          </enum>
          <enum name="next-hop-tunnels">
            <description>
              <text>Trace next-hop-tunnels operations</text>
            </description>
          </enum>
          <enum name="all">
            <description>
              <text>Trace everything</text>
            </description>
          </enum>
        </type>
      </leaf>
    </list>
    <container name="gateway-filter">
      <description>
        <text>Set gateway filter for trace</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="local-address">
        <description>
          <text>Use an IPV4 address to identify the local gateway</text>
        </description>
        <type name="jt:ipv4addr"/>
      </leaf>
      <leaf name="remote-address">
        <description>
          <text>Use an IPV4 address to identify the remote gateway</text>
        </description>
        <type name="jt:ipv4addr"/>
      </leaf>
    </container>
  </grouping>
  <grouping name="ipsec-gvpn-member-template">
    <description>
      <text>Configure a Group VPN</text>
    </description>
    <leaf name="name">
      <description>
        <text>Name of the VPN</text>
      </description>
      <type name="string">
        <length value="1 .. 32"/>
      </type>
    </leaf>
    <uses name="apply-advanced"/>
    <leaf name="ike-gateway">
      <description>
        <text>Name of IKE gateway</text>
      </description>
      <type name="string">
        <length value="1 .. 32"/>
      </type>
    </leaf>
    <leaf name="group-vpn-external-interface">
      <description>
        <text>External interface for Group VPN</text>
      </description>
      <type name="union">
        <type name="jt:interface-name"/>
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
      </type>
    </leaf>
    <leaf name="group">
      <description>
        <text>Enable Group VPN by defining group id</text>
      </description>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="uint32">
          <range value="1 .. 4294967295"/>
        </type>
      </type>
    </leaf>
    <leaf name="heartbeat-threshold">
      <description>
        <text>Define heartbeat threshold for Group VPN</text>
      </description>
      <status value="deprecated"/>
      <default value="3"/>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="uint32">
          <range value="0 .. 10"/>
        </type>
      </type>
    </leaf>
    <leaf name="recovery-probe">
      <description>
        <text>Enable triggering recovery probe mechanism</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="df-bit">
      <description>
        <text>Specifies how to handle the Don't Fragment bit</text>
      </description>
      <default value="clear"/>
      <type name="enumeration">
        <enum name="clear">
          <description>
            <text>Clear DF bit of outer header</text>
          </description>
        </enum>
        <enum name="set">
          <description>
            <text>Set DF bit of outer header</text>
          </description>
        </enum>
        <enum name="copy">
          <description>
            <text>Copy DF bit from inner header</text>
          </description>
        </enum>
      </type>
    </leaf>
    <container name="fail-open">
      <description>
        <text>List of fail open rules</text>
      </description>
      <uses name="ipsec_gvpn_fail_open_rule_object"/>
    </container>
    <container name="exclude">
      <description>
        <text>List of exclude rules</text>
      </description>
      <uses name="ipsec_gvpn_exclude_rule_object"/>
    </container>
  </grouping>
  <grouping name="ipsec_gvpn_exclude_rule_object">
    <uses name="apply-advanced"/>
    <list name="rule">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Define exlude rules upto 10</text>
      </description>
      <max-elements value="10"/>
      <uses name="ipsec_gvpn_rule_address_object"/>
    </list>
  </grouping>
  <grouping name="ipsec_gvpn_fail_open_rule_object">
    <uses name="apply-advanced"/>
    <list name="rule">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Define fail open rules upto 10</text>
      </description>
      <max-elements value="10"/>
      <uses name="ipsec_gvpn_rule_address_object"/>
    </list>
  </grouping>
  <grouping name="ipsec_gvpn_rule_address_object">
    <leaf name="name">
      <description>
        <text>Rule name</text>
      </description>
      <type name="string">
        <length value="1 .. 63"/>
      </type>
    </leaf>
    <uses name="apply-advanced"/>
    <container name="source-address">
      <description>
        <text>Match IP source address</text>
      </description>
      <presence value="enable source-address"/>
      <uses name="ipsec_gvpn_addr_object"/>
    </container>
    <container name="destination-address">
      <description>
        <text>Match IP destination address</text>
      </description>
      <presence value="enable destination-address"/>
      <uses name="ipsec_gvpn_addr_object"/>
    </container>
    <leaf name="application">
      <description>
        <text>Match application</text>
      </description>
      <type name="string"/>
    </leaf>
  </grouping>
  <grouping name="ipsec_gvpn_addr_object">
    <uses name="apply-advanced"/>
    <leaf name="address">
      <description>
        <text>Prefix to match</text>
      </description>
      <type name="jt:ipv4prefix-only"/>
    </leaf>
  </grouping>
  <grouping name="security-ike">
    <uses name="apply-advanced"/>
    <container name="traceoptions">
      <description>
        <text>Trace options for IPSec key management</text>
      </description>
      <uses name="security-traceoptions"/>
    </container>
    <container name="respond-bad-spi">
      <presence value="enable respond-bad-spi"/>
      <description>
        <text>Respond to IPSec packets with bad SPI values</text>
      </description>
      <leaf name="max-responses">
        <description>
          <text>Maximum number of times to respond</text>
        </description>
        <default value="5"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 30"/>
          </type>
        </type>
      </leaf>
    </container>
    <list name="proposal">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Define an IKE proposal</text>
      </description>
      <uses name="ike-proposal"/>
    </list>
    <list name="policy">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Define an IKE policy</text>
      </description>
      <uses name="ike-policy"/>
    </list>
    <list name="gateway">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Define an IKE gateway</text>
      </description>
      <leaf name="name">
        <description>
          <text>Label for the remote (peer) gateway</text>
        </description>
        <type name="string">
          <length value="1 .. 32"/>
        </type>
      </leaf>
      <uses name="apply-advanced"/>
      <leaf name="ike-policy">
        <description>
          <text>Name of the IKE policy</text>
        </description>
        <type name="string">
          <length value="1 .. 32"/>
        </type>
      </leaf>
      <choice name="remote-identifier">
        <case name="case_1">
          <leaf-list name="address">
            <ordered-by value="user"/>
            <description>
              <text>Addresses or hostnames of peer:1 primary, upto 4 backups</text>
            </description>
            <type name="string"/>
          </leaf-list>
        </case>
        <case name="case_2">
          <container name="dynamic">
            <description>
              <text>Site to site peer with dynamic IP address</text>
            </description>
            <uses name="apply-advanced"/>
            <choice name="remote-id-type">
              <case name="case_1">
                <container name="distinguished-name">
                  <presence value="enable distinguished-name"/>
                  <description>
                    <text>Use a distinguished name: </text>
                  </description>
                  <uses name="apply-advanced"/>
                  <choice name="name-type">
                    <case name="case_1">
                      <leaf name="container">
                        <description>
                          <text>Specify the container string</text>
                        </description>
                        <type name="string"/>
                      </leaf>
                    </case>
                    <case name="case_2">
                      <leaf name="wildcard">
                        <description>
                          <text>Specify the wildcard string</text>
                        </description>
                        <type name="string"/>
                      </leaf>
                    </case>
                  </choice>
                </container>
              </case>
              <case name="case_2">
                <leaf name="hostname">
                  <description>
                    <text>Use a fully-qualified domain name</text>
                  </description>
                  <type name="string"/>
                </leaf>
              </case>
              <case name="case_3">
                <leaf name="inet">
                  <description>
                    <text>Use an IPV4 address to identify the dynamic peer</text>
                  </description>
                  <type name="jt:ipv4addr"/>
                </leaf>
              </case>
              <case name="case_4">
                <leaf name="inet6">
                  <description>
                    <text>Use an IPV6 address to identify the dynamic peer</text>
                  </description>
                  <type name="jt:ipv6addr"/>
                </leaf>
              </case>
              <case name="case_5">
                <leaf name="user-at-hostname">
                  <description>
                    <text>Use an e-mail address</text>
                  </description>
                  <type name="string"/>
                </leaf>
              </case>
            </choice>
            <leaf name="connections-limit">
              <description>
                <text>Maximum number of users connected to gateway</text>
              </description>
              <type name="union">
                <type name="uint32"/>
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
              </type>
            </leaf>
            <leaf name="ike-user-type">
              <description>
                <text>Type of the IKE ID</text>
              </description>
              <type name="enumeration">
                <enum name="group-ike-id">
                  <description>
                    <text>Email address or FQDN shared as IKED ID by multiple users</text>
                  </description>
                </enum>
                <enum name="shared-ike-id">
                  <description>
                    <text>Email address shared as IKED ID by multiple users</text>
                  </description>
                </enum>
              </type>
            </leaf>
            <leaf name="reject-duplicate-connection">
              <description>
                <text>Reject new connection from duplicate IKE-id</text>
              </description>
              <type name="empty"/>
            </leaf>
          </container>
        </case>
      </choice>
      <container name="dead-peer-detection">
        <presence value="enable dead-peer-detection"/>
        <description>
          <text>Enable Dead Peer Detection </text>
        </description>
        <uses name="apply-advanced"/>
        <choice name="mode">
          <case name="case_1">
            <leaf name="optimized">
              <description>
                <text>Send probes only when there is outgoing and no incoming data traffic - RFC3706 (Default mode)</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
          <case name="case_2">
            <leaf name="probe-idle-tunnel">
              <description>
                <text>Send probes same as in optimized mode and also when there is no outgoing &amp; incoming data traffic</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
          <case name="case_3">
            <leaf name="always-send">
              <description>
                <text>Send probes periodically regardless of incoming and outgoing data traffic </text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
        </choice>
        <leaf name="interval">
          <description>
            <text>The time between DPD probe messages  Default :10</text>
          </description>
          <default value="10"/>
          <units name="seconds"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="2 .. 60"/>
            </type>
          </type>
        </leaf>
        <leaf name="threshold">
          <description>
            <text>Maximum number of DPD retransmissions Default :5</text>
          </description>
          <default value="5"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="1 .. 5"/>
            </type>
          </type>
        </leaf>
      </container>
      <leaf name="no-nat-traversal">
        <description>
          <text>Disable IPSec NAT traversal</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="nat-keepalive">
        <description>
          <text>Interval at which to send NAT keepalives</text>
        </description>
        <default value="20"/>
        <units name="seconds"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 300"/>
          </type>
        </type>
      </leaf>
      <container name="local-identity">
        <description>
          <text>Set the local IKE identity</text>
        </description>
        <choice name="identity-type">
          <case name="case_1">
            <container name="inet">
              <description>
                <text>Use an IPv4 address</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="identity-ipv4">
                <description>
                  <text>The local IPv4 identity</text>
                </description>
                <type name="jt:ipv4addr"/>
              </leaf>
            </container>
          </case>
          <case name="case_2">
            <container name="inet6">
              <description>
                <text>Use an IPv6 address</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="identity-ipv6">
                <description>
                  <text>The local IPv6 identity</text>
                </description>
                <type name="jt:ipv6addr"/>
              </leaf>
            </container>
          </case>
          <case name="case_3">
            <container name="hostname">
              <description>
                <text>Use a fully-qualified domain name</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="identity-hostname">
                <description>
                  <text>The local hostname</text>
                </description>
                <type name="string"/>
              </leaf>
            </container>
          </case>
          <case name="case_4">
            <container name="user-at-hostname">
              <description>
                <text>Use an e-mail address</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="identity-user">
                <description>
                  <text>The local user-FQDN</text>
                </description>
                <type name="string"/>
              </leaf>
            </container>
          </case>
          <case name="case_5">
            <leaf name="distinguished-name">
              <description>
                <text>Use a distinguished name specified in local certificate</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
          <case name="case_6">
            <container name="key-id">
              <description>
                <text>Key ID identification values in ASCII string</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="string-key-id">
                <description>
                  <text>Key ID in ASCII string</text>
                </description>
                <type name="string">
                  <length value="1 .. 127"/>
                </type>
              </leaf>
            </container>
          </case>
        </choice>
      </container>
