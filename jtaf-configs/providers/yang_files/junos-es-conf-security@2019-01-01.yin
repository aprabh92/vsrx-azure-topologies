            <enum name="30">
              <description>
                <text>Confidentiality not protected for initial 30 octets of MACSec PDU data</text>
              </description>
            </enum>
            <enum name="50">
              <description>
                <text>Confidentiality not protected for initial 50 octets of MACSec PDU data</text>
              </description>
