          <description>
            <text>Secure channel name</text>
          </description>
          <type name="string"/>
        </leaf>
        <uses name="apply-advanced"/>
        <container name="id">
          <description>
            <text>Secure channel identifier</text>
          </description>
          <uses name="apply-advanced"/>
