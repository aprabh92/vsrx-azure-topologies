                    <type name="empty"/>
                  </leaf>
                  <leaf name="destination-lsb">
                    <description>
                      <text>Include the least significant byte of the destination port</text>
                    </description>
                    <type name="empty"/>
                  </leaf>
                </container>
              </case>
            </choice>
          </container>
        </container>
      </container>
