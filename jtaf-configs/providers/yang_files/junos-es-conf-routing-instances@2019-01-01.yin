<?xml version="1.0" encoding="UTF-8"?>
<module name="junos-es-conf-routing-instances"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:jc-routing-instances="http://yang.juniper.net/junos-es/conf/routing-instances"
        xmlns:jt="http://yang.juniper.net/junos/common/types"
        xmlns:jc="http://yang.juniper.net/junos-es/conf/root">
  <namespace uri="http://yang.juniper.net/junos-es/conf/routing-instances"/>
  <prefix value="jc-routing-instances"/>
  <import module="junos-common-types">
    <prefix value="jt"/>
    <revision-date date="2019-01-01"/>
  </import>
  <import module="junos-es-conf-root">
    <prefix value="jc"/>
    <revision-date date="2019-01-01"/>
  </import>
  <organization>
    <text>Juniper Networks, Inc.</text>
  </organization>
  <contact>
    <text>yang-support@juniper.net</text>
  </contact>
  <description>
    <text>Junos routing-instances configuration module</text>
  </description>
  <revision date="2019-01-01">
    <description>
      <text>Junos: 21.3R1.9</text>
    </description>
  </revision>
  <augment target-node="/jc:configuration">
    <uses name="routing-instances-group"/>
  </augment>
  <augment target-node="/jc:configuration/jc:groups">
    <uses name="routing-instances-group"/>
  </augment>
  <grouping name="routing-instances-group">
    <container name="routing-instances">
      <description>
        <text>Routing instance configuration</text>
      </description>
      <uses name="apply-advanced"/>
      <list name="instance">
        <key value="name"/>
        <uses name="juniper-routing-instance"/>
      </list>
    </container>
  </grouping>
  <grouping name="apply-advanced">
    <description>
      <text>Apply advanced configuration logic</text>
    </description>
    <leaf-list name="apply-groups">
      <ordered-by value="user"/>
      <description>
        <text>Groups from which to inherit configuration data</text>
      </description>
      <type name="string"/>
    </leaf-list>
    <leaf-list name="apply-groups-except">
      <ordered-by value="user"/>
      <description>
        <text>Don't inherit configuration data from these groups</text>
      </description>
      <type name="string"/>
    </leaf-list>
    <list name="apply-macro">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Macro and parameters for commit script expansion</text>
      </description>
      <uses name="apply-macro-type"/>
    </list>
  </grouping>
  <grouping name="apply-macro-type">
    <description>
      <text>Macro data for commit-script expansion</text>
    </description>
    <leaf name="name">
      <description>
        <text>Name of the macro to be expanded</text>
      </description>
      <type name="string"/>
    </leaf>
    <list name="data">
      <key value="name"/>
      <uses name="macro-data-type"/>
    </list>
  </grouping>
  <grouping name="juniper-routing-instance">
    <description>
      <text>Routing instance</text>
    </description>
    <leaf name="name">
      <description>
        <text>Routing instance name</text>
      </description>
      <type name="string"/>
    </leaf>
    <uses name="apply-advanced"/>
    <container name="vlans">
      <description>
        <text>VLAN configuration</text>
      </description>
      <uses name="apply-advanced"/>
      <list name="vlan">
        <key value="name"/>
        <description>
          <text>Virtual LAN</text>
        </description>
        <uses name="vlan-types"/>
      </list>
    </container>
    <container name="protocols">
      <description>
        <text>Routing protocol configuration</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="bfd">
        <presence value="enable bfd"/>
        <uses name="juniper-ri-protocols-bfd"/>
      </container>
      <container name="igmp-snooping">
        <description>
          <text>IGMP snooping configuration</text>
        </description>
        <presence value="enable igmp-snooping"/>
        <uses name="juniper-ri-protocols-igmp-snooping"/>
      </container>
      <container name="mld-snooping">
        <description>
          <text>MLD snooping configuration</text>
        </description>
        <presence value="enable mld-snooping"/>
        <uses name="juniper-ri-protocols-mld-snooping"/>
      </container>
      <container name="amt">
        <description>
          <text>AMT relay configuration</text>
        </description>
        <uses name="juniper-protocols-amt"/>
      </container>
      <container name="bgp">
        <description>
          <text>BGP options</text>
        </description>
        <uses name="juniper-protocols-bgp"/>
      </container>
      <container name="esis">
        <description>
          <text>ES-IS configuration</text>
        </description>
        <uses name="juniper-protocols-esis"/>
      </container>
      <container name="router-discovery">
        <description>
          <text>ICMP router discovery options</text>
        </description>
        <uses name="juniper-protocols-router-discovery"/>
      </container>
      <container name="isis">
        <description>
          <text>IS-IS configuration</text>
        </description>
        <uses name="juniper-protocols-isis"/>
      </container>
      <container name="l2vpn">
        <description>
          <text>Layer 2 VPN configuration</text>
        </description>
        <uses name="juniper-protocols-l2vpn"/>
      </container>
      <container name="vpls">
        <description>
          <text>VPLS configuration</text>
        </description>
        <presence value="enable vpls"/>
        <uses name="juniper-protocols-l2vpn"/>
      </container>
      <container name="ldp">
        <description>
          <text>LDP configuration</text>
        </description>
        <uses name="juniper-protocols-ldp"/>
      </container>
      <container name="mpls">
        <description>
          <text>MPLS configuration</text>
        </description>
        <uses name="juniper-protocols-mpls"/>
      </container>
      <container name="msdp">
        <description>
          <text>MSDP configuration</text>
        </description>
        <uses name="juniper-protocols-msdp"/>
      </container>
      <container name="mvpn">
        <description>
          <text>BGP-MVPN configuration</text>
        </description>
        <presence value="enable mvpn"/>
        <uses name="juniper-protocols-mvpn"/>
      </container>
      <container name="ospf">
        <description>
          <text>OSPF configuration</text>
        </description>
        <uses name="juniper-protocols-ospf"/>
      </container>
      <container name="ospf3">
        <description>
          <text>OSPF3 configuration</text>
        </description>
        <uses name="juniper-protocols-ospf3"/>
      </container>
      <container name="pim">
        <description>
          <text>PIM configuration</text>
        </description>
        <uses name="juniper-protocols-pim"/>
      </container>
      <container name="ripng">
        <description>
          <text>RIPng options</text>
        </description>
        <uses name="juniper-protocols-ripng"/>
      </container>
      <container name="rip">
        <description>
          <text>RIP options</text>
        </description>
        <uses name="juniper-protocols-rip"/>
      </container>
      <container name="rsvp">
        <description>
          <text>RSVP configuration</text>
        </description>
        <uses name="juniper-protocols-rsvp"/>
      </container>
      <container name="pim-snooping">
        <description>
          <text>PIM snooping configuration</text>
        </description>
        <presence value="enable pim-snooping"/>
        <uses name="juniper-protocols-pim-snooping"/>
      </container>
      <container name="mvrp">
        <description>
          <text>MVRP configuration</text>
        </description>
        <uses name="juniper-protocols-mvrp"/>
      </container>
      <container name="rstp">
        <description>
          <text>RSTP configuration</text>
        </description>
        <uses name="juniper-protocols-stp"/>
      </container>
      <container name="mstp">
        <description>
          <text>MSTP configuration</text>
        </description>
        <uses name="juniper-protocols-mstp"/>
      </container>
      <container name="vstp">
        <description>
          <text>VSTP configuration</text>
        </description>
        <uses name="juniper-protocols-vstp"/>
      </container>
    </container>
    <list name="interface">
      <key value="name"/>
      <description>
        <text>Interface name for this routing instance</text>
      </description>
      <leaf name="name">
        <type name="string"/>
      </leaf>
      <uses name="apply-advanced"/>
      <choice name="mode">
        <case name="case_1">
          <leaf name="any">
            <description>
              <text>Interface used for both unicast and multicast traffic</text>
            </description>
            <type name="empty"/>
          </leaf>
        </case>
        <case name="case_2">
          <leaf name="unicast">
            <description>
              <text>Interface used for unicast traffic only</text>
            </description>
            <type name="empty"/>
          </leaf>
        </case>
        <case name="case_3">
          <leaf name="multicast">
            <description>
              <text>Interface used for multicast traffic only</text>
            </description>
            <type name="empty"/>
          </leaf>
        </case>
      </choice>
      <leaf name="primary">
        <description>
          <text>Preferred multicast vt interface for the routing-instance</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="protect-interface">
        <description>
          <text>Name of protect interface</text>
        </description>
        <type name="union">
          <type name="jt:interface-name"/>
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
        </type>
      </leaf>
    </list>
    <leaf name="description">
      <description>
        <text>Text description of routing instance</text>
      </description>
      <type name="string"/>
    </leaf>
    <leaf name="vlan-model">
      <description>
        <text>Subscriber vlan-model in L2Wholesale framework</text>
      </description>
      <type name="enumeration">
        <enum name="one-to-one">
          <description>
            <text>Customer VLAN model (1:1)</text>
          </description>
        </enum>
      </type>
    </leaf>
    <container name="vtep-source-interface">
      <description>
        <text>Source layer-3 IFL for VXLAN</text>
      </description>
      <leaf name="interface-name">
        <type name="union">
          <type name="jt:interface-name"/>
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
        </type>
      </leaf>
      <choice name="family">
        <case name="case_1">
          <container name="inet">
            <presence value="enable inet"/>
            <description>
              <text>IPv4 source</text>
            </description>
            <uses name="apply-advanced"/>
          </container>
        </case>
        <case name="case_2">
          <container name="inet6">
            <presence value="enable inet6"/>
            <description>
              <text>IPv6 source</text>
            </description>
            <uses name="apply-advanced"/>
          </container>
        </case>
      </choice>
    </container>
    <container name="vtep-remote-interface">
      <description>
        <text>Remote VTEP interface</text>
      </description>
      <uses name="apply-advanced"/>
      <list name="remote-ip">
        <key value="name"/>
        <ordered-by value="user"/>
        <description>
          <text>Remote VTEP IP address</text>
        </description>
        <leaf name="name">
          <description>
            <text>IP address</text>
          </description>
          <type name="jt:ipv4addr"/>
        </leaf>
        <uses name="apply-advanced"/>
        <leaf name="dynamic-profile">
          <description>
            <text>Define associate dynamic profile</text>
          </description>
          <type name="string"/>
        </leaf>
      </list>
      <container name="default">
        <description>
          <text>To all remote vtep interface</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="dynamic-profile">
          <description>
            <text>Define associate dynamic profile</text>
          </description>
          <type name="string"/>
        </leaf>
      </container>
    </container>
    <leaf-list name="remote-vtep-list">
      <description>
        <text>Configure static remote VXLAN tunnel endpoints</text>
      </description>
      <type name="jt:ipaddr"/>
      <max-elements value="1024"/>
    </leaf-list>
    <leaf-list name="remote-vtep-v6-list">
      <description>
        <text>Configurate static ipv6 remote VXLAN tunnel endpoints</text>
      </description>
      <type name="jt:ipv6addr"/>
      <max-elements value="1024"/>
    </leaf-list>
    <leaf name="instance-role">
      <description>
        <text>Primary role of L2Backhaul-vpn router</text>
      </description>
      <type name="enumeration">
        <enum name="access">
          <description>
            <text>Role of Access/Aggregation router</text>
          </description>
        </enum>
        <enum name="nni">
          <description>
            <text>Role of Provider network interfacing router</text>
          </description>
        </enum>
      </type>
    </leaf>
    <leaf name="instance-type">
      <description>
        <text>Type of routing instance</text>
      </description>
      <type name="enumeration">
        <enum name="virtual-switch">
          <description>
            <text>Virtual switch routing instance</text>
          </description>
        </enum>
        <enum name="layer2-control">
          <description>
            <text>Layer 2 control protocols</text>
          </description>
        </enum>
        <enum name="mac-vrf">
          <description>
            <text>MAC-VRF routing instance</text>
          </description>
        </enum>
        <enum name="forwarding">
          <description>
            <text>Forwarding instance</text>
          </description>
        </enum>
        <enum name="no-forwarding">
          <description>
            <text>Nonforwarding instance</text>
          </description>
        </enum>
        <enum name="l2vpn">
          <description>
            <text>Layer 2 VPN routing instance</text>
          </description>
        </enum>
        <enum name="vpls">
          <description>
            <text>VPLS routing instance</text>
          </description>
        </enum>
        <enum name="virtual-router">
          <description>
            <text>Virtual routing instance</text>
          </description>
        </enum>
        <enum name="l2backhaul-vpn">
          <description>
            <text>L2Backhaul/L2Wholesale routing instance</text>
          </description>
        </enum>
        <enum name="mpls-internet-multicast">
          <description>
            <text>Internet Multicast over MPLS routing instance</text>
          </description>
        </enum>
        <enum name="evpn">
          <description>
            <text>EVPN routing instance</text>
          </description>
        </enum>
        <enum name="mpls-forwarding">
          <description>
            <text>Routing instance provides a MPLS forwarding-context</text>
          </description>
        </enum>
        <enum name="evpn-vpws">
          <description>
            <text>EVPN VPWS routing instance</text>
          </description>
        </enum>
        <enum name="vrf">
          <description>
            <text>Virtual routing forwarding instance</text>
          </description>
        </enum>
      </type>
    </leaf>
    <choice name="vlan_choice">
      <case name="case_1">
        <leaf name="vlan-id">
          <description>
            <text>IEEE 802.1q VLAN identifier for bridging domain</text>
          </description>
          <type name="string"/>
        </leaf>
      </case>
      <case name="case_2">
        <container name="vlan-tags">
          <presence value="enable vlan-tags"/>
          <description>
            <text>IEEE 802.1q VLAN tags for bridging domain</text>
          </description>
          <leaf name="outer">
            <description>
              <text>[tpid.]vlan-id, tpid format is 0xNNNN and is optional</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="inner">
            <description>
              <text>[tpid.]vlan-id, tpid format is 0xNNNN and is optional</text>
            </description>
            <type name="string"/>
          </leaf>
        </container>
      </case>
    </choice>
    <container name="system">
      <description>
        <text>System parameters</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="services">
        <description>
          <text>System services</text>
        </description>
        <uses name="apply-advanced"/>
        <container name="dhcp-local-server">
          <description>
            <text>Dynamic Host Configuration Protocol server configuration</text>
          </description>
          <uses name="jdhcp-local-server-type"/>
        </container>
        <container name="static-subscribers">
          <description>
            <text>Static Subscriber Client configuration</text>
          </description>
          <uses name="jsscd-static-subscribers-type"/>
        </container>
      </container>
    </container>
    <container name="access">
      <description>
        <text>Network access configuration</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="address-assignment">
        <description>
          <text>Address assignment configuration</text>
        </description>
        <uses name="address-assignment-type"/>
      </container>
      <container name="address-protection">
        <presence value="enable address-protection"/>
        <description>
          <text>Initiate Duplicate Address Protection</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="reassign-on-match">
          <description>
            <text>Disconnect owning session and reassign to this session</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
    </container>
    <container name="access-profile">
      <description>
        <text>Access profile for this instance</text>
      </description>
      <leaf name="access-profile-name">
        <description>
          <text>Profile name</text>
        </description>
        <type name="string"/>
      </leaf>
    </container>
    <leaf name="routing-interface">
      <description>
        <text>Routing interface name for this routing-instance</text>
      </description>
      <type name="union">
        <type name="jt:interface-unit"/>
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
      </type>
    </leaf>
    <container name="vxlan">
      <description>
        <text>VXLAN options</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="ovsdb-managed">
        <description>
          <text>Managed remotely via VXLAN OVSDB Controller</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="vni">
        <description>
          <text>VXLAN identifier</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="0 .. 16777214"/>
          </type>
        </type>
      </leaf>
      <leaf name="translation-vni">
        <description>
          <text>Translated VXLAN identifier</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 16777214"/>
          </type>
        </type>
      </leaf>
      <leaf name="multicast-group">
        <description>
          <text>Multicast group registered for VXLAN segment</text>
        </description>
        <type name="jt:ipv4addr"/>
      </leaf>
      <leaf name="multicast-v6-group">
        <description>
          <text>Multicast IPv6 group registered for VXLAN segment</text>
        </description>
        <type name="jt:ipv6addr"/>
      </leaf>
      <leaf name="encapsulate-inner-vlan">
        <description>
          <text>Retain inner VLAN in the packet</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="decapsulate-accept-inner-vlan">
        <description>
          <text>Accept VXLAN packets with inner VLAN</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="unreachable-vtep-aging-timer">
        <description>
          <text>Unreachable VXLAN tunnel endpoint removal timer</text>
        </description>
        <units name="seconds"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint16">
            <range value="300 .. 1800"/>
          </type>
        </type>
      </leaf>
      <leaf name="ingress-node-replication">
        <description>
          <text>Enable ingress node replication</text>
        </description>
        <type name="empty"/>
      </leaf>
    </container>
    <leaf name="no-local-switching">
      <description>
        <text>Disable local switching within CE-facing interfaces</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="no-normalization">
      <description>
        <text>Disable vlan id normalization for interfaces</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="qualified-bum-pruning-mode">
      <description>
        <text>Enable BUM pruning for VPLS instance</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="no-irb-layer-2-copy">
      <description>
        <text>Disable transmission of layer-2 copy of packets of irb routing-interface</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="connector-id-advertise">
      <description>
        <text>Advertise connector-id attribute</text>
      </description>
      <type name="empty"/>
    </leaf>
    <container name="forwarding-options">
      <description>
        <text>Forwarding options configuration</text>
      </description>
      <uses name="juniper-forwarding-options"/>
    </container>
    <container name="bridge-domains">
      <description>
        <text>Bridge domain configuration</text>
      </description>
      <uses name="apply-advanced"/>
      <list name="domain">
        <key value="name"/>
        <uses name="juniper-bridge-domains"/>
      </list>
    </container>
    <container name="switch-options">
      <description>
        <text>L2 options for routing-instance of type virtual-switch</text>
      </description>
      <uses name="juniper-routing-instance-switch-options"/>
    </container>
    <container name="pbb-options">
      <description>
        <text>Provider backbone bridging options for routing-instance</text>
      </description>
      <uses name="juniper-routing-instance-pbb-options"/>
    </container>
    <list name="service-groups">
      <key value="name"/>
      <description>
        <text>Service group configuration for routing-instance</text>
      </description>
      <uses name="juniper-routing-instance-service-groups"/>
    </list>
    <leaf name="service-type">
      <description>
        <text>Sevice type</text>
      </description>
      <type name="enumeration">
        <enum name="vlan-based">
          <description>
            <text>Instance service type vlan-based</text>
          </description>
        </enum>
        <enum name="vlan-bundle">
          <description>
            <text>Instance service type vlan-bundle</text>
          </description>
        </enum>
        <enum name="vlan-aware">
          <description>
            <text>Instance service type vlan-aware</text>
          </description>
        </enum>
      </type>
    </leaf>
    <container name="l2vpn-id">
      <description>
        <text>Layer-2 vpn-id for this instance</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="community">
        <description>
          <text>L2VPN ID community for FEC129 VPLS/VPWS with BGP auto-discovery</text>
        </description>
        <type name="string"/>
      </leaf>
    </container>
    <container name="routing-options">
      <description>
        <text>Protocol-independent routing option configuration</text>
      </description>
      <uses name="juniper-routing-options"/>
    </container>
    <container name="egress-protection">
      <description>
        <text>Egress instance protection</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="protector">
        <description>
          <text>Enable Edge Protector functionality for this VPN</text>
        </description>
        <type name="empty"/>
      </leaf>
      <container name="context-identifier">
        <description>
          <text>Context identifier</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="context-id">
          <description>
            <text>IP address</text>
          </description>
          <type name="jt:ipv4addr"/>
        </leaf>
      </container>
    </container>
    <container name="route-distinguisher">
      <description>
