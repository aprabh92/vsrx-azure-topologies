          <text>Enable MAC pinning</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="persistent-learning">
        <description>
          <text>Enable persistent MAC learning on this interface</text>
        </description>
        <type name="empty"/>
      </leaf>
    </list>
    <container name="traceoptions">
      <description>
        <text>Trace options for this bridge domain</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="no-remote-trace">
        <description>
          <text>Disable remote tracing</text>
        </description>
        <type name="empty"/>
      </leaf>
      <container name="file">
        <description>
          <text>Trace file information</text>
        </description>
        <leaf name="filename">
          <description>
            <text>Name of file in which to write trace information</text>
          </description>
          <type name="string">
            <length value="1 .. 1024"/>
          </type>
        </leaf>
        <leaf name="size">
          <description>
            <text>Maximum trace file size</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="files">
          <description>
            <text>Maximum number of trace files</text>
          </description>
          <default value="3"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="2 .. 1000"/>
            </type>
          </type>
        </leaf>
        <choice name="world-readable-choice">
          <leaf name="world-readable">
            <description>
              <text>Allow any user to read the log file</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="no-world-readable">
            <description>
              <text>Don't allow any user to read the log file</text>
            </description>
            <type name="empty"/>
          </leaf>
        </choice>
        <leaf name="match">
          <description>
            <text>Regular expression for lines to be logged</text>
          </description>
          <type name="jt:regular-expression"/>
        </leaf>
      </container>
      <leaf name="level">
        <description>
          <text>Level of debugging output</text>
        </description>
        <default value="error"/>
        <type name="enumeration">
          <enum name="error">
            <description>
              <text>Match error conditions</text>
            </description>
          </enum>
          <enum name="warning">
            <description>
              <text>Match warning messages</text>
            </description>
          </enum>
          <enum name="notice">
            <description>
              <text>Match conditions that should be handled specially</text>
            </description>
          </enum>
          <enum name="info">
            <description>
              <text>Match informational messages</text>
            </description>
          </enum>
          <enum name="verbose">
            <description>
              <text>Match verbose messages</text>
            </description>
          </enum>
          <enum name="all">
            <description>
              <text>Match all levels</text>
            </description>
          </enum>
        </type>
      </leaf>
      <list name="flag">
        <key value="name"/>
        <ordered-by value="user"/>
        <description>
          <text>Type of operation or event to include in trace</text>
        </description>
        <leaf name="name">
          <type name="enumeration">
            <enum name="configuration">
              <description>
                <text>Configuration operations</text>
              </description>
            </enum>
            <enum name="routing-socket">
              <description>
                <text>Routing socket operations</text>
              </description>
            </enum>
            <enum name="interface-device">
              <description>
                <text>Interface device operations</text>
              </description>
            </enum>
            <enum name="interface-logical">
              <description>
                <text>Logical interface operations</text>
              </description>
            </enum>
            <enum name="interface-family">
              <description>
                <text>Interface family operations</text>
              </description>
            </enum>
            <enum name="learning-domain">
              <description>
                <text>Learning domain operations</text>
              </description>
            </enum>
            <enum name="ipc">
              <description>
                <text>Inter-process communications operations</text>
              </description>
            </enum>
            <enum name="mac-learning">
              <description>
                <text>MAC address learning operations</text>
              </description>
            </enum>
            <enum name="initialization">
              <description>
                <text>Initialization operations</text>
              </description>
            </enum>
            <enum name="flood-next-hop">
              <description>
                <text>Flood next hop operations</text>
              </description>
            </enum>
            <enum name="storm-control">
              <description>
                <text>Storm-control</text>
              </description>
            </enum>
            <enum name="unknown-unicast-forwarding">
              <description>
                <text>Trace unknown unicast forwarding events</text>
              </description>
            </enum>
            <enum name="all">
              <description>
                <text>All operations</text>
              </description>
            </enum>
          </type>
        </leaf>
      </list>
    </container>
  </grouping>
  <grouping name="write-option-82-type">
    <uses name="apply-advanced"/>
  </grouping>
</module>
