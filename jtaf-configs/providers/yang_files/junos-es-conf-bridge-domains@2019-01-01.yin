<?xml version="1.0" encoding="UTF-8"?>
<module name="junos-es-conf-bridge-domains"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:jc-bridge-domains="http://yang.juniper.net/junos-es/conf/bridge-domains"
        xmlns:jt="http://yang.juniper.net/junos/common/types"
        xmlns:jc="http://yang.juniper.net/junos-es/conf/root">
  <namespace uri="http://yang.juniper.net/junos-es/conf/bridge-domains"/>
  <prefix value="jc-bridge-domains"/>
  <import module="junos-common-types">
    <prefix value="jt"/>
    <revision-date date="2019-01-01"/>
  </import>
  <import module="junos-es-conf-root">
    <prefix value="jc"/>
    <revision-date date="2019-01-01"/>
  </import>
  <organization>
    <text>Juniper Networks, Inc.</text>
  </organization>
  <contact>
    <text>yang-support@juniper.net</text>
  </contact>
  <description>
    <text>Junos bridge-domains configuration module</text>
  </description>
  <revision date="2019-01-01">
    <description>
      <text>Junos: 21.3R1.9</text>
    </description>
  </revision>
  <augment target-node="/jc:configuration">
    <uses name="bridge-domains-group"/>
  </augment>
  <augment target-node="/jc:configuration/jc:groups">
    <uses name="bridge-domains-group"/>
  </augment>
  <grouping name="bridge-domains-group">
    <container name="bridge-domains">
      <description>
        <text>Bridge domain configuration</text>
      </description>
      <uses name="apply-advanced"/>
      <list name="domain">
        <key value="name"/>
        <uses name="juniper-bridge-domains"/>
      </list>
    </container>
  </grouping>
  <grouping name="apply-advanced">
    <description>
      <text>Apply advanced configuration logic</text>
    </description>
    <leaf-list name="apply-groups">
      <ordered-by value="user"/>
      <description>
        <text>Groups from which to inherit configuration data</text>
      </description>
      <type name="string"/>
    </leaf-list>
    <leaf-list name="apply-groups-except">
      <ordered-by value="user"/>
      <description>
        <text>Don't inherit configuration data from these groups</text>
      </description>
      <type name="string"/>
    </leaf-list>
    <list name="apply-macro">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Macro and parameters for commit script expansion</text>
      </description>
      <uses name="apply-macro-type"/>
    </list>
  </grouping>
  <grouping name="apply-macro-type">
    <description>
      <text>Macro data for commit-script expansion</text>
    </description>
    <leaf name="name">
      <description>
        <text>Name of the macro to be expanded</text>
      </description>
      <type name="string"/>
    </leaf>
    <list name="data">
      <key value="name"/>
      <uses name="macro-data-type"/>
    </list>
  </grouping>
  <grouping name="juniper-bridge-domains">
    <description>
      <text>Bridge domains</text>
    </description>
    <leaf name="name">
      <description>
        <text>Bridge domain name</text>
      </description>
      <type name="string"/>
    </leaf>
    <uses name="apply-advanced"/>
    <leaf name="description">
      <description>
        <text>Text description of bridge domain</text>
      </description>
      <type name="string"/>
    </leaf>
    <leaf name="domain-type">
      <description>
        <text>Type of bridge domain</text>
      </description>
      <type name="enumeration">
        <enum name="bridge">
          <description>
            <text>Forwarding instance</text>
          </description>
        </enum>
      </type>
    </leaf>
    <choice name="vlan_choice">
      <case name="case_1">
        <leaf name="vlan-id">
          <description>
            <text>IEEE 802.1q VLAN identifier for bridging domain</text>
          </description>
          <type name="string"/>
        </leaf>
      </case>
      <case name="case_2">
        <container name="vlan-tags">
          <presence value="enable vlan-tags"/>
          <description>
            <text>IEEE 802.1q VLAN tags for bridging domain</text>
          </description>
          <leaf name="outer">
            <description>
              <text>[tpid.]vlan-id, tpid format is 0xNNNN and is optional</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="inner">
            <description>
              <text>[tpid.]vlan-id, tpid format is 0xNNNN and is optional</text>
            </description>
            <type name="string"/>
          </leaf>
        </container>
      </case>
      <case name="case_3">
        <leaf-list name="vlan-id-list">
          <description>
            <text>Create bridge-domain for each of the vlan-id specified in the vlan-id-list</text>
          </description>
          <type name="jt:vlan-range"/>
        </leaf-list>
      </case>
    </choice>
    <leaf-list name="isid-list">
      <ordered-by value="user"/>
      <description>
        <text>Create bridge-domain for isid (Valid isid:256..16777214)</text>
      </description>
      <type name="string"/>
    </leaf-list>
    <leaf name="vlan-id-scope-local">
      <description>
        <text>Enable the scope of vlan-id local to avoid transmitting vlan tagged packets</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="service-id">
      <description>
        <text>Service id required if bridge-domain is of type MC-AE and vlan-id all or vlan-id none or vlan-tags</text>
      </description>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="uint32">
          <range value="1 .. 65535"/>
        </type>
      </type>
    </leaf>
    <leaf name="domain-id">
      <description>
        <text>Domain-id for auto derived Route Target</text>
      </description>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="uint32">
          <range value="1 .. 15"/>
        </type>
      </type>
    </leaf>
    <leaf name="no-local-switching">
      <description>
        <text>Disable local switching within CE-facing interfaces</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="mcae-mac-synchronize">
      <description>
        <text>Enable IRB MAC synchronization in this bridge domain</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="mcae-mac-flush">
      <description>
        <text>Enable MCAE MAC flush in a/s mode for a bridge domain on MCAE link up</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="no-irb-layer-2-copy">
      <description>
        <text>Disable transmission of layer-2 copy of packets of irb routing-interface</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="enable-mac-move-action">
      <description>
        <text>Enable blocking action due to mac-move in this Bridge Domain</text>
      </description>
      <type name="empty"/>
    </leaf>
    <list name="interface">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Interface name for this bridge domain</text>
      </description>
      <leaf name="name">
        <type name="string"/>
      </leaf>
      <uses name="apply-advanced"/>
      <leaf name="protect-interface">
        <description>
          <text>Name of protect interface</text>
        </description>
        <type name="union">
          <type name="jt:interface-name"/>
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
        </type>
      </leaf>
    </list>
    <leaf name="routing-interface">
      <description>
        <text>Routing interface name for this bridge-domain</text>
      </description>
      <type name="union">
        <type name="jt:interface-unit"/>
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
      </type>
    </leaf>
    <container name="forwarding-options">
      <description>
        <text>Forwarding options configuration</text>
      </description>
      <uses name="juniper-bridge-forwarding-options"/>
    </container>
    <container name="bridge-options">
      <description>
        <text>Bridge domain configuration</text>
      </description>
      <uses name="juniper-protocols-bd"/>
    </container>
    <container name="protocols">
      <presence value="enable protocols"/>
      <uses name="apply-advanced"/>
      <container name="igmp-snooping">
        <description>
          <text>IGMP snooping configuration</text>
        </description>
        <presence value="enable igmp-snooping"/>
        <uses name="juniper-bd-protocols-igmp-snooping"/>
      </container>
      <container name="mld-snooping">
        <description>
          <text>MLD snooping configuration</text>
        </description>
        <presence value="enable mld-snooping"/>
        <uses name="juniper-bd-protocols-mld-snooping"/>
      </container>
    </container>
    <container name="vxlan">
      <description>
        <text>VXLAN options</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="ovsdb-managed">
        <description>
          <text>Bridge-domain is managed remotely via VXLAN OVSDB Controller</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="vni">
        <description>
          <text>VXLAN identifier</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="0 .. 16777214"/>
          </type>
        </type>
      </leaf>
      <leaf name="translation-vni">
        <description>
          <text>Translated VXLAN identifier</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 16777214"/>
          </type>
        </type>
      </leaf>
      <leaf name="multicast-group">
        <description>
          <text>Multicast group registered for VXLAN segment</text>
        </description>
        <type name="jt:ipv4addr"/>
      </leaf>
      <leaf name="encapsulate-inner-vlan">
        <description>
          <text>Retain inner VLAN in the packet</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="decapsulate-accept-inner-vlan">
        <description>
          <text>Accept VXLAN packets with inner VLAN</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="unreachable-vtep-aging-timer">
        <description>
          <text>Unreachable VXLAN tunnel endpoint removal timer</text>
        </description>
        <units name="seconds"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint16">
            <range value="300 .. 1800"/>
          </type>
        </type>
      </leaf>
      <leaf name="ingress-node-replication">
        <description>
          <text>Enable ingress node replication</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf-list name="static-remote-vtep-list">
        <description>
          <text>Configure bridge domain specific static remote VXLAN tunnel endpoints</text>
        </description>
        <type name="jt:ipaddr"/>
        <max-elements value="1024"/>
      </leaf-list>
    </container>
    <leaf name="isolated-vlan">
      <description>
        <text>Isolated VLAN ID for private vlan bridge domain</text>
      </description>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="uint32">
          <range value="1 .. 4094"/>
        </type>
      </type>
    </leaf>
    <leaf-list name="community-vlans">
      <description>
        <text>List of Community VLANs for private vlan bridge domain</text>
      </description>
      <type name="jt:vlan-range"/>
    </leaf-list>
    <container name="proxy-mac">
      <description>
        <text>Proxy MAC settings</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="irb">
        <description>
          <text>Reply with virtual-gateway MAC or IRB MAC</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="proxy-mac-address">
        <description>
          <text>Reply with configured MAC for all requests</text>
        </description>
        <type name="jt:mac-unicast"/>
      </leaf>
    </container>
    <container name="multicast-snooping-options">
      <description>
        <text>Multicast snooping option configuration</text>
      </description>
      <uses name="juniper-multicast-snooping-options"/>
    </container>
  </grouping>
  <grouping name="juniper-bd-protocols-igmp-snooping">
    <description>
      <text>IGMP snooping options</text>
    </description>
    <uses name="apply-advanced"/>
    <container name="traceoptions">
      <description>
        <text>Trace options for IGMP Snooping</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="file">
        <description>
          <text>Trace file options</text>
        </description>
        <leaf name="filename">
          <description>
            <text>Name of file in which to write trace information</text>
          </description>
          <type name="string">
            <length value="1 .. 1024"/>
          </type>
        </leaf>
        <leaf name="replace">
          <description>
            <text>Replace trace file rather than appending to it</text>
          </description>
          <status value="deprecated"/>
          <type name="empty"/>
        </leaf>
        <leaf name="size">
          <description>
            <text>Maximum trace file size</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="files">
          <description>
            <text>Maximum number of trace files</text>
          </description>
          <default value="10"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="2 .. 1000"/>
            </type>
          </type>
        </leaf>
        <leaf name="no-stamp">
          <description>
            <text>Do not timestamp trace file</text>
          </description>
          <status value="deprecated"/>
          <type name="empty"/>
        </leaf>
        <choice name="world-readable-choice">
          <leaf name="world-readable">
            <description>
              <text>Allow any user to read the log file</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="no-world-readable">
            <description>
              <text>Don't allow any user to read the log file</text>
            </description>
            <type name="empty"/>
          </leaf>
        </choice>
      </container>
      <list name="flag">
        <key value="name"/>
        <ordered-by value="user"/>
        <description>
          <text>Tracing parameters</text>
        </description>
        <leaf name="name">
          <type name="enumeration">
            <enum name="packets">
              <description>
                <text>Trace all IGMP packets</text>
              </description>
            </enum>
            <enum name="query">
              <description>
                <text>Trace IGMP membership query messages</text>
              </description>
            </enum>
            <enum name="report">
              <description>
                <text>Trace membership report messages</text>
              </description>
            </enum>
            <enum name="leave">
              <description>
                <text>Trace leave group messages (IGMPv2 only)</text>
              </description>
            </enum>
            <enum name="group">
              <description>
                <text>Trace group operations</text>
              </description>
            </enum>
            <enum name="client-notification">
              <description>
                <text>Trace notifications</text>
              </description>
            </enum>
            <enum name="host-notification">
              <description>
                <text>Trace host notifications</text>
              </description>
            </enum>
            <enum name="route">
              <description>
                <text>Trace routing information</text>
              </description>
            </enum>
            <enum name="normal">
              <description>
                <text>Trace normal events</text>
              </description>
            </enum>
            <enum name="general">
              <description>
                <text>Trace general events</text>
              </description>
            </enum>
            <enum name="state">
              <description>
                <text>Trace state transitions</text>
              </description>
            </enum>
            <enum name="policy">
              <description>
                <text>Trace policy processing</text>
              </description>
            </enum>
            <enum name="task">
              <description>
                <text>Trace routing protocol task processing</text>
              </description>
            </enum>
            <enum name="timer">
              <description>
                <text>Trace routing protocol timer processing</text>
              </description>
            </enum>
            <enum name="all">
              <description>
                <text>Trace everything</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="send">
          <description>
            <text>Trace transmitted packets</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="receive">
          <description>
            <text>Trace received packets</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="detail">
          <description>
            <text>Trace detailed information</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="disable">
          <description>
            <text>Disable this trace flag</text>
          </description>
          <type name="empty"/>
        </leaf>
      </list>
    </container>
    <leaf name="query-interval">
      <description>
        <text>When to send host query messages</text>
      </description>
      <default value="125"/>
      <units name="seconds"/>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="uint32">
          <range value="1 .. 1024"/>
        </type>
      </type>
    </leaf>
    <container name="l2-querier">
      <description>
        <text>Enable L2 querier mode</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="source-address">
        <description>
          <text>Source IP address to use for L2 querier</text>
        </description>
        <type name="jt:ipv4addr"/>
      </leaf>
    </container>
    <leaf name="query-response-interval">
      <description>
        <text>How long to wait for a host query response</text>
      </description>
      <default value="10"/>
      <units name="seconds"/>
      <type name="string"/>
    </leaf>
    <leaf name="query-last-member-interval">
      <description>
        <text>When to send group query messages</text>
      </description>
      <default value="1"/>
      <units name="seconds"/>
      <type name="string"/>
    </leaf>
    <leaf name="robust-count">
      <description>
        <text>Expected packet loss on a subnet</text>
      </description>
      <default value="2"/>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="uint32">
          <range value="2 .. 10"/>
        </type>
      </type>
    </leaf>
    <leaf name="learn-pim-router">
      <description>
        <text>Learn PIM router interfaces from PIM hellos</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="immediate-leave">
      <description>
        <text>Enable immediate group leave on interfaces</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="evpn-ssm-reports-only">
      <description>
        <text>Accept and process only &lt;s,g&gt; reports of SSM groups</text>
      </description>
      <type name="empty"/>
    </leaf>
    <container name="proxy">
      <presence value="enable proxy"/>
      <description>
        <text>Enable proxy mode</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="source-address">
        <description>
          <text>Source IP address to use for proxy</text>
        </description>
        <type name="jt:ipv4addr"/>
      </leaf>
      <leaf name="irb">
        <description>
          <text>Proxy IGMP reports to IRB</text>
        </description>
        <type name="empty"/>
      </leaf>
    </container>
    <list name="interface">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Interface options for IGMP</text>
      </description>
      <leaf name="name">
        <description>
          <text>Interface name</text>
        </description>
        <type name="union">
          <type name="jt:interface-name"/>
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
        </type>
      </leaf>
      <uses name="apply-advanced"/>
      <leaf name="multicast-router-interface">
        <description>
          <text>Enabling multicast-router-interface on the interface</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="immediate-leave">
        <description>
          <text>Enable immediate group leave on interfaces</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="host-only-interface">
        <description>
          <text>Enable interfaces to be treated as host-side interfaces</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="group-limit">
        <description>
          <text>Maximum number of (source,group) per interface</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint16">
            <range value="1 .. 65535"/>
          </type>
        </type>
      </leaf>
      <container name="static">
        <description>
          <text>Static group or source membership</text>
        </description>
        <uses name="apply-advanced"/>
        <list name="group">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>IP multicast group address</text>
          </description>
          <leaf name="name">
            <description>
              <text>IP multicast group address</text>
            </description>
            <type name="jt:ipv4addr"/>
          </leaf>
          <uses name="apply-advanced"/>
          <list name="source">
            <key value="name"/>
            <ordered-by value="user"/>
            <description>
              <text>IP multicast source address</text>
            </description>
            <leaf name="name">
              <description>
                <text>Source address of IP multicast data</text>
              </description>
              <type name="jt:ipv4addr"/>
            </leaf>
            <uses name="apply-advanced"/>
          </list>
        </list>
      </container>
    </list>
    <list name="pseudowire-remote-address">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Pseudowire interface options for IGMP</text>
      </description>
      <leaf name="name">
        <description>
          <text>Pseudowire endpoint address</text>
        </description>
        <type name="jt:ipv4addr"/>
      </leaf>
      <uses name="apply-advanced"/>
      <leaf name="multicast-router-interface">
        <description>
          <text>Enabling multicast-router-interface on the interface</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="immediate-leave">
        <description>
          <text>Enable immediate group leave on interfaces</text>
        </description>
        <type name="empty"/>
      </leaf>
    </list>
    <list name="vlan">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Vlan options</text>
      </description>
      <leaf name="name">
        <description>
          <text>VLAN name</text>
        </description>
        <type name="string">
          <length value="1 .. 64"/>
        </type>
      </leaf>
      <uses name="apply-advanced"/>
      <leaf name="query-interval">
        <description>
          <text>When to send host query messages</text>
        </description>
        <units name="seconds"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 1024"/>
          </type>
        </type>
      </leaf>
      <leaf name="query-response-interval">
        <description>
          <text>How long to wait for a host query response</text>
        </description>
        <units name="seconds"/>
        <type name="string"/>
      </leaf>
      <leaf name="query-last-member-interval">
        <description>
          <text>When to send group query messages</text>
        </description>
        <units name="seconds"/>
        <type name="string"/>
      </leaf>
      <leaf name="robust-count">
        <description>
          <text>Expected packet loss on a subnet</text>
        </description>
        <type name="union">
