<?xml version="1.0" encoding="UTF-8"?>
<module name="junos-es-conf-services"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:jc-services="http://yang.juniper.net/junos-es/conf/services"
        xmlns:jt="http://yang.juniper.net/junos/common/types"
        xmlns:jc="http://yang.juniper.net/junos-es/conf/root">
  <namespace uri="http://yang.juniper.net/junos-es/conf/services"/>
  <prefix value="jc-services"/>
  <import module="junos-common-types">
    <prefix value="jt"/>
    <revision-date date="2019-01-01"/>
  </import>
  <import module="junos-es-conf-root">
    <prefix value="jc"/>
    <revision-date date="2019-01-01"/>
  </import>
  <organization>
    <text>Juniper Networks, Inc.</text>
  </organization>
  <contact>
    <text>yang-support@juniper.net</text>
  </contact>
  <description>
    <text>Junos services configuration module</text>
  </description>
  <revision date="2019-01-01">
    <description>
      <text>Junos: 21.3R1.9</text>
    </description>
  </revision>
  <augment target-node="/jc:configuration">
    <uses name="services-group"/>
  </augment>
  <augment target-node="/jc:configuration/jc:groups">
    <uses name="services-group"/>
  </augment>
  <grouping name="services-group">
    <container name="services">
      <description>
        <text>System services</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="flow-tap">
        <description>
          <text>Configure flow-tap parameters</text>
        </description>
        <uses name="apply-advanced"/>
        <container name="family">
          <description>
            <text>Address family of packets to tap</text>
          </description>
          <uses name="apply-advanced"/>
          <leaf name="inet">
            <description>
              <text>IPv4 family</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="inet6">
            <description>
              <text>IPv4 family</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="ccc">
            <description>
              <text>CCC family</text>
            </description>
            <type name="empty"/>
          </leaf>
        </container>
        <leaf name="interface">
          <description>
            <text>Service interface on which to configure flow-tap service</text>
          </description>
          <type name="union">
            <type name="jt:interface-name"/>
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
          </type>
        </leaf>
        <leaf name="tunnel-interface">
          <description>
            <text>Tunnel interface through which flow-tap would communicate with MD</text>
          </description>
          <type name="union">
            <type name="jt:interface-name"/>
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
          </type>
        </leaf>
      </container>
      <container name="radius-flow-tap">
        <description>
          <text>Configure radius triggered flow-tap parameters</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="forwarding-class">
          <description>
            <text>Forwarding class assigned to intercepted packets</text>
          </description>
          <type name="string"/>
        </leaf>
        <leaf name="source-ipv4-address">
          <description>
            <text>IP Address to use as source address in IPv4 header appended to intercepted packets</text>
          </description>
          <type name="jt:ipv4addr"/>
        </leaf>
        <leaf name="multicast-interception">
          <description>
            <text>Enable Multicast Tapping</text>
          </description>
          <type name="empty"/>
        </leaf>
        <container name="interfaces">
          <description>
            <text>Tunnel Interfaces</text>
          </description>
          <uses name="apply-advanced"/>
          <list name="tunnel-interface">
            <key value="name"/>
            <uses name="tunnel_interface_type"/>
          </list>
        </container>
        <choice name="ri_or_ls">
          <case name="case_1">
            <leaf name="routing-instance">
              <description>
                <text>Routing instance to be used for radius flow tap</text>
              </description>
              <type name="string"/>
            </leaf>
          </case>
          <case name="case_2">
            <list name="logical-system">
              <key value="name"/>
              <ordered-by value="user"/>
              <description>
                <text>Logical system to be used for radius flow tap</text>
              </description>
              <max-elements value="1"/>
              <leaf name="name">
                <description>
                  <text>Logical system name</text>
                </description>
                <type name="string"/>
              </leaf>
              <uses name="apply-advanced"/>
              <leaf name="routing-instance">
                <description>
                  <text>Routing instance to be used for radius flow tap</text>
                </description>
                <type name="string"/>
              </leaf>
            </list>
          </case>
        </choice>
        <list name="policy">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Policy</text>
          </description>
          <leaf name="name">
            <description>
              <text>Policy Name</text>
            </description>
            <type name="string"/>
          </leaf>
          <uses name="apply-advanced"/>
          <container name="inet">
            <description>
              <text>Protocol family IPv4 drop policy terms</text>
            </description>
            <uses name="apply-advanced"/>
            <list name="drop-policy">
              <key value="name"/>
              <ordered-by value="user"/>
              <description>
                <text>Define an IPv4 drop policy</text>
              </description>
              <uses name="drop-policy-term"/>
            </list>
          </container>
          <container name="inet6">
            <description>
              <text>Protocol family IPv6 drop policy terms</text>
            </description>
            <uses name="apply-advanced"/>
            <list name="drop-policy">
              <key value="name"/>
              <ordered-by value="user"/>
              <description>
                <text>Define an IPv6 drop policy</text>
              </description>
              <uses name="drop-policy6-term"/>
            </list>
          </container>
        </list>
        <container name="snmp">
          <description>
            <text>SNMP options for radius flow tap</text>
          </description>
          <uses name="apply-advanced"/>
          <list name="notify-targets">
            <key value="name"/>
            <ordered-by value="user"/>
            <description>
              <text>Target list for packet mirror SNMP notifications</text>
            </description>
            <leaf name="name">
              <description>
                <text>Target IP address</text>
              </description>
              <type name="jt:ipaddr"/>
            </leaf>
            <uses name="apply-advanced"/>
          </list>
        </container>
      </container>
      <container name="mobile-flow-tap">
        <description>
          <text>Configure mobile triggered flow-tap parameters</text>
        </description>
        <uses name="apply-advanced"/>
        <container name="source-interface">
          <presence value="enable source-interface"/>
          <description>
            <text>Source interface from which IRI packets will be sent</text>
          </description>
          <leaf name="interface-name">
            <description>
              <text>Interface name</text>
            </description>
            <type name="union">
              <type name="jt:interface-name"/>
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
            </type>
          </leaf>
          <leaf name="ipv4-address">
            <description>
              <text>Source IPv4 address to be used</text>
            </description>
            <type name="jt:ipv4addr"/>
          </leaf>
        </container>
      </container>
      <container name="flow-monitoring">
        <presence value="enable flow-monitoring"/>
        <description>
          <text>Configure flow monitoring</text>
        </description>
        <uses name="apply-advanced"/>
        <container name="version9">
          <description>
            <text>Version 9 configuration</text>
          </description>
          <uses name="apply-advanced"/>
          <list name="template">
            <key value="name"/>
            <ordered-by value="user"/>
            <description>
              <text>One or more version 9 templates</text>
            </description>
            <max-elements value="10"/>
            <uses name="version9-template"/>
          </list>
        </container>
        <container name="version-ipfix">
          <description>
            <text>Version IP-Fix configuration</text>
          </description>
          <uses name="apply-advanced"/>
          <list name="template">
            <key value="name"/>
            <ordered-by value="user"/>
            <description>
              <text>One or more version ip-fix templates</text>
            </description>
            <max-elements value="10"/>
            <uses name="version-ipfix-template"/>
          </list>
        </container>
      </container>
      <container name="jdaf">
        <description>
          <text>Juniper distributed application framework (JDAF)</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf-list name="routing-instances">
          <ordered-by value="user"/>
          <description>
            <text>List of routing-instance name for JDAF clients</text>
          </description>
          <type name="string">
            <length value="1 .. 128"/>
          </type>
        </leaf-list>
      </container>
      <container name="rpm">
        <presence value="enable rpm"/>
        <description>
          <text>Real-time performance monitoring</text>
        </description>
        <uses name="apply-advanced"/>
        <container name="traceoptions">
          <description>
            <text>RMOPD trace options</text>
          </description>
          <uses name="rmopd-traceoptions"/>
        </container>
        <container name="bgp">
          <description>
            <text>BGP options for real-time performance monitoring</text>
          </description>
          <uses name="apply-advanced"/>
          <leaf name="probe-type">
            <description>
              <text>RPM-BGP probe request type</text>
            </description>
            <default value="icmp-ping"/>
            <type name="enumeration">
              <enum name="icmp-ping">
                <description>
                  <text>Send ICMP echo request to target address</text>
                </description>
              </enum>
              <enum name="icmp-ping-timestamp">
                <description>
                  <text>Send ICMP timestamp request to target address</text>
                </description>
              </enum>
              <enum name="icmp6-ping">
                <description>
                  <text>Send ICMP6 echo request to target address</text>
                </description>
              </enum>
              <enum name="tcp-ping">
                <description>
                  <text>Send TCP packets to target</text>
                </description>
              </enum>
              <enum name="udp-ping">
                <description>
                  <text>Send UDP packets to target</text>
                </description>
              </enum>
              <enum name="udp-ping-timestamp">
                <description>
                  <text>Send UDP packets with timestamp to target</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="probe-count">
            <description>
              <text>Total number of probes per test</text>
            </description>
            <default value="1"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="1 .. 15"/>
              </type>
            </type>
          </leaf>
          <leaf name="probe-interval">
            <description>
              <text>Delay between probes</text>
            </description>
            <default value="3"/>
            <units name="seconds"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="1 .. 255"/>
              </type>
            </type>
          </leaf>
          <leaf name="test-interval">
            <description>
              <text>Delay between tests</text>
            </description>
            <default value="1"/>
            <units name="seconds"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="0 .. 86400"/>
              </type>
            </type>
          </leaf>
          <leaf name="destination-port">
            <description>
              <text>TCP/UDP port number</text>
            </description>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint16">
                <range value="7 .. 65535"/>
              </type>
            </type>
          </leaf>
          <leaf name="history-size">
            <description>
              <text>Number of stored history entries</text>
            </description>
            <default value="50"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="0 .. 512"/>
              </type>
            </type>
          </leaf>
          <leaf name="moving-average-size">
            <description>
              <text>Number of samples used for moving average</text>
            </description>
            <default value="0"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="0 .. 1024"/>
              </type>
            </type>
          </leaf>
          <leaf name="data-size">
            <description>
              <text>Size of the data portion of the probes</text>
            </description>
            <default value="0"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="0 .. 65400"/>
              </type>
            </type>
          </leaf>
          <leaf name="data-fill">
            <description>
              <text>Define contents of the data portion of the probes</text>
            </description>
            <type name="string">
              <length value="1 .. 2048"/>
            </type>
          </leaf>
          <leaf name="ttl">
            <description>
              <text>Time to Live (hop-limit) value for an RPM IPv4(IPv6) packet</text>
            </description>
            <default value="64"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="1 .. 254"/>
              </type>
            </type>
          </leaf>
          <list name="logical-system">
            <key value="name"/>
            <description>
              <text>Logical systems</text>
            </description>
            <uses name="bgp-logical-system"/>
          </list>
          <list name="routing-instances">
            <key value="name"/>
            <description>
              <text>Routing instances</text>
            </description>
            <uses name="bgp-routing-instances"/>
          </list>
        </container>
        <list name="probe">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>TCP/UDP/ICMP ping</text>
          </description>
          <leaf name="name">
            <description>
              <text>Name of owner</text>
            </description>
            <type name="string">
              <length value="1 .. 32"/>
            </type>
          </leaf>
          <uses name="apply-advanced"/>
          <leaf name="delegate-probes">
            <description>
              <text>Offload real-time performance monitoring probes to MS-MIC/MS-MPC card</text>
            </description>
            <type name="empty"/>
          </leaf>
          <list name="test">
            <key value="name"/>
            <ordered-by value="user"/>
            <description>
              <text>TCP/UDP/ICMP/ICMP6 ping test</text>
            </description>
            <leaf name="name">
              <description>
                <text>Name of test</text>
              </description>
              <type name="string">
                <length value="1 .. 32"/>
              </type>
            </leaf>
            <uses name="apply-advanced"/>
            <container name="rpm-scale">
              <presence value="enable rpm-scale"/>
              <description>
                <text>Configuring real-time performance monitoring scale tests</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="tests-count">
                <description>
                  <text>Number of probe-tests generated using scale config</text>
                </description>
                <type name="union">
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                  <type name="uint32">
                    <range value="1 .. 500000"/>
                  </type>
                </type>
              </leaf>
              <choice name="target-type">
                <case name="case_1">
                  <container name="target">
                    <presence value="enable target"/>
                    <description>
                      <text>Target address generation for scale test config</text>
                    </description>
                    <uses name="apply-advanced"/>
                    <leaf name="address-base">
                      <description>
                        <text>Base address of target host in a.b.c.d format</text>
                      </description>
                      <type name="jt:ipv4addr"/>
                    </leaf>
                    <leaf name="step">
                      <description>
                        <text>Steps to increment target address in a.b.c.d format</text>
                      </description>
                      <type name="jt:ipv4addr"/>
                    </leaf>
                    <leaf name="count">
                      <description>
                        <text>Target address count</text>
                      </description>
                      <type name="union">
                        <type name="string">
                          <pattern value="&lt;.*&gt;|$.*"/>
                        </type>
                        <type name="uint32">
                          <range value="1 .. 500000"/>
                        </type>
                      </type>
                    </leaf>
                  </container>
                </case>
                <case name="case_2">
                  <container name="target-inet6">
                    <presence value="enable target-inet6"/>
                    <description>
                      <text>IPv6 target address generation for scale test config</text>
                    </description>
                    <uses name="apply-advanced"/>
                    <leaf name="address-base">
                      <description>
                        <text>Base address of target host in a:b:c:d:e:f:g:h format</text>
                      </description>
                      <type name="jt:ipv6addr"/>
                    </leaf>
                    <leaf name="step">
                      <description>
                        <text>Steps to increment target address in a:b:c:d:e:f:g:h format</text>
                      </description>
                      <type name="jt:ipv6addr"/>
                    </leaf>
                    <leaf name="count">
                      <description>
                        <text>Target address count</text>
                      </description>
                      <type name="union">
                        <type name="string">
                          <pattern value="&lt;.*&gt;|$.*"/>
                        </type>
                        <type name="uint32">
                          <range value="1 .. 500000"/>
                        </type>
                      </type>
                    </leaf>
                  </container>
                </case>
              </choice>
              <choice name="source-type">
                <case name="case_1">
                  <container name="source">
                    <presence value="enable source"/>
                    <description>
                      <text>Source address generation in scale tests</text>
                    </description>
                    <uses name="apply-advanced"/>
                    <leaf name="address-base">
                      <description>
                        <text>Base address of host in a.b.c.d format</text>
                      </description>
                      <type name="jt:ipv4addr"/>
                    </leaf>
                    <leaf name="step">
                      <description>
                        <text>Steps to increment src address in a.b.c.d format</text>
                      </description>
                      <type name="jt:ipv4addr"/>
                    </leaf>
                    <leaf name="count">
                      <description>
                        <text>Source-address count</text>
                      </description>
                      <type name="union">
                        <type name="string">
                          <pattern value="&lt;.*&gt;|$.*"/>
                        </type>
                        <type name="uint32">
                          <range value="1 .. 500000"/>
                        </type>
                      </type>
                    </leaf>
                  </container>
                </case>
                <case name="case_2">
                  <container name="source-inet6">
                    <presence value="enable source-inet6"/>
                    <description>
                      <text>IPv6 source address generation in scale tests</text>
                    </description>
                    <uses name="apply-advanced"/>
                    <leaf name="address-base">
                      <description>
                        <text>Base address of host in a:b:c:d:e:f:g:h format</text>
                      </description>
                      <type name="jt:ipv6addr"/>
                    </leaf>
                    <leaf name="step">
                      <description>
                        <text>Steps to increment src address in a:b:c:d:e:f:g:h format</text>
                      </description>
                      <type name="jt:ipv6addr"/>
                    </leaf>
                    <leaf name="count">
                      <description>
                        <text>Source-address count</text>
                      </description>
                      <type name="union">
                        <type name="string">
                          <pattern value="&lt;.*&gt;|$.*"/>
                        </type>
                        <type name="uint32">
                          <range value="1 .. 500000"/>
                        </type>
                      </type>
                    </leaf>
                  </container>
                </case>
              </choice>
              <container name="destination">
                <presence value="enable destination"/>
                <description>
                  <text>Name of output interface for probes</text>
                </description>
                <uses name="apply-advanced"/>
                <leaf name="interface">
                  <description>
                    <text>Base destination interface for scale test</text>
                  </description>
                  <type name="union">
                    <type name="jt:interface-name"/>
                    <type name="string">
                      <pattern value="&lt;.*&gt;|$.*"/>
                    </type>
                  </type>
                </leaf>
                <leaf name="subunit-cnt">
                  <description>
                    <text>Subunit count for destination interface for scale test</text>
                  </description>
                  <type name="union">
                    <type name="string">
                      <pattern value="&lt;.*&gt;|$.*"/>
                    </type>
                    <type name="uint32">
                      <range value="1 .. 500000"/>
                    </type>
                  </type>
                </leaf>
              </container>
            </container>
            <leaf name="probe-type">
              <description>
                <text>Probe request type</text>
              </description>
              <default value="icmp-ping"/>
              <type name="enumeration">
                <enum name="http-get">
                  <description>
                    <text>Perform HTTP Get request at target URL</text>
                  </description>
                </enum>
                <enum name="http-metadata-get">
                  <description>
                    <text>Perform HTTP Get request of metadata at target URL</text>
                  </description>
                </enum>
                <enum name="icmp-ping">
                  <description>
                    <text>Send ICMP echo request to target address</text>
                  </description>
                </enum>
                <enum name="icmp-ping-timestamp">
                  <description>
                    <text>Send ICMP timestamp request to target address</text>
                  </description>
                </enum>
                <enum name="icmp6-ping">
                  <description>
                    <text>Send ICMP6 echo request to target address</text>
                  </description>
                </enum>
                <enum name="tcp-ping">
                  <description>
                    <text>Send TCP packets to target</text>
                  </description>
                </enum>
                <enum name="udp-ping">
                  <description>
                    <text>Send UDP packets to target</text>
                  </description>
                </enum>
                <enum name="udp-ping-timestamp">
                  <description>
                    <text>Send UDP packets with timestamp to target</text>
                  </description>
                </enum>
              </type>
            </leaf>
            <container name="target">
              <presence value="enable target"/>
              <description>
                <text>Target destination for probe</text>
              </description>
              <choice name="target-type">
                <case name="case_1">
                  <leaf name="address">
                    <description>
                      <text>Address of target host</text>
                    </description>
                    <type name="jt:ipv4addr"/>
                  </leaf>
                </case>
                <case name="case_2">
                  <leaf name="inet6-address">
                    <description>
                      <text>Inet6 Address of target host</text>
                    </description>
                    <type name="jt:ipv6addr"/>
                  </leaf>
                </case>
                <case name="case_3">
                  <leaf name="url">
                    <description>
                      <text>Fully formed target URL</text>
                    </description>
                    <type name="string"/>
                  </leaf>
                </case>
                <case name="case_4">
                  <leaf name="inet6-url">
                    <description>
                      <text>Fully formed target IPV6 URL</text>
                    </description>
                    <type name="string"/>
                  </leaf>
                </case>
              </choice>
            </container>
            <container name="inet6-options">
              <presence value="enable inet6-options"/>
              <description>
                <text>IPV6 related options</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="source-address">
                <description>
                  <text>Inet6 Source Address of the probe</text>
                </description>
                <type name="jt:ipv6addr"/>
              </leaf>
            </container>
            <leaf name="probe-count">
              <description>
                <text>Total number of probes per test</text>
              </description>
              <default value="1"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="1 .. 15"/>
                </type>
              </type>
            </leaf>
            <leaf name="probe-interval">
              <description>
                <text>Delay between probes</text>
              </description>
              <default value="3"/>
              <units name="seconds"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="1 .. 255"/>
                </type>
              </type>
            </leaf>
            <leaf name="test-interval">
              <description>
