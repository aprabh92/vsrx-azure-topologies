<?xml version="1.0" encoding="UTF-8"?>
<module name="junos-es-conf-wlan"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:jc-wlan="http://yang.juniper.net/junos-es/conf/wlan"
        xmlns:jt="http://yang.juniper.net/junos/common/types"
        xmlns:jc="http://yang.juniper.net/junos-es/conf/root">
  <namespace uri="http://yang.juniper.net/junos-es/conf/wlan"/>
  <prefix value="jc-wlan"/>
  <import module="junos-common-types">
    <prefix value="jt"/>
    <revision-date date="2019-01-01"/>
  </import>
  <import module="junos-es-conf-root">
    <prefix value="jc"/>
    <revision-date date="2019-01-01"/>
  </import>
  <organization>
    <text>Juniper Networks, Inc.</text>
  </organization>
  <contact>
    <text>yang-support@juniper.net</text>
  </contact>
  <description>
    <text>Junos wlan configuration module</text>
  </description>
  <revision date="2019-01-01">
    <description>
      <text>Junos: 21.3R1.9</text>
    </description>
  </revision>
  <augment target-node="/jc:configuration">
    <uses name="wlan-group"/>
  </augment>
  <augment target-node="/jc:configuration/jc:groups">
    <uses name="wlan-group"/>
  </augment>
  <grouping name="wlan-group">
    <container name="wlan">
      <presence value="enable wlan"/>
      <description>
        <text>Wireless access point configuration</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="admin-authentication">
        <description>
          <text>Authentication information for WLAN access points</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="encrypted-password">
          <description>
            <text>Encrypted password</text>
          </description>
          <type name="jt:unreadable"/>
        </leaf>
      </container>
      <list name="access-point">
        <key value="name"/>
        <ordered-by value="user"/>
        <description>
          <text>WLAN access point configuration</text>
        </description>
        <leaf name="name">
          <description>
            <text>Specify the access point name</text>
          </description>
          <type name="string"/>
        </leaf>
        <uses name="apply-advanced"/>
        <leaf name="description">
          <description>
            <text>Specify the access point description</text>
          </description>
          <type name="string">
            <length value="1 .. 64"/>
          </type>
        </leaf>
        <list name="interface">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Interface device name</text>
          </description>
          <uses name="ap-interface-list-type"/>
        </list>
        <leaf name="mac-address">
          <description>
            <text>Access point ethernet port MAC address</text>
          </description>
          <type name="jt:mac-unicast"/>
        </leaf>
        <leaf name="location">
          <description>
            <text>Location of the access point</text>
          </description>
          <type name="string">
            <length value="1 .. 64"/>
          </type>
        </leaf>
        <choice name="access-point-type">
          <case name="case_1">
            <container name="external">
              <presence value="enable external"/>
              <description>
                <text>External type access point</text>
              </description>
              <uses name="apply-advanced"/>
              <container name="system">
                <description>
                  <text>System information</text>
                </description>
                <uses name="apply-advanced"/>
                <leaf name="ntp-server">
                  <description>
                    <text>NTP server name</text>
                  </description>
                  <status value="deprecated"/>
                  <type name="string">
                    <length value="1 .. 256"/>
                  </type>
                </leaf>
                <container name="ports">
                  <description>
                    <text>Port configuration</text>
                  </description>
                  <uses name="apply-advanced"/>
                  <container name="ethernet">
                    <description>
                      <text>Ethernet settings</text>
                    </description>
                    <uses name="apply-advanced"/>
                    <leaf name="management-vlan">
                      <description>
                        <text>Management VLAN identifier</text>
                      </description>
                      <default value="1"/>
                      <type name="union">
                        <type name="string">
                          <pattern value="&lt;.*&gt;|$.*"/>
                        </type>
                        <type name="int32">
                          <range value="1 .. 4094"/>
                        </type>
                      </type>
                    </leaf>
                    <leaf name="untagged-vlan">
                      <description>
                        <text>Untagged VLAN identifier</text>
                      </description>
                      <default value="1"/>
                      <type name="union">
                        <type name="string">
                          <pattern value="&lt;.*&gt;|$.*"/>
                        </type>
                        <type name="int32">
                          <range value="1 .. 4094"/>
                        </type>
                      </type>
                    </leaf>
                    <choice name="inet-address">
                      <case name="case_1">
                        <container name="static">
                          <presence value="enable static"/>
                          <description>
                            <text>Specify static parameters</text>
                          </description>
                          <uses name="apply-advanced"/>
                          <leaf name="address">
                            <description>
                              <text>Static IP address</text>
                            </description>
                            <type name="jt:ipv4prefix"/>
                          </leaf>
                          <leaf name="default-gateway">
                            <description>
                              <text>Default gateway IP</text>
                            </description>
                            <type name="jt:ipaddr"/>
                          </leaf>
                        </container>
                      </case>
                    </choice>
                    <leaf-list name="name-server">
                      <ordered-by value="user"/>
                      <description>
                        <text>Domain name server IP address</text>
                      </description>
                      <type name="jt:ipaddr"/>
                      <max-elements value="2"/>
                    </leaf-list>
                  </container>
                </container>
                <container name="console">
                  <uses name="apply-advanced"/>
                  <leaf name="baud-rate">
                    <description>
                      <text>Baud-rate values are 9600,19200,38400,57600,115200</text>
                    </description>
                    <default value="115200"/>
                    <type name="union">
                      <type name="int32"/>
                      <type name="string">
                        <pattern value="&lt;.*&gt;|$.*"/>
                      </type>
                    </type>
                  </leaf>
                </container>
                <container name="services">
                  <description>
                    <text>System services configuration</text>
                  </description>
                  <uses name="apply-advanced"/>
                  <leaf name="ssh">
                    <description>
                      <text>Enable SSH service on the access point</text>
                    </description>
                    <type name="empty"/>
                  </leaf>
                </container>
              </container>
              <container name="dot1x-supplicant">
                <uses name="apply-advanced"/>
                <leaf name="username">
                  <description>
                    <text>Username</text>
                  </description>
                  <type name="string">
                    <length value="1 .. 64"/>
                  </type>
                </leaf>
                <leaf name="password">
                  <description>
                    <text>Dot1x supplicant mode password</text>
                  </description>
                  <type name="string">
                    <length value="1 .. 64"/>
                  </type>
                </leaf>
              </container>
            </container>
          </case>
        </choice>
        <container name="access-point-options">
          <description>
            <text>Configure access point options</text>
          </description>
          <uses name="ap-options-type"/>
        </container>
        <container name="logging-options">
          <description>
            <text>Configure access point logging options</text>
          </description>
          <uses name="log-options-type"/>
        </container>
        <container name="captive-portal">
          <presence value="enable captive-portal"/>
          <description>
            <text>Access point captive portal settings</text>
          </description>
          <uses name="apply-advanced"/>
          <container name="authentication">
            <description>
              <text>Authentication settings for captive portal</text>
            </description>
            <presence value="enable authentication"/>
            <uses name="captive-portal-auth-type"/>
          </container>
          <container name="dhcp">
            <presence value="enable dhcp"/>
            <description>
              <text>DHCP settings</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="gateway">
              <description>
                <text>Set DHCP server IP address</text>
              </description>
              <type name="jt:ipaddr"/>
            </leaf>
            <leaf name="port">
              <description>
                <text>Set DHCP server port</text>
              </description>
              <default value="67"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint16">
                  <range value="1 .. 65535"/>
                </type>
              </type>
            </leaf>
            <leaf name="relay-agent">
              <description>
                <text>Set DHCP relay agent</text>
              </description>
              <type name="jt:ipprefix"/>
            </leaf>
          </container>
          <container name="splash-page">
            <presence value="enable splash-page"/>
            <description>
              <text>Splash page settings</text>
            </description>
            <uses name="apply-advanced"/>
            <container name="title">
              <description>
                <text>Specify the title of splash page</text>
              </description>
              <presence value="enable title"/>
              <uses name="splash-page-title-type"/>
            </container>
            <container name="header">
              <description>
                <text>Specify the header of splash page</text>
              </description>
              <presence value="enable header"/>
              <uses name="splash-page-header-type"/>
            </container>
            <container name="footer">
              <description>
                <text>Specify the footer of splash page</text>
              </description>
              <presence value="enable footer"/>
              <uses name="splash-page-footer-type"/>
            </container>
            <leaf name="background-color">
              <description>
                <text>Specify the background color of splash page</text>
              </description>
              <type name="string"/>
            </leaf>
            <container name="logo-image">
              <description>
                <text>Specify the logo image for splash page</text>
              </description>
              <presence value="enable logo-image"/>
              <uses name="splash-page-logo-image-type"/>
            </container>
          </container>
          <container name="ssl">
            <presence value="enable ssl"/>
            <description>
              <text>SSL settings</text>
            </description>
            <uses name="apply-advanced"/>
            <choice name="enable-ssl">
              <case name="case_1">
                <leaf name="on">
                  <description>
                    <text>Turn on ssl</text>
                  </description>
                  <type name="empty"/>
                </leaf>
              </case>
              <case name="case_2">
                <leaf name="off">
                  <description>
                    <text>Turn off ssl</text>
                  </description>
                  <type name="empty"/>
                </leaf>
              </case>
            </choice>
            <leaf name="cert-file">
              <description>
                <text>Specify the certification file</text>
              </description>
              <type name="string"/>
            </leaf>
          </container>
          <leaf name="session-timeout">
            <description>
              <text>Specify the session timeout</text>
            </description>
            <default value="300"/>
            <units name="seconds"/>
            <type name="union">
              <type name="uint32"/>
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
            </type>
          </leaf>
          <leaf name="idle-timeout">
            <description>
              <text>Specify the idle timeout</text>
            </description>
            <default value="180"/>
            <units name="seconds"/>
            <type name="union">
              <type name="uint32"/>
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
            </type>
          </leaf>
        </container>
        <list name="radio">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Access point radio settings</text>
          </description>
          <max-elements value="2"/>
          <leaf name="name">
            <description>
              <text>Specify radio identifier value (1: 5GHz; 2: 2.4GHz)</text>
            </description>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="int32">
                <range value="1 .. 2"/>
              </type>
            </type>
          </leaf>
          <uses name="apply-advanced"/>
          <container name="radio-options">
            <description>
              <text>Configure radio options</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="radio-off">
              <description>
                <text>Radio is off</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="dfs-off">
              <description>
                <text>Dynamic frequency selection turn off</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="disable-dot11d">
              <description>
                <text>Disable dot11d regulatory domain support</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="mode">
              <description>
                <text>Mode for radio operation</text>
              </description>
              <type name="enumeration">
                <enum name="an">
                  <description>
                    <text>Wireless Mode a/n  (IEEE 802.11a/n)</text>
                  </description>
                </enum>
                <enum name="5GHz">
                  <description>
                    <text>Radio Frequency -5GHz-n</text>
                  </description>
                </enum>
                <enum name="2.4GHz">
                  <description>
                    <text>Radio Frequency -2.4GHz-n</text>
                  </description>
                </enum>
                <enum name="acn">
                  <description>
                    <text>Wireless Mode ac/n (IEEE 802.11ac/n)</text>
                  </description>
                </enum>
                <enum name="gn">
                  <description>
                    <text>Wireless Mode g/n (IEEE 802.11g/n)</text>
                  </description>
                </enum>
                <enum name="g">
                  <description>
                    <text>Wireless Mode g (IEEE 802.11g)</text>
                  </description>
                </enum>
              </type>
            </leaf>
            <container name="channel">
              <description>
                <text>Configure channel settings</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="number">
                <description>
                  <text>Provide channel selection</text>
                </description>
                <default value="auto"/>
                <type name="enumeration">
                  <enum name="1">
                    <description>
                      <text>Channel 1</text>
                    </description>
                  </enum>
                  <enum name="2">
                    <description>
                      <text>Channel 2</text>
                    </description>
                  </enum>
                  <enum name="3">
                    <description>
                      <text>Channel 3</text>
                    </description>
                  </enum>
                  <enum name="4">
                    <description>
                      <text>Channel 4</text>
                    </description>
                  </enum>
                  <enum name="5">
                    <description>
                      <text>Channel 5</text>
                    </description>
                  </enum>
                  <enum name="6">
                    <description>
                      <text>Channel 6</text>
                    </description>
                  </enum>
                  <enum name="7">
                    <description>
                      <text>Channel 7</text>
                    </description>
                  </enum>
                  <enum name="8">
                    <description>
                      <text>Channel 8</text>
                    </description>
                  </enum>
                  <enum name="9">
                    <description>
                      <text>Channel 9</text>
                    </description>
                  </enum>
                  <enum name="10">
                    <description>
                      <text>Channel 10</text>
                    </description>
                  </enum>
                  <enum name="11">
                    <description>
                      <text>Channel 11</text>
                    </description>
                  </enum>
                  <enum name="12">
                    <description>
                      <text>Channel 12</text>
                    </description>
                  </enum>
                  <enum name="13">
                    <description>
                      <text>Channel 13</text>
                    </description>
                  </enum>
                  <enum name="36">
                    <description>
                      <text>Channel 36</text>
                    </description>
                  </enum>
                  <enum name="40">
                    <description>
                      <text>Channel 40</text>
                    </description>
                  </enum>
                  <enum name="44">
                    <description>
                      <text>Channel 44</text>
                    </description>
                  </enum>
                  <enum name="48">
                    <description>
                      <text>Channel 48</text>
                    </description>
                  </enum>
                  <enum name="52">
                    <description>
                      <text>Channel 52</text>
                    </description>
                  </enum>
                  <enum name="56">
                    <description>
                      <text>Channel 56</text>
                    </description>
                  </enum>
                  <enum name="60">
                    <description>
                      <text>Channel 60</text>
                    </description>
                  </enum>
                  <enum name="64">
                    <description>
                      <text>Channel 64</text>
                    </description>
                  </enum>
                  <enum name="100">
                    <description>
                      <text>Channel 100</text>
                    </description>
                  </enum>
                  <enum name="104">
                    <description>
                      <text>Channel 104</text>
                    </description>
                  </enum>
                  <enum name="108">
                    <description>
                      <text>Channel 108</text>
                    </description>
                  </enum>
                  <enum name="112">
                    <description>
                      <text>Channel 112</text>
                    </description>
                  </enum>
                  <enum name="116">
                    <description>
                      <text>Channel 116</text>
                    </description>
                  </enum>
                  <enum name="120">
                    <description>
                      <text>Channel 120</text>
                    </description>
                  </enum>
                  <enum name="124">
                    <description>
                      <text>Channel 124</text>
                    </description>
                  </enum>
                  <enum name="128">
                    <description>
                      <text>Channel 128</text>
                    </description>
                  </enum>
                  <enum name="132">
                    <description>
                      <text>Channel 132</text>
                    </description>
                  </enum>
                  <enum name="136">
                    <description>
                      <text>Channel 136</text>
                    </description>
                  </enum>
                  <enum name="140">
                    <description>
                      <text>Channel 140</text>
                    </description>
                  </enum>
                  <enum name="144">
                    <description>
                      <text>Channel 144</text>
                    </description>
                  </enum>
                  <enum name="149">
                    <description>
                      <text>Channel 149</text>
                    </description>
                  </enum>
                  <enum name="153">
                    <description>
                      <text>Channel 153</text>
                    </description>
                  </enum>
                  <enum name="157">
                    <description>
                      <text>Channel 157</text>
                    </description>
                  </enum>
                  <enum name="161">
                    <description>
                      <text>Channel 161</text>
                    </description>
                  </enum>
                  <enum name="165">
                    <description>
                      <text>Channel 165</text>
                    </description>
                  </enum>
                  <enum name="auto">
                    <description>
                      <text>Automatically selected</text>
                    </description>
                  </enum>
                </type>
              </leaf>
              <leaf name="bandwidth">
                <description>
                  <text>Provide channel bandwidth in MHz(2.4G: 20/40; 5G: 20/40/80)</text>
                </description>
                <type name="union">
                  <type name="int32"/>
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                </type>
              </leaf>
              <leaf name="primary">
                <description>
                  <text>Select primary channel</text>
                </description>
                <type name="enumeration">
                  <enum name="lower">
                    <description>
                      <text>Set lower channel as primary</text>
                    </description>
                  </enum>
                  <enum name="upper">
                    <description>
                      <text>Set upper channel as primary</text>
                    </description>
                  </enum>
                </type>
              </leaf>
            </container>
            <leaf name="space-time-block-coding">
              <description>
                <text>Enable space time block coding</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="protection">
              <description>
                <text>Select protection mode</text>
              </description>
              <type name="enumeration">
                <enum name="auto">
                  <description>
                    <text>Protection mode- AUTO</text>
                  </description>
                </enum>
                <enum name="off">
                  <description>
                    <text>Protection mode- OFF</text>
                  </description>
                </enum>
              </type>
            </leaf>
            <leaf name="beacon-interval">
              <description>
                <text>Set the beacon interval</text>
              </description>
              <default value="100"/>
              <units name="milliseconds"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="int32">
                  <range value="20 .. 2000"/>
                </type>
              </type>
            </leaf>
            <leaf name="dtim-period">
              <description>
                <text>Set delivery traffic indication message period (beacon count)</text>
              </description>
              <default value="2"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="int32">
                  <range value="1 .. 255"/>
                </type>
              </type>
            </leaf>
            <leaf name="rts-threshold">
              <description>
                <text>Set RTS- threshold</text>
              </description>
              <default value="2347"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="int32">
                  <range value="0 .. 2347"/>
                </type>
              </type>
            </leaf>
            <leaf name="no-short-guard-interval">
              <description>
                <text>Disable 802.11n short inter-symbol guard interval</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="fragmentation-threshold">
              <description>
                <text>Packet fragment size(even numbers)</text>
              </description>
              <default value="2346"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="int32">
                  <range value="256 .. 2346"/>
                </type>
              </type>
            </leaf>
            <leaf name="maximum-stations">
              <description>
                <text>Set limit on maximum supported stations</text>
              </description>
              <default value="200"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="int32">
                  <range value="0 .. 200"/>
                </type>
              </type>
            </leaf>
            <leaf name="transmit-power">
              <description>
                <text>Set transmit power</text>
              </description>
              <default value="100"/>
              <units name="percent"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="int32">
                  <range value="3 .. 100"/>
                </type>
              </type>
            </leaf>
            <leaf name="fixed-multicast-rate">
              <description>
                <text>Provide multicast rate</text>
              </description>
              <type name="enumeration">
                <enum name="auto">
                  <description>
                    <text>Auto rate selection</text>
                  </description>
                </enum>
                <enum name="1">
                  <description>
                    <text>Rate-1Mbps </text>
                  </description>
                </enum>
                <enum name="2">
                  <description>
                    <text>Rate-2Mbps </text>
                  </description>
                </enum>
                <enum name="5.5">
                  <description>
                    <text>Rate-5.5Mbps </text>
                  </description>
                </enum>
                <enum name="6">
                  <description>
                    <text>Rate-6Mbps </text>
                  </description>
                </enum>
                <enum name="9">
                  <description>
                    <text>Rate-9Mbps </text>
                  </description>
                </enum>
                <enum name="11">
                  <description>
                    <text>Rate-11Mbps </text>
                  </description>
                </enum>
                <enum name="12">
                  <description>
                    <text>Rate-12Mbps </text>
                  </description>
                </enum>
                <enum name="18">
                  <description>
                    <text>Rate-18Mbps </text>
                  </description>
                </enum>
                <enum name="24">
                  <description>
                    <text>Rate-24Mbps </text>
                  </description>
                </enum>
                <enum name="36">
                  <description>
                    <text>Rate-36Mbps </text>
                  </description>
                </enum>
                <enum name="48">
                  <description>
                    <text>Rate-48Mbps </text>
                  </description>
                </enum>
                <enum name="54">
                  <description>
                    <text>Rate-54Mbps </text>
                  </description>
                </enum>
              </type>
            </leaf>
            <container name="transmit-rate-sets">
              <description>
                <text>Specify the transmit rate sets</text>
              </description>
              <uses name="transmit-rate-sets"/>
            </container>
            <container name="broadcast-multicast-rate-limit">
              <description>
                <text>Configure rate limit for broadcast and multicast traffic</text>
              </description>
              <uses name="broadcast-multicast-rate-limit"/>
            </container>
          </container>
          <leaf name="station-isolation">
            <description>
              <text>Isolate the clients connected to the same VAP within a radio</text>
            </description>
            <type name="empty"/>
          </leaf>
          <list name="virtual-access-point">
            <key value="name"/>
            <ordered-by value="user"/>
            <description>
              <text>Virtual access point configuration</text>
            </description>
            <max-elements value="8"/>
            <uses name="vap-type"/>
          </list>
          <container name="quality-of-service">
            <description>
              <text>Wireless quality of service configuration</text>
            </description>
            <uses name="qos-parameter-type"/>
          </container>
        </list>
      </list>
      <container name="syslog-options">
        <description>
          <text>Configure Access Point Logging options</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="log-size">
          <description>
            <text>Configure Max log file size</text>
          </description>
          <units name="kilobytes"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="4 .. 1024"/>
            </type>
          </type>
        </leaf>
        <leaf name="period">
          <description>
            <text>Configure log fetch interval</text>
          </description>
          <units name="seconds"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="int32">
              <range value="60 .. 86400"/>
            </type>
          </type>
        </leaf>
      </container>
    </container>
  </grouping>
  <grouping name="ap-interface-list-type">
    <description>
      <text>Logical interface in this cluster</text>
    </description>
    <leaf name="name">
      <description>
        <text>Interface device name</text>
      </description>
      <type name="union">
        <type name="jt:interface-device"/>
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
      </type>
    </leaf>
    <uses name="apply-advanced"/>
  </grouping>
  <grouping name="ap-options-type">
    <description>
      <text>Configure access point options</text>
    </description>
    <uses name="apply-advanced"/>
    <container name="country">
      <description>
        <text>Country name</text>
      </description>
      <uses name="country"/>
    </container>
    <container name="station-mac-filter">
      <uses name="station-mac-filter"/>
    </container>
  </grouping>
  <grouping name="apply-advanced">
    <description>
      <text>Apply advanced configuration logic</text>
    </description>
    <leaf-list name="apply-groups">
      <ordered-by value="user"/>
      <description>
        <text>Groups from which to inherit configuration data</text>
      </description>
      <type name="string"/>
    </leaf-list>
    <leaf-list name="apply-groups-except">
      <ordered-by value="user"/>
      <description>
        <text>Don't inherit configuration data from these groups</text>
      </description>
      <type name="string"/>
    </leaf-list>
    <list name="apply-macro">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Macro and parameters for commit script expansion</text>
      </description>
      <uses name="apply-macro-type"/>
    </list>
  </grouping>
  <grouping name="apply-macro-type">
    <description>
      <text>Macro data for commit-script expansion</text>
    </description>
    <leaf name="name">
      <description>
        <text>Name of the macro to be expanded</text>
      </description>
      <type name="string"/>
    </leaf>
    <list name="data">
      <key value="name"/>
      <uses name="macro-data-type"/>
    </list>
  </grouping>
  <grouping name="broadcast-multicast-rate-limit">
    <description>
      <text>Specify broadcast and multicast rate limit</text>
    </description>
    <uses name="apply-advanced"/>
    <leaf name="rate-limit">
      <description>
        <text>Rate limit</text>
      </description>
      <default value="50"/>
      <units name="pps"/>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="int32">
          <range value="1 .. 50"/>
        </type>
      </type>
    </leaf>
    <leaf name="rate-limit-bursts">
      <description>
        <text>Rate limit bursts</text>
      </description>
      <default value="75"/>
      <units name="pps"/>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="int32">
          <range value="1 .. 75"/>
        </type>
      </type>
    </leaf>
  </grouping>
  <grouping name="captive-portal-auth-type">
    <description>
      <text>Configure authentication settings</text>
    </description>
    <uses name="apply-advanced"/>
    <choice name="authentication-type">
      <case name="case_1">
        <container name="simple-pass">
          <description>
            <text>Simple password configuration</text>
          </description>
          <presence value="enable simple-pass"/>
          <uses name="simple-pass-type"/>
        </container>
      </case>
      <case name="case_2">
        <container name="external-server">
          <description>
            <text>External server configuration</text>
          </description>
          <presence value="enable external-server"/>
          <uses name="external-server-type"/>
        </container>
      </case>
    </choice>
  </grouping>
