    <leaf name="vlan">
      <description>
        <text>VLAN id for the virtual access point</text>
      </description>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="int32">
          <range value="1 .. 4094"/>
        </type>
      </type>
    </leaf>
    <leaf name="download-limit">
      <description>
        <text>Download rate limit Kbps</text>
      </description>
      <type name="union">
        <type name="string">
          <pattern value="&lt;.*&gt;|$.*"/>
        </type>
        <type name="int32">
          <range value="256 .. 1048576"/>
        </type>
      </type>
    </leaf>
    <leaf name="upload-limit">
      <description>
        <text>Upload rate limit Kbps</text>
      </description>
      <type name="union">
        <type name="string">
