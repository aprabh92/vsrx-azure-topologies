  <grouping name="security">
    <description>
      <text>Configure one of security settings for the VAP</text>
    </description>
    <uses name="apply-advanced"/>
    <leaf name="mac-authentication-type">
      <description>
        <text>Select MAC authentication type</text>
      </description>
      <type name="enumeration">
        <enum name="disabled">
          <description>
            <text>Disable MAC authentication</text>
          </description>
        </enum>
        <enum name="local">
          <description>
            <text>Select local MAC authentication</text>
          </description>
        </enum>
        <enum name="radius">
          <description>
            <text>Select RADIUS server authentication</text>
          </description>
        </enum>
      </type>
    </leaf>
    <choice name="security-type">
      <case name="case_1">
        <container name="static-wep">
          <description>
            <text>Static WEP configuration</text>
          </description>
          <presence value="enable static-wep"/>
          <uses name="static-wep"/>
        </container>
      </case>
      <case name="case_2">
        <container name="dot1x">
          <description>
            <text>Dot1x configuration</text>
          </description>
          <presence value="enable dot1x"/>
          <uses name="dot1x"/>
        </container>
      </case>
      <case name="case_3">
        <container name="wpa-personal">
          <description>
            <text>Set WPA personal settings</text>
          </description>
          <presence value="enable wpa-personal"/>
          <uses name="wpa-personal"/>
        </container>
      </case>
      <case name="case_4">
        <container name="wpa-enterprise">
          <description>
            <text>Set WAP enterprise settings</text>
          </description>
          <presence value="enable wpa-enterprise"/>
          <uses name="wpa-enterprise"/>
        </container>
      </case>
      <case name="case_5">
        <leaf name="none">
          <description>
            <text>Unsecure plain-text communication</text>
          </description>
          <type name="empty"/>
        </leaf>
      </case>
    </choice>
  </grouping>
