        <container name="layer-4">
          <presence value="enable layer-4"/>
          <description>
            <text>Include Layer 4 (TCP or UDP) data in the hash key</text>
          </description>
          <uses name="apply-advanced"/>
          <leaf name="destination-port">
            <description>
              <text>Include IP destination port in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="source-port">
            <description>
              <text>Include IP source port in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
        </container>
        <leaf name="session-id">
          <description>
            <text>Include session ID in the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
      <container name="mpls">
        <description>
          <text>MPLS protocol family</text>
        </description>
        <uses name="apply-advanced"/>
        <choice name="label">
          <case name="case_1">
            <leaf name="label-1">
              <description>
                <text>Include the first MPLS label in the hash key</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
          <case name="case_2">
            <leaf name="all-labels">
              <description>
                <text>Include all MPLS labels in hash key</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
          <case name="case_3">
            <leaf name="no-labels">
              <description>
                <text>Exclude all MPLS labels from hash key</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
          <case name="case_4">
            <leaf name="bottom-label-1">
              <description>
