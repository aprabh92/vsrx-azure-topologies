          <description>
            <text>Match error conditions</text>
          </description>
        </enum>
        <enum name="warning">
          <description>
            <text>Match warning messages</text>
          </description>
        </enum>
        <enum name="notice">
          <description>
            <text>Match conditions that should be handled specially</text>
          </description>
        </enum>
        <enum name="info">
          <description>
            <text>Match informational messages</text>
          </description>
        </enum>
        <enum name="verbose">
          <description>
            <text>Match verbose messages</text>
          </description>
        </enum>
        <enum name="all">
          <description>
            <text>Match all levels</text>
          </description>
        </enum>
      </type>
    </leaf>
    <list name="flag">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Area of route-record to enable debuging output</text>
      </description>
      <leaf name="name">
        <type name="enumeration">
          <enum name="parse">
            <description>
              <text>Trace parsing code</text>
            </description>
          </enum>
          <enum name="all">
            <description>
              <text>Trace all area of jflow-service code</text>
            </description>
          </enum>
        </type>
      </leaf>
    </list>
