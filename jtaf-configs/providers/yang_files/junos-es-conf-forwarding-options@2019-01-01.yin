        </container>
        <leaf name="session-id">
          <description>
            <text>Include session ID in the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
      <container name="inet6">
        <description>
          <text>IPv6 protocol family</text>
        </description>
        <uses name="apply-advanced"/>
        <container name="layer-3">
          <presence value="enable layer-3"/>
          <description>
            <text>Include Layer 3 (IP) data in the hash key</text>
          </description>
          <uses name="apply-advanced"/>
          <leaf name="destination-address">
            <description>
              <text>Include IP destination address in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="incoming-interface-index">
            <description>
              <text>Include incoming interface index in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="next-header">
            <description>
              <text>Include IP Next Header field in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="source-address">
            <description>
              <text>Include IP source address in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="traffic-class">
            <description>
              <text>Include Traffic Class field in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
