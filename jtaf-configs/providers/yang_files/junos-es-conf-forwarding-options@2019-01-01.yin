<?xml version="1.0" encoding="UTF-8"?>
<module name="junos-es-conf-forwarding-options"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:jc-forwarding-options="http://yang.juniper.net/junos-es/conf/forwarding-options"
        xmlns:jt="http://yang.juniper.net/junos/common/types"
        xmlns:jc="http://yang.juniper.net/junos-es/conf/root">
  <namespace uri="http://yang.juniper.net/junos-es/conf/forwarding-options"/>
  <prefix value="jc-forwarding-options"/>
  <import module="junos-common-types">
    <prefix value="jt"/>
    <revision-date date="2019-01-01"/>
  </import>
  <import module="junos-es-conf-root">
    <prefix value="jc"/>
    <revision-date date="2019-01-01"/>
  </import>
  <organization>
    <text>Juniper Networks, Inc.</text>
  </organization>
  <contact>
    <text>yang-support@juniper.net</text>
  </contact>
  <description>
    <text>Junos forwarding-options configuration module</text>
  </description>
  <revision date="2019-01-01">
    <description>
      <text>Junos: 21.3R1.9</text>
    </description>
  </revision>
  <augment target-node="/jc:configuration">
    <uses name="forwarding-options-group"/>
  </augment>
  <augment target-node="/jc:configuration/jc:groups">
    <uses name="forwarding-options-group"/>
  </augment>
  <grouping name="forwarding-options-group">
    <container name="forwarding-options">
      <description>
        <text>Configure options to control packet forwarding</text>
      </description>
      <uses name="juniper-forwarding-options"/>
    </container>
  </grouping>
  <grouping name="juniper-forwarding-options">
    <uses name="apply-advanced"/>
    <list name="storm-control-profiles">
      <key value="name"/>
      <description>
        <text>Storm control profile for this instance</text>
      </description>
      <leaf name="name">
        <description>
          <text>Storm control profile name</text>
        </description>
        <type name="string">
          <length value="1 .. 127"/>
        </type>
      </leaf>
      <uses name="apply-advanced"/>
      <container name="all">
        <presence value="enable all"/>
        <description>
          <text>For all BUM traffic</text>
        </description>
        <uses name="apply-advanced"/>
        <choice name="bandwidth">
          <case name="case_1">
            <leaf name="bandwidth-percentage">
              <description>
                <text>Percentage of link bandwidth</text>
              </description>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="1 .. 100"/>
                </type>
              </type>
            </leaf>
          </case>
          <case name="case_2">
            <leaf name="bandwidth-level">
              <description>
                <text>Link bandwidth</text>
              </description>
              <units name="kbps"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32"/>
              </type>
            </leaf>
          </case>
        </choice>
        <leaf name="burst-size">
          <description>
            <text>Burst size</text>
          </description>
          <units name="bytes"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32"/>
          </type>
        </leaf>
        <leaf name="no-broadcast">
          <description>
            <text>Disable broadcast storm control</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-unknown-unicast">
          <description>
            <text>Disable unknown unicast storm control</text>
          </description>
          <type name="empty"/>
        </leaf>
        <choice name="no-multicast-choices">
          <case name="case_1">
            <leaf name="no-multicast">
              <description>
                <text>Disable multicast storm control</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
          <case name="case_2">
            <leaf name="no-registered-multicast">
              <description>
                <text>Disable registered multicast storm control</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
          <case name="case_3">
            <leaf name="no-unregistered-multicast">
              <description>
                <text>Disable unregistered multicast storm control</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
        </choice>
      </container>
      <leaf name="action-shutdown">
        <description>
          <text>Disable port for excessive storm control errors</text>
        </description>
        <type name="empty"/>
      </leaf>
    </list>
    <choice name="sampling-or-packet-capture">
      <case name="case_1">
        <container name="sampling">
          <description>
            <text>Statistical traffic sampling options</text>
          </description>
          <uses name="juniper-sampling-options"/>
        </container>
      </case>
      <case name="case_2">
        <container name="packet-capture">
          <description>
            <text>Packet capture options</text>
          </description>
          <uses name="juniper-packet-capture-options"/>
        </container>
      </case>
    </choice>
    <list name="accounting">
      <key value="name"/>
      <description>
        <text>Configure accounting of traffic</text>
      </description>
      <uses name="juniper-packet-accounting-options"/>
    </list>
    <container name="port-mirroring">
      <description>
        <text>Configure port mirroring of traffic</text>
      </description>
      <uses name="juniper-port-mirror-options"/>
    </container>
    <container name="evpn-vxlan">
      <description>
        <text>EVPN VXLAN configurations</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="shared-tunnels">
        <description>
          <text>Create VTEP tunnels to EVPN PE</text>
        </description>
        <type name="empty"/>
      </leaf>
      <container name="reroute-address">
        <description>
          <text>Reroute IP address for vxlan encapsulation</text>
        </description>
        <choice name="family">
          <case name="case_1">
            <container name="inet">
              <presence value="enable inet"/>
              <description>
                <text>IPv4 address family</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="address">
                <description>
                  <text>Reroute address for IPV4 FRR</text>
                </description>
                <type name="jt:ipv4addr"/>
              </leaf>
            </container>
          </case>
          <case name="case_2">
            <container name="inet6">
              <presence value="enable inet6"/>
              <description>
                <text>IPv6 address family</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="address">
                <description>
                  <text>Reroute address for IPV6 FRR</text>
                </description>
                <type name="jt:ipv6addr"/>
              </leaf>
            </container>
          </case>
        </choice>
      </container>
    </container>
    <container name="multicast-replication">
      <description>
        <text>Set mode of multicast replication</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="evpn">
        <description>
          <text>EVPN IRB multicast related options</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="irb">
          <type name="enumeration">
            <enum name="local-only">
              <description>
                <text>Multicast forward in local-only mode</text>
              </description>
            </enum>
            <enum name="local-remote">
              <description>
                <text>Multicast forward in local-remote mode</text>
              </description>
            </enum>
            <enum name="oism">
              <description>
                <text>Optimized inter subnet multicast mode</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="smet-nexthop-limit">
          <description>
            <text>Flood traffic to all egress when count goes above limit</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="10000 .. 40000"/>
            </type>
          </type>
        </leaf>
      </container>
    </container>
    <container name="load-balance">
      <description>
        <text>Configure load-balancing attributes on the forwarding path</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="indexed-load-balance">
        <presence value="enable indexed-load-balance"/>
        <description>
          <text>Use indexed permuted next hop lists for unilist and aggregate next hops</text>
        </description>
        <uses name="apply-advanced"/>
      </container>
      <container name="per-flow">
        <uses name="apply-advanced"/>
      </container>
      <container name="per-prefix">
        <presence value="enable per-prefix"/>
        <uses name="apply-advanced"/>
        <leaf name="hash-seed">
          <description>
            <text>Specifies per-router input value for per-prefix load-balancing hash function</text>
          </description>
          <default value="0"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint16">
              <range value="0 .. 65534"/>
            </type>
          </type>
        </leaf>
      </container>
    </container>
    <container name="hash-key">
      <description>
        <text>Select data used in the hash key</text>
      </description>
      <uses name="junos_hash_key"/>
    </container>
    <container name="enhanced-hash-key">
      <description>
        <text>Select data used in the hash key for Enhanced IP Forwarding Engines</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="lag-dlb">
        <presence value="enable lag-dlb"/>
        <description>
          <text>Setting dynamic load balancing options for LAG</text>
        </description>
        <uses name="apply-advanced"/>
        <container name="ether-type">
          <presence value="enable ether-type"/>
          <description>
            <text>Setting ether type for LAG DLB</text>
          </description>
          <uses name="apply-advanced"/>
          <leaf name="ipv4">
            <description>
              <text>Ether type is ipv4</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="ipv6">
            <description>
              <text>Ether type is ipv6</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="mpls">
            <description>
              <text>Ether type is mpls</text>
            </description>
            <type name="empty"/>
          </leaf>
        </container>
      </container>
      <container name="ecmp-dlb">
        <description>
          <text>Set load balancing options for VC-Fabric forwarding</text>
        </description>
        <uses name="apply-advanced"/>
        <choice name="mode">
          <case name="case_1">
            <leaf name="assigned-flow">
              <description>
                <text>Flow-based fixed link assignment</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
          <case name="case_2">
            <container name="flowlet">
              <presence value="enable flowlet"/>
              <description>
                <text>Inactivity-based flowlet link assignment (default)</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="inactivity-interval">
                <description>
                  <text>Minimum inactivity interval in micro-seconds for flowlet detection</text>
                </description>
                <default value="256"/>
                <type name="union">
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                  <type name="uint32"/>
                </type>
              </leaf>
            </container>
          </case>
          <case name="case_3">
            <leaf name="per-packet">
              <description>
                <text>Per-packet optimal spraying</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
        </choice>
        <container name="ether-type">
          <presence value="enable ether-type"/>
          <description>
            <text>Ether type</text>
          </description>
          <uses name="apply-advanced"/>
          <leaf name="ipv4">
            <description>
              <text>Set DLB for ipv4</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="ipv6">
            <description>
              <text>Set DLB for ipv6</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="mpls">
            <description>
              <text>Set DLB for mpls</text>
            </description>
            <type name="empty"/>
          </leaf>
        </container>
      </container>
      <container name="hash-seed">
        <presence value="enable hash-seed"/>
        <description>
          <text>Hash seed</text>
        </description>
        <uses name="apply-advanced"/>
      </container>
      <container name="mpls">
        <description>
          <text>Configure mpls fields</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="no-incoming-port">
          <description>
            <text>Exclude incoming port from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-incoming-device">
          <description>
            <text>Exclude incoming device from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
      <container name="layer2">
        <description>
          <text>Configure layer2 fields</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="no-incoming-port">
          <description>
            <text>Exclude incoming port from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-incoming-device">
          <description>
            <text>Exclude incoming device from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-source-mac-address">
          <description>
            <text>Exclude source MAC address from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-destination-mac-address">
          <description>
            <text>Exclude destination MAC address from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-ether-type">
          <description>
            <text>Exclude ether type from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="vlan-id">
          <description>
            <text>Include incoming vlan-id in hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
      <container name="inet">
        <description>
          <text>Configure inet4 fields</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="no-incoming-port">
          <description>
            <text>Exclude incoming port from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-incoming-device">
          <description>
            <text>Exclude incoming device from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-l4-source-port">
          <description>
            <text>Exclude l4 source port from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-l4-destination-port">
          <description>
            <text>Exclude l4 dest port from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-protocol">
          <description>
            <text>Exclude protocol from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-ipv4-source-address">
          <description>
            <text>Exclude IPv4 source address</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-ipv4-destination-address">
          <description>
            <text>Exclude IPv4 destination address</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="vlan-id">
          <description>
            <text>Include incoming vlan-id in hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
      <container name="inet6">
        <description>
          <text>Configure inet6 fields</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="no-incoming-port">
          <description>
            <text>Exclude incoming port from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-incoming-device">
          <description>
            <text>Exclude incoming device from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-l4-source-port">
          <description>
            <text>Exclude l4 source port from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-l4-destination-port">
          <description>
            <text>Exclude l4 dest port from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-next-header">
          <description>
            <text>Exclude next header from the hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-ipv6-source-address">
          <description>
            <text>Exclude IPv6 source address</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="no-ipv6-destination-address">
          <description>
            <text>Exclude IPv6 destination address</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="vlan-id">
          <description>
            <text>Include vlan-id in hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
      <leaf name="no-mpls">
        <description>
          <text>Disable mpls in hash key</text>
        </description>
        <type name="empty"/>
      </leaf>
      <container name="gre">
        <description>
          <text>Configure for GRE </text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="key">
          <description>
            <text>Include key in hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="protocol">
          <description>
            <text>Include protocol in hash key</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
      <container name="services-loadbalancing">
        <description>
          <text>Select key to load balance across service PICs</text>
        </description>
        <uses name="apply-advanced"/>
        <container name="family">
          <description>
            <text>Protocol family</text>
          </description>
          <container name="inet">
            <description>
              <text>IPv4 protocol family</text>
            </description>
            <uses name="apply-advanced"/>
            <container name="layer-3-services">
              <description>
                <text>Include Layer 3 (IP) data in the hash key</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="source-address">
                <description>
                  <text>Include IP source address in the hash key</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="destination-address">
                <description>
                  <text>Include IP destination address in the hash key</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="incoming-interface-index">
                <description>
                  <text>Include incoming interface index in the hash key</text>
                </description>
                <type name="empty"/>
              </leaf>
            </container>
          </container>
          <container name="inet6">
            <description>
              <text>IPv6 protocol family</text>
            </description>
            <uses name="apply-advanced"/>
            <container name="layer-3-services">
              <description>
                <text>Include Layer 3 (IP) data in the hash key</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="source-address">
                <description>
                  <text>Include IP source address in the hash key</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="destination-address">
                <description>
                  <text>Include IP destination address in the hash key</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="incoming-interface-index">
                <description>
                  <text>Include incoming interface index in the hash key</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="src-prefix-len">
                <description>
                  <text>Enhanced hash key inet6 source prefix length</text>
                </description>
                <default value="127"/>
                <units name="bits"/>
                <type name="union">
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                  <type name="uint32">
                    <range value="56 .. 127"/>
                  </type>
                </type>
              </leaf>
            </container>
          </container>
        </container>
      </container>
      <container name="family">
        <description>
          <text>Protocol family</text>
        </description>
        <container name="inet">
          <description>
            <text>IPv4 protocol family</text>
          </description>
          <uses name="apply-advanced"/>
          <leaf name="incoming-interface-index">
            <description>
              <text>Include incoming interface index in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="no-destination-port">
            <description>
              <text>Omit IP destination port in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="no-source-port">
            <description>
              <text>Omit IP source port in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="type-of-service">
            <description>
              <text>Include TOS byte in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="gtp-tunnel-endpoint-identifier">
            <description>
              <text>Include TEID in the hash key for GTP-U packets</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="l2tp-tunnel-session-identifier">
            <description>
              <text>Include TID SID in the hash key for L2TP packets</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="session-id">
            <description>
              <text>Include session ID in the enhanced hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
        </container>
        <container name="inet6">
          <description>
            <text>IPv6 protocol family</text>
          </description>
          <uses name="apply-advanced"/>
          <leaf name="incoming-interface-index">
            <description>
              <text>Include incoming interface index in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="traffic-class">
            <description>
              <text>Include Traffic Class byte in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="no-destination-port">
            <description>
              <text>Omit IP destination port in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="no-source-port">
            <description>
              <text>Omit IP source port in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="gtp-tunnel-endpoint-identifier">
            <description>
              <text>Include TEID in the hash key for GTP-U packets</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="session-id">
            <description>
              <text>Include session ID in the enhanced hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="no-flow-label">
            <description>
              <text>Omit Flow label in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="type-of-service">
            <description>
              <text>Include TOS byte in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
        </container>
        <container name="mpls">
          <description>
            <text>MPLS protocol family</text>
          </description>
          <uses name="apply-advanced"/>
          <leaf name="label-1-exp">
            <description>
              <text>Include EXP of first MPLS label from the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="incoming-interface-index">
            <description>
              <text>Include incoming interface index in the hash key</text>
            </description>
            <type name="empty"/>
          </leaf>
          <choice name="pseudowire_option">
            <case name="case_1">
              <leaf name="no-ether-pseudowire">
                <description>
                  <text>Omit IP payload over ethernet PW from the hash-key</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
            <case name="case_2">
