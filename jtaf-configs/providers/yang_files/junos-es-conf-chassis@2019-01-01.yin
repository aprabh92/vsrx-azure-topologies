          </description>
          <type name="enumeration">
            <enum name="log-only">
              <description>
                <text>Log the bandwidth degradation only</text>
              </description>
            </enum>
            <enum name="offline">
              <description>
                <text>Offline the FPC, default action</text>
              </description>
            </enum>
            <enum name="restart">
              <description>
                <text>Restart the FPC</text>
              </description>
            </enum>
            <enum name="restart-then-offline">
              <description>
                <text>Restart the FPC and offline in subsequent action</text>
              </description>
            </enum>
          </type>
        </leaf>
