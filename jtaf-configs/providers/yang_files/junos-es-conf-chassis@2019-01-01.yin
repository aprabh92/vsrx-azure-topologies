<?xml version="1.0" encoding="UTF-8"?>
<module name="junos-es-conf-chassis"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:jc-chassis="http://yang.juniper.net/junos-es/conf/chassis"
        xmlns:jt="http://yang.juniper.net/junos/common/types"
        xmlns:jc="http://yang.juniper.net/junos-es/conf/root">
  <namespace uri="http://yang.juniper.net/junos-es/conf/chassis"/>
  <prefix value="jc-chassis"/>
  <import module="junos-common-types">
    <prefix value="jt"/>
    <revision-date date="2019-01-01"/>
  </import>
  <import module="junos-es-conf-root">
    <prefix value="jc"/>
    <revision-date date="2019-01-01"/>
  </import>
  <organization>
    <text>Juniper Networks, Inc.</text>
  </organization>
  <contact>
    <text>yang-support@juniper.net</text>
  </contact>
  <description>
    <text>Junos chassis configuration module</text>
  </description>
  <revision date="2019-01-01">
    <description>
      <text>Junos: 21.3R1.9</text>
    </description>
  </revision>
  <augment target-node="/jc:configuration">
    <uses name="chassis-group"/>
  </augment>
  <augment target-node="/jc:configuration/jc:groups">
    <uses name="chassis-group"/>
  </augment>
  <grouping name="chassis-group">
    <container name="chassis">
      <description>
        <text>Chassis configuration</text>
      </description>
      <uses name="chassis_type"/>
    </container>
  </grouping>
  <grouping name="chassis_type">
    <uses name="apply-advanced"/>
    <container name="psu">
      <description>
        <text>Power Supply Unit redundancy configuration</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="redundancy">
        <description>
          <text>PSU Redundancy settings</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="n-plus-n">
          <description>
            <text>PSU redundancy: N+N configuration</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
    </container>
    <leaf name="craft-lockout">
      <description>
        <text>Disable craft interface input</text>
      </description>
      <type name="empty"/>
    </leaf>
    <container name="config-button">
      <description>
        <text>Config button behavior settings</text>
      </description>
      <leaf name="no-rescue">
        <description>
          <text>Don't reset to rescue configuration</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="no-clear">
        <description>
          <text>Don't reset to factory-default configuration</text>
        </description>
        <type name="empty"/>
      </leaf>
    </container>
    <choice name="source-route-choice">
      <leaf name="source-route">
        <description>
          <text>Enable IP source-route processing</text>
        </description>
        <status value="deprecated"/>
        <type name="empty"/>
      </leaf>
      <leaf name="no-source-route">
        <description>
          <text>Don't enable IP source-route processing</text>
        </description>
        <status value="deprecated"/>
        <type name="empty"/>
      </leaf>
    </choice>
    <choice name="packet-scheduling-choice">
      <leaf name="packet-scheduling">
        <description>
          <text>Enable DX2.0 packet scheduling</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="no-packet-scheduling">
        <description>
          <text>Don't enable DX2.0 packet scheduling</text>
        </description>
        <type name="empty"/>
      </leaf>
    </choice>
    <container name="icmp">
      <description>
        <text>ICMP protocol</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="per-iff-rate-limit">
        <description>
          <text>Configure icmp per iff rate limit value in the range 50-500</text>
        </description>
        <default value="50"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="50 .. 500"/>
          </type>
        </type>
      </leaf>
    </container>
    <container name="icmp6">
      <description>
        <text>ICMP version 6 protocol</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="per-iff-rate-limit">
        <description>
          <text>Configure icmp6 per iff rate limit value in the range 50-500</text>
        </description>
        <default value="50"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="50 .. 500"/>
          </type>
        </type>
      </leaf>
    </container>
    <leaf name="maximum-ecmp">
      <description>
        <text>Maximum ECMP limit for nexthops</text>
      </description>
      <type name="enumeration">
        <enum name="16">
          <description>
            <text>Maximum 16 ECMP routes</text>
          </description>
        </enum>
        <enum name="32">
          <description>
            <text>Maximum 32 ECMP routes</text>
          </description>
        </enum>
        <enum name="64">
          <description>
            <text>Maximum 64 ECMP routes</text>
          </description>
        </enum>
        <enum name="96">
          <description>
            <text>Maximum 96 ECMP routes</text>
          </description>
        </enum>
        <enum name="128">
          <description>
            <text>Maximum 128 ECMP routes</text>
          </description>
        </enum>
        <enum name="160">
          <description>
            <text>Maximum 160 ECMP routes</text>
          </description>
        </enum>
        <enum name="192">
          <description>
            <text>Maximum 192 ECMP routes</text>
          </description>
        </enum>
        <enum name="224">
          <description>
            <text>Maximum 224 ECMP routes</text>
          </description>
        </enum>
        <enum name="256">
          <description>
            <text>Maximum 256 ECMP routes</text>
          </description>
        </enum>
        <enum name="288">
          <description>
            <text>Maximum 288 ECMP routes</text>
          </description>
        </enum>
        <enum name="320">
          <description>
            <text>Maximum 320 ECMP routes</text>
          </description>
        </enum>
        <enum name="352">
          <description>
            <text>Maximum 352 ECMP routes</text>
          </description>
        </enum>
        <enum name="384">
          <description>
            <text>Maximum 384 ECMP routes</text>
          </description>
        </enum>
        <enum name="416">
          <description>
            <text>Maximum 416 ECMP routes</text>
          </description>
        </enum>
        <enum name="448">
          <description>
            <text>Maximum 448 ECMP routes</text>
          </description>
        </enum>
        <enum name="480">
          <description>
            <text>Maximum 480 ECMP routes</text>
          </description>
        </enum>
        <enum name="512">
          <description>
            <text>Maximum 512 ECMP routes</text>
          </description>
        </enum>
      </type>
    </leaf>
    <container name="redundancy">
      <description>
        <text>Redundancy settings</text>
      </description>
      <uses name="chassis_redundancy_type"/>
    </container>
    <container name="routing-engine">
      <description>
        <text>Routing Engine settings</text>
      </description>
      <uses name="chassis_routing_engine_type"/>
    </container>
    <container name="aggregated-devices">
      <description>
        <text>Aggregated devices configuration</text>
      </description>
      <uses name="chassis_agg_dev_type"/>
    </container>
    <list name="disk-partition">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Chassis disk monitor configuration</text>
      </description>
      <leaf name="name">
        <description>
          <text>Choose disk partition to monitor</text>
        </description>
        <type name="enumeration">
          <enum name="/var">
            <description>
              <text>Monitor /var partition</text>
            </description>
          </enum>
          <enum name="/config">
            <description>
              <text>Monitor /config partition</text>
            </description>
          </enum>
        </type>
      </leaf>
      <uses name="apply-advanced"/>
      <list name="level">
        <key value="name"/>
        <ordered-by value="user"/>
        <description>
          <text>Threshold level</text>
        </description>
        <leaf name="name">
          <type name="enumeration">
            <enum name="high">
              <description>
                <text>Set high level of threshold</text>
              </description>
            </enum>
            <enum name="full">
              <description>
                <text>Set full level of threshold</text>
              </description>
            </enum>
          </type>
        </leaf>
        <uses name="apply-advanced"/>
        <container name="free-space">
          <description>
            <text>Enter threshold value &amp; choose the metric</text>
          </description>
          <leaf name="thres-free">
            <description>
              <text>Enter free space threshold value</text>
            </description>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="0 .. 100"/>
              </type>
            </type>
          </leaf>
          <choice name="thres-metric">
            <case name="case_1">
              <leaf name="percent">
                <description>
                  <text>Free space threshold in %</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
            <case name="case_2">
              <leaf name="mb">
                <description>
                  <text>Free space threshold in MB</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
          </choice>
        </container>
      </list>
    </list>
    <container name="pseudowire-service">
      <description>
        <text>Pseudowire L3 termination device configuration</text>
      </description>
      <presence value="enable pseudowire-service"/>
      <uses name="chassis-pw-type"/>
    </container>
    <container name="fabric">
      <description>
        <text>Switch fabric settings</text>
      </description>
      <uses name="chassis_fabric_type"/>
    </container>
    <list name="fpc">
      <key value="name"/>
      <description>
        <text>Flexible PIC Concentrator parameters</text>
      </description>
      <uses name="chassis_fpc_type"/>
    </list>
    <leaf name="disable-power-management">
      <description>
        <text>Disable Power Management in this chassis</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="dedicated-ukern-cpu">
      <description>
        <text>Run Microkernel on a dedicated CPU core</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="realtime-ukern-thread">
      <description>
        <text>Run Microkernel on a realtime CPU thread</text>
      </description>
      <type name="empty"/>
    </leaf>
    <container name="fpc-feb-connectivity">
      <description>
        <text>Connectivity between Flexible PIC Concentrators and Forwarding Engine Boards</text>
      </description>
      <uses name="apply-advanced"/>
      <list name="fpc">
        <key value="name"/>
        <leaf name="name">
          <description>
            <text>FPC slot number</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32"/>
          </type>
        </leaf>
        <uses name="apply-advanced"/>
        <container name="feb">
          <description>
            <text>FEB slot number</text>
          </description>
          <choice name="feb-slot-choice">
            <case name="case_1">
              <leaf name="none">
                <description>
                  <text>FPC not connected to any FEB</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
            <case name="case_2">
              <leaf name="feb-slot">
                <description>
                  <text>FEB slot number</text>
                </description>
                <type name="union">
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                  <type name="uint32"/>
                </type>
              </leaf>
            </case>
          </choice>
        </container>
      </list>
    </container>
    <container name="ioc-npc-connectivity">
      <description>
        <text>Connectivity between IOC and NPC</text>
      </description>
      <uses name="apply-advanced"/>
      <list name="ioc">
        <key value="name"/>
        <leaf name="name">
          <description>
            <text>IOC-FPC slot number</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32"/>
          </type>
        </leaf>
        <uses name="apply-advanced"/>
        <container name="npc">
          <description>
            <text>NPC-FPC slot number</text>
          </description>
          <choice name="npc-slot-choice">
            <case name="case_1">
              <leaf name="none">
                <description>
                  <text>IOC not connected to any NPC</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
            <case name="case_2">
              <leaf name="npc-slot">
                <description>
                  <text>NPC-FPC slot number</text>
                </description>
                <type name="union">
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                  <type name="uint32"/>
                </type>
              </leaf>
            </case>
          </choice>
        </container>
      </list>
    </container>
    <container name="pem">
      <description>
        <text>Power supply (PEM) parameters</text>
      </description>
      <uses name="chassis_pem_type"/>
    </container>
    <container name="tfeb">
      <description>
        <text>Taz Forwarding Engine Board parameters</text>
      </description>
      <uses name="chassis_feb_type"/>
    </container>
    <container name="alarm">
      <description>
        <text>Global alarm settings</text>
      </description>
      <uses name="chassis_alarm_type"/>
    </container>
    <leaf name="slow-pfe-alarm">
      <description>
        <text>Enable slow (potential) PFE alarm</text>
      </description>
      <type name="empty"/>
    </leaf>
    <leaf name="onbox-av-load-flavor">
      <description>
        <text>Select onbox anti-virus traffic load flavor</text>
      </description>
      <type name="enumeration">
        <enum name="heavy">
          <description>
            <text>Allocates additional resources for improved performance</text>
          </description>
        </enum>
      </type>
    </leaf>
    <leaf name="ambient-temperature">
      <description>
        <text>Chassis ambient-temperature value in degree celsius</text>
      </description>
      <type name="enumeration">
        <enum name="55C">
          <description>
            <text>55 degree celsius</text>
          </description>
        </enum>
        <enum name="40C">
          <description>
            <text>40 degree celsius</text>
          </description>
        </enum>
        <enum name="25C">
          <description>
            <text>25 degree celsius</text>
          </description>
        </enum>
        <enum name="32C">
          <description>
            <text>32 degree celsius</text>
          </description>
        </enum>
      </type>
    </leaf>
    <container name="resource-watch">
      <description>
        <text>Chassis routing engine resource watch configuration</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="resource-watch-enable">
        <description>
          <text>Enable resource watch</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="cpu-statistics">
        <description>
          <text>Set statistics collection, 0 disable</text>
        </description>
        <units name="seconds"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="0 .. 604800"/>
          </type>
        </type>
      </leaf>
      <leaf name="junos-statistics">
        <description>
          <text>Set statistics collection, 0 disable</text>
        </description>
        <units name="seconds"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="0 .. 604800"/>
          </type>
        </type>
      </leaf>
      <leaf name="orange-zone-enable">
        <description>
          <text>Enable orange zone</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="orange-zone-threshold">
        <description>
          <text>Set orange zone value (CPU idle %), default 20</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 99"/>
          </type>
        </type>
      </leaf>
      <leaf name="red-zone-enable">
        <description>
          <text>Enable red zone</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="red-zone-cpu-threshold">
        <description>
          <text>Set red zone value (CPU idle %), default 10</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 99"/>
          </type>
        </type>
      </leaf>
      <leaf name="red-zone-jkl-threshold">
        <description>
          <text>Set red zone value (Junos load %), default 50</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 99"/>
          </type>
        </type>
      </leaf>
      <leaf name="red-zone-snmp-enable">
        <description>
          <text>Enable red zone snmp</text>
        </description>
        <type name="empty"/>
      </leaf>
    </container>
    <leaf name="network-services">
      <description>
        <text>Chassis network services configuration</text>
      </description>
      <type name="enumeration">
        <enum name="ip">
          <description>
            <text>IP network services</text>
          </description>
        </enum>
        <enum name="ethernet">
          <description>
            <text>Ethernet network services</text>
          </description>
        </enum>
        <enum name="enhanced-ip">
          <description>
            <text>Enhanced IP network services</text>
          </description>
        </enum>
        <enum name="enhanced-ethernet">
          <description>
            <text>Enhanced ethernet network services</text>
          </description>
        </enum>
        <enum name="enhanced-mode">
          <description>
            <text>Enhanced network services</text>
          </description>
        </enum>
        <enum name="lan">
          <description>
            <text>Ethernet LAN services</text>
          </description>
        </enum>
      </type>
    </leaf>
    <container name="usb">
      <description>
        <text>USB control flags</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="storage">
        <description>
          <text>USB storage flags</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="disable">
          <description>
            <text>Disable USB storage</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
    </container>
    <list name="lcc">
      <key value="name"/>
      <description>
        <text>Line-card chassis configuration</text>
      </description>
      <leaf name="name">
        <description>
          <text>LCC number</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32"/>
        </type>
      </leaf>
      <uses name="apply-advanced"/>
      <list name="fpc">
        <key value="name"/>
        <description>
          <text>Flexible PIC Concentrator parameters</text>
        </description>
        <uses name="chassis_fpc_type"/>
      </list>
      <container name="pem">
        <description>
          <text>Power supply (PEM) parameters</text>
        </description>
        <uses name="chassis_pem_type"/>
      </container>
      <choice name="state">
        <case name="case_1">
          <leaf name="online-expected">
            <description>
              <text>LCC is expected to be online</text>
            </description>
            <type name="empty"/>
          </leaf>
        </case>
        <case name="case_2">
          <leaf name="offline">
            <description>
              <text>LCC is expected to be offline</text>
            </description>
            <type name="empty"/>
          </leaf>
        </case>
      </choice>
    </list>
    <container name="lcd">
      <description>
        <text>Chassis LCD</text>
      </description>
      <status value="deprecated"/>
      <uses name="apply-advanced"/>
      <choice name="menu-item">
        <case name="case_1">
          <leaf name="maintenance-menu">
            <description>
              <text>LCD maintenance menu</text>
            </description>
            <type name="empty"/>
          </leaf>
        </case>
      </choice>
      <choice name="enable-disable">
        <case name="case_1">
          <leaf name="disable">
            <description>
              <text>Disable maintenance-menu</text>
            </description>
            <type name="empty"/>
          </leaf>
        </case>
      </choice>
    </container>
    <container name="forwarding-options">
      <description>
        <text>Configure options to control packet forwarding</text>
      </description>
      <uses name="apply-advanced"/>
      <choice name="uft_object">
        <case name="case_1">
          <container name="l2-profile-one">
            <presence value="enable l2-profile-one"/>
            <description>
              <text>MAC: 288K L3-host: 16K LPM: 16K EM:0, needs reboot for VC; restarts PFE if standalone</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="num-65-127-prefix">
              <description>
                <text>IPV6 prefixes &gt; 64, in multiples of 1K e.g 3 = 3x1K entries(default 1)</text>
              </description>
              <default value="1"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="int32">
                  <range value="0 .. 4"/>
                </type>
              </type>
            </leaf>
          </container>
        </case>
        <case name="case_2">
          <container name="l2-profile-two">
            <presence value="enable l2-profile-two"/>
            <description>
              <text>MAC: 224K L3-host: 80K LPM: 16K EM:0, needs reboot for VC; restarts PFE if standalone</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="num-65-127-prefix">
              <description>
                <text>IPV6 prefixes &gt; 64, in multiples of 1K e.g 3 = 3x1K entries(default 1)</text>
              </description>
              <default value="1"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="int32">
                  <range value="0 .. 4"/>
                </type>
              </type>
            </leaf>
          </container>
        </case>
        <case name="case_3">
          <container name="l2-profile-three">
            <presence value="enable l2-profile-three"/>
            <description>
              <text>(default) MAC: 160K L3-host: 144K LPM: 16K, needs reboot for VC;restarts PFE if standalone</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="num-65-127-prefix">
              <description>
                <text>IPV6 prefixes &gt; 64, in multiples of 1K e.g 3 = 3x1K entries(default 1)</text>
              </description>
              <default value="1"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="int32">
                  <range value="0 .. 4"/>
                </type>
              </type>
            </leaf>
          </container>
        </case>
        <case name="case_4">
          <container name="l3-profile">
            <presence value="enable l3-profile"/>
            <description>
              <text>MAC: 96K L3-host: 208K LPM: 16K EM:0, needs reboot for VC; restarts PFE if standalone</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="num-65-127-prefix">
              <description>
                <text>IPV6 prefixes &gt; 64, in multiples of 1K e.g 3 = 3x1K entries(default 1)</text>
              </description>
              <default value="1"/>
