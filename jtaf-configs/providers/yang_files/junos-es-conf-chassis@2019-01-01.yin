                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="int32">
                  <range value="0 .. 4"/>
                </type>
              </type>
            </leaf>
          </container>
        </case>
        <case name="case_5">
          <container name="lpm-profile">
            <presence value="enable lpm-profile"/>
            <description>
              <text>MAC: 32K L3-host: 16K LPM: 128K EM:0, needs reboot for VC; restarts PFE if standalone</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="prefix-65-127-disable">
              <description>
                <text>Disable IPV6 prefixes with length &gt; 64</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="unicast-in-lpm">
              <description>
                <text>Install unicast host entries in LPM table</text>
              </description>
              <type name="empty"/>
            </leaf>
          </container>
        </case>
        <case name="case_6">
          <container name="exact-match-profile">
            <presence value="enable exact-match-profile"/>
            <description>
              <text>MAC: 8K L3-host: 8K LPM: 16K EM: 64K, needs reboot for VC; restarts PFE if standalone</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="num-65-127-prefix">
              <description>
                <text>IPV6 prefixes &gt; 64, in multiples of 1K e.g 3 = 3x1K entries(default 1)</text>
              </description>
              <default value="1"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="int32">
                  <range value="0 .. 4"/>
                </type>
              </type>
            </leaf>
          </container>
        </case>
        <case name="case_7">
          <container name="vxlan-gbp-profile">
            <presence value="enable vxlan-gbp-profile"/>
            <description>
              <text>MAC: 48K L3-host: 48K LPM: 16K EM:32K, needs reboot for VC; restarts PFE if standalone</text>
            </description>
          </container>
        </case>
        <case name="case_8">
          <container name="custom-profile">
            <description>
              <text>(Partition the shared banks, will automatically restart the PFE when configured)</text>
            </description>
            <uses name="apply-advanced"/>
            <container name="l2-entries">
              <description>
                <text>Customize l2 entries needed</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="num-banks">
                <description>
                  <text>Number of banks needed for entries of this type</text>
                </description>
                <default value="0"/>
                <type name="union">
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                  <type name="int32">
                    <range value="0 .. 4"/>
                  </type>
                </type>
              </leaf>
            </container>
            <container name="l3-entries">
              <description>
                <text>Customize l3 entries needed</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="num-banks">
                <description>
                  <text>Number of banks needed for entries of this type</text>
                </description>
                <default value="0"/>
                <type name="union">
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                  <type name="int32">
                    <range value="0 .. 4"/>
                  </type>
                </type>
              </leaf>
            </container>
            <container name="exact-match-entries">
              <description>
                <text>Customize exact match entries needed</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="num-banks">
                <description>
                  <text>Number of banks needed for entries of this type</text>
                </description>
                <default value="0"/>
                <type name="union">
                  <type name="string">
                    <pattern value="&lt;.*&gt;|$.*"/>
                  </type>
                  <type name="int32">
                    <range value="0 .. 4"/>
                  </type>
                </type>
              </leaf>
            </container>
            <container name="lpm-entries">
              <description>
                <text>Customize lpm banks needed</text>
              </description>
              <uses name="apply-advanced"/>
              <leaf name="num-banks">
                <description>
                  <text>Number of banks needed for entries of this type</text>
                </description>
                <default value="0"/>
                <type name="enumeration">
                  <enum name="0"/>
                  <enum name="2"/>
                  <enum name="4"/>
                </type>
              </leaf>
              <leaf name="prefix-65-127-disable">
                <description>
                  <text>Disable IPV6 prefixes with length &gt; 64</text>
                </description>
                <type name="empty"/>
              </leaf>
            </container>
            <leaf name="num-65-127-prefix">
              <description>
                <text>IPV6 prefixes &gt; 64, in multiples of 1K e.g 3 = 3x1K entries(default 1)</text>
              </description>
              <default value="1"/>
