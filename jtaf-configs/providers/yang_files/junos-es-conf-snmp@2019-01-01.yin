<?xml version="1.0" encoding="UTF-8"?>
<module name="junos-es-conf-snmp"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:jc-snmp="http://yang.juniper.net/junos-es/conf/snmp"
        xmlns:jt="http://yang.juniper.net/junos/common/types"
        xmlns:jc="http://yang.juniper.net/junos-es/conf/root">
  <namespace uri="http://yang.juniper.net/junos-es/conf/snmp"/>
  <prefix value="jc-snmp"/>
  <import module="junos-common-types">
    <prefix value="jt"/>
    <revision-date date="2019-01-01"/>
  </import>
  <import module="junos-es-conf-root">
    <prefix value="jc"/>
    <revision-date date="2019-01-01"/>
  </import>
  <organization>
    <text>Juniper Networks, Inc.</text>
  </organization>
  <contact>
    <text>yang-support@juniper.net</text>
  </contact>
  <description>
    <text>Junos snmp configuration module</text>
  </description>
  <revision date="2019-01-01">
    <description>
      <text>Junos: 21.3R1.9</text>
    </description>
  </revision>
  <augment target-node="/jc:configuration">
    <uses name="snmp-group"/>
  </augment>
  <augment target-node="/jc:configuration/jc:groups">
    <uses name="snmp-group"/>
  </augment>
  <grouping name="snmp-group">
    <container name="snmp">
      <description>
        <text>Simple Network Management Protocol configuration</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="system-name">
        <description>
          <text>System name override</text>
        </description>
        <type name="string"/>
      </leaf>
      <leaf name="description">
        <description>
          <text>System description</text>
        </description>
        <type name="string"/>
      </leaf>
      <leaf name="location">
        <description>
          <text>Physical location of system</text>
        </description>
        <type name="string"/>
      </leaf>
      <leaf name="contact">
        <description>
          <text>Contact information for administrator</text>
        </description>
        <type name="string"/>
      </leaf>
      <leaf-list name="interface">
        <ordered-by value="user"/>
        <description>
          <text>Restrict SNMP requests to interfaces</text>
        </description>
        <type name="union">
          <type name="jt:interface-name"/>
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
        </type>
      </leaf-list>
      <container name="filter-interfaces">
        <presence value="enable filter-interfaces"/>
        <description>
          <text>List of interfaces that needs to be filtered</text>
        </description>
        <uses name="apply-advanced"/>
        <list name="interfaces">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Filter specified interfaces</text>
          </description>
          <leaf name="name">
            <type name="string"/>
          </leaf>
          <uses name="apply-advanced"/>
        </list>
        <leaf name="all-internal-interfaces">
          <description>
            <text>Filter all internal interfaces</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
      <leaf name="if-count-with-filter-interfaces">
        <description>
          <text>Filter interfaces config for ifNumber and ipv6Interfaces</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="filter-duplicates">
        <description>
          <text>Filter requests with duplicate source address/port and request ID</text>
        </description>
        <type name="empty"/>
      </leaf>
      <container name="nonvolatile">
        <description>
          <text>Configure the handling of nonvolatile SNMP Set requests</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="commit-delay">
          <description>
            <text>Delay between affirmative SNMP Set reply and start of commit</text>
          </description>
          <default value="5"/>
          <units name="seconds"/>
          <type name="union">
            <type name="uint32"/>
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
          </type>
        </leaf>
      </container>
      <container name="v3">
        <description>
          <text>SNMPv3 configuration information</text>
        </description>
        <uses name="apply-advanced"/>
        <container name="usm">
          <description>
            <text>User-based security model (USM) information</text>
          </description>
          <uses name="apply-advanced"/>
          <container name="local-engine">
            <description>
              <text>Local engine user configuration</text>
            </description>
            <uses name="apply-advanced"/>
            <list name="user">
              <key value="name"/>
              <ordered-by value="user"/>
              <description>
                <text>SNMPv3 USM user information</text>
              </description>
              <uses name="v3-user-config"/>
            </list>
          </container>
          <list name="remote-engine">
            <key value="name"/>
            <ordered-by value="user"/>
            <description>
              <text>Remote engine user configuration</text>
            </description>
            <leaf name="name">
              <description>
                <text>Remote engine id (Hex format)</text>
              </description>
              <type name="string"/>
            </leaf>
            <uses name="apply-advanced"/>
            <list name="user">
              <key value="name"/>
              <ordered-by value="user"/>
              <description>
                <text>SNMPv3 USM user information</text>
              </description>
              <uses name="v3-user-config"/>
            </list>
          </list>
        </container>
        <container name="vacm">
          <description>
            <text>View-based access control model (VACM) information</text>
          </description>
          <uses name="apply-advanced"/>
          <container name="security-to-group">
            <description>
              <text>Assigns security names to group</text>
            </description>
            <uses name="apply-advanced"/>
            <list name="security-model">
              <key value="name"/>
              <ordered-by value="user"/>
              <description>
                <text>Security model context for group assignment</text>
              </description>
              <leaf name="name">
                <description>
                  <text>SNMPv3 security-to-group model name</text>
                </description>
                <type name="enumeration">
                  <enum name="usm">
                    <description>
                      <text>User-based security model</text>
                    </description>
                  </enum>
                  <enum name="v1">
                    <description>
                      <text>SNMPv1 model</text>
                    </description>
                  </enum>
                  <enum name="v2c">
                    <description>
                      <text>SNMPv2c model</text>
                    </description>
                  </enum>
                </type>
              </leaf>
              <uses name="apply-advanced"/>
              <list name="security-name">
                <key value="name"/>
                <ordered-by value="user"/>
                <description>
                  <text>Security name to assign to group</text>
                </description>
                <leaf name="name">
                  <description>
                    <text>Security name</text>
                  </description>
                  <type name="string">
                    <length value="1 .. 32"/>
                  </type>
                </leaf>
                <uses name="apply-advanced"/>
                <leaf name="group">
                  <description>
                    <text>Group to which to assign security name</text>
                  </description>
                  <type name="string">
                    <length value="1 .. 32"/>
                  </type>
                </leaf>
              </list>
            </list>
          </container>
          <container name="access">
            <description>
              <text>Specify SNMP access limits</text>
            </description>
            <uses name="apply-advanced"/>
            <list name="group">
              <key value="name"/>
              <ordered-by value="user"/>
              <description>
                <text>Group access configuration</text>
              </description>
              <leaf name="name">
                <description>
                  <text>SNMPv3 VACM group name</text>
                </description>
                <type name="string">
                  <length value="1 .. 32"/>
                </type>
              </leaf>
              <uses name="apply-advanced"/>
              <container name="default-context-prefix">
                <description>
                  <text>Default context-prefix access configuration</text>
                </description>
                <uses name="apply-advanced"/>
                <list name="security-model">
                  <key value="name"/>
                  <ordered-by value="user"/>
                  <description>
                    <text>Security model access configuration</text>
                  </description>
                  <uses name="security-model-access"/>
                </list>
              </container>
              <list name="context-prefix">
                <key value="name"/>
                <ordered-by value="user"/>
                <description>
                  <text>Context-prefix access configuration</text>
                </description>
                <leaf name="name">
                  <description>
                    <text>SNMPv3 VACM context prefix</text>
                  </description>
                  <type name="string"/>
                </leaf>
                <uses name="apply-advanced"/>
                <list name="security-model">
                  <key value="name"/>
                  <ordered-by value="user"/>
                  <description>
                    <text>Security model access configuration</text>
                  </description>
                  <uses name="security-model-access"/>
                </list>
              </list>
            </list>
          </container>
        </container>
        <list name="target-address">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Identifies notification targets as well as allowed management stations</text>
          </description>
          <leaf name="name">
            <description>
              <text>SNMP target address name</text>
            </description>
            <type name="string">
              <length value="1 .. 32"/>
            </type>
          </leaf>
          <uses name="apply-advanced"/>
          <leaf name="address">
            <description>
              <text>SNMP target address</text>
            </description>
            <type name="jt:ipaddr"/>
          </leaf>
          <leaf name="port">
            <description>
              <text>SNMP target port number</text>
            </description>
            <default value="162"/>
            <type name="union">
              <type name="uint16"/>
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
            </type>
          </leaf>
          <leaf name="timeout">
            <description>
              <text>Acknowledgment timeout for confirmed SNMP notifications</text>
            </description>
            <default value="15"/>
            <units name="seconds"/>
            <type name="union">
              <type name="uint32"/>
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
            </type>
          </leaf>
          <leaf name="retry-count">
            <description>
              <text>Maximum retry count for confirmed SNMP notifications</text>
            </description>
            <default value="3"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="0 .. 255"/>
              </type>
            </type>
          </leaf>
          <leaf name="tag-list">
            <description>
              <text>SNMP tag list used to select target addresses</text>
            </description>
            <type name="string">
              <length value="0 .. 255"/>
            </type>
          </leaf>
          <leaf name="address-mask">
            <description>
              <text>Mask range of addresses for community string access control.</text>
            </description>
            <type name="jt:ipaddr"/>
          </leaf>
          <leaf name="routing-instance">
            <description>
              <text>Routing instance for trap destination</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="logical-system">
            <description>
              <text>Logical-system name for trap destination</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="target-parameters">
            <description>
              <text>SNMPv3 target parameter name in the target parameters table</text>
            </description>
            <type name="string">
              <length value="1 .. 32"/>
            </type>
          </leaf>
        </list>
        <list name="target-parameters">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Parameters and filter name used when sending notifications</text>
          </description>
          <leaf name="name">
            <description>
              <text>SNMPv3 target parameters name</text>
            </description>
            <type name="string">
              <length value="1 .. 32"/>
            </type>
          </leaf>
          <uses name="apply-advanced"/>
          <container name="parameters">
            <description>
              <text>Parameters used when sending notifications</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="message-processing-model">
              <description>
                <text>The message processing model to be used when generating SNMP notifications</text>
              </description>
              <type name="enumeration">
                <enum name="v1">
                  <description>
                    <text>SNMPv1 model</text>
                  </description>
                </enum>
                <enum name="v2c">
                  <description>
                    <text>SNMPv2c model</text>
                  </description>
                </enum>
                <enum name="v3">
                  <description>
                    <text>SNMPv3 model</text>
                  </description>
                </enum>
              </type>
            </leaf>
            <leaf name="security-model">
              <description>
                <text>Security-model used when generating SNMP notifications</text>
              </description>
              <type name="enumeration">
                <enum name="usm">
                  <description>
                    <text>User-based security model</text>
                  </description>
                </enum>
                <enum name="v1">
                  <description>
                    <text>SNMPv1 model</text>
                  </description>
                </enum>
                <enum name="v2c">
                  <description>
                    <text>SNMPv2c model</text>
                  </description>
                </enum>
              </type>
            </leaf>
            <leaf name="security-level">
              <description>
                <text>Security-level used when generating SNMP notifications</text>
              </description>
              <type name="enumeration">
                <enum name="none">
                  <description>
                    <text>None (noAuthNoPriv)</text>
                  </description>
                </enum>
                <enum name="authentication">
                  <description>
                    <text>Authentication (authNoPriv)</text>
                  </description>
                </enum>
                <enum name="privacy">
                  <description>
                    <text>Privacy and authentication (authPriv)</text>
                  </description>
                </enum>
              </type>
            </leaf>
            <leaf name="security-name">
              <description>
                <text>Security name used when generating SNMP notifications</text>
              </description>
              <type name="string">
                <length value="1 .. 32"/>
              </type>
            </leaf>
          </container>
          <container name="notify-filter">
            <description>
              <text>Notify filter to apply to notifications</text>
            </description>
            <leaf name="filter">
              <description>
                <text>Filter name to apply to notifications</text>
              </description>
              <type name="string">
                <length value="1 .. 32"/>
              </type>
            </leaf>
          </container>
        </list>
        <list name="notify">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Used to select management targets for notifications as well as the type of notifications</text>
          </description>
          <leaf name="name">
            <description>
              <text>Notify name</text>
            </description>
            <type name="string">
              <length value="1 .. 32"/>
            </type>
          </leaf>
          <uses name="apply-advanced"/>
          <leaf name="type">
            <description>
              <text>Notification type</text>
            </description>
            <default value="trap"/>
            <type name="enumeration">
              <enum name="trap">
                <description>
                  <text>SNMP trap</text>
                </description>
              </enum>
              <enum name="inform">
                <description>
                  <text>Inform request</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="tag">
            <description>
              <text>Notifications will be sent to all targets configured with this tag</text>
            </description>
            <type name="string">
              <length value="0 .. 255"/>
            </type>
          </leaf>
        </list>
        <list name="notify-filter">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Filters to apply to SNMP notifications</text>
          </description>
          <leaf name="name">
            <description>
              <text>SNMP profile name in the notify filter table</text>
            </description>
            <type name="string">
              <length value="1 .. 32"/>
            </type>
          </leaf>
          <uses name="apply-advanced"/>
          <list name="oid">
            <key value="name"/>
            <ordered-by value="user"/>
            <description>
              <text>OID include/exclude list</text>
            </description>
            <leaf name="name">
              <description>
                <text>OID to include/exclude from notify filter</text>
              </description>
              <type name="string"/>
            </leaf>
            <choice name="filter-action">
              <case name="case_1">
                <leaf name="include">
                  <description>
                    <text>Include this OID in the notify filter</text>
                  </description>
                  <type name="empty"/>
                </leaf>
              </case>
              <case name="case_2">
                <leaf name="exclude">
                  <description>
                    <text>Exclude this OID from the notify filter</text>
                  </description>
                  <type name="empty"/>
                </leaf>
              </case>
            </choice>
          </list>
        </list>
        <leaf name="management-routing-instance">
          <description>
            <text>Enable SNMPv3 access for all routing instances from this default context</text>
          </description>
          <type name="string"/>
        </leaf>
        <list name="snmp-community">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>SNMP community and view-based access control model configuration</text>
          </description>
          <leaf name="name">
            <description>
              <text>Unique index value in this community table entry</text>
            </description>
            <type name="string">
              <length value="1 .. 32"/>
            </type>
          </leaf>
          <uses name="apply-advanced"/>
          <leaf name="community-name">
            <description>
              <text>SNMPv1/v2c community name (default is same as community-index)</text>
            </description>
            <type name="jt:unreadable"/>
          </leaf>
          <leaf name="security-name">
            <description>
              <text>Security name used when performing access control</text>
            </description>
            <type name="string">
              <length value="1 .. 32"/>
            </type>
          </leaf>
          <leaf name="context">
            <description>
              <text>Context used when performing access control</text>
            </description>
            <type name="string">
              <length value="0 .. 32"/>
            </type>
          </leaf>
          <leaf name="tag">
            <description>
              <text>Tag identifier for set of targets allowed to use this community string</text>
            </description>
            <type name="string">
              <length value="0 .. 255"/>
            </type>
          </leaf>
        </list>
      </container>
      <list name="proxy">
        <key value="name"/>
        <ordered-by value="user"/>
        <description>
          <text>SNMP proxy configuration</text>
        </description>
        <leaf name="name">
          <description>
            <text>Unique proxy name</text>
          </description>
          <type name="string"/>
        </leaf>
        <uses name="apply-advanced"/>
        <leaf name="device-name">
          <description>
            <text>Satellite/Proxied Device name or IP address</text>
          </description>
          <type name="string"/>
        </leaf>
        <choice name="version">
          <case name="case_1">
            <container name="version-v1">
              <description>
                <text>For v1 proxy configuration define snmp-community</text>
              </description>
              <uses name="comm-object"/>
            </container>
          </case>
          <case name="case_2">
            <container name="version-v2c">
              <description>
                <text>For v2c proxy configuration define snmp-community</text>
              </description>
              <uses name="comm-object"/>
            </container>
          </case>
          <case name="case_3">
            <container name="version-v3">
              <description>
                <text>For v3 proxy configuration define security-name</text>
              </description>
              <uses name="sec-object"/>
            </container>
          </case>
        </choice>
        <list name="routing-instance">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Associate routing-instance name for proxy forwarding</text>
          </description>
          <leaf name="name">
            <description>
              <text>Routing-instance name for proxy forwarding</text>
            </description>
            <type name="string"/>
          </leaf>
          <uses name="apply-advanced"/>
        </list>
        <list name="logical-system">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Associate logical-system name for proxy forwarding</text>
          </description>
          <leaf name="name">
            <description>
              <text>Associate logical-system name for proxy forwarding</text>
            </description>
            <type name="string"/>
          </leaf>
          <uses name="apply-advanced"/>
          <list name="routing-instance">
            <key value="name"/>
            <ordered-by value="user"/>
            <description>
              <text>Associate routing-instance name for proxy forwarding</text>
            </description>
            <leaf name="name">
              <description>
                <text>Routing-instance name for proxy forwarding</text>
              </description>
              <type name="string"/>
            </leaf>
            <uses name="apply-advanced"/>
          </list>
        </list>
      </list>
      <container name="subagent">
        <description>
          <text>SNMP subagent configuration</text>
        </description>
        <uses name="apply-advanced"/>
        <container name="tcp">
          <presence value="enable tcp"/>
          <description>
            <text>Allow SNMP subagent tcp connection</text>
          </description>
          <uses name="apply-advanced"/>
          <container name="routing-instance">
            <description>
              <text>Specify routing-instance name for tcp connection</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="default">
              <description>
                <text>Allow connections over default routing-instance</text>
              </description>
              <type name="empty"/>
            </leaf>
          </container>
        </container>
      </container>
      <container name="engine-id">
        <description>
          <text>SNMPv3 engine ID</text>
        </description>
        <uses name="apply-advanced"/>
        <choice name="engine-id-choice">
          <case name="case_1">
            <leaf name="use-mac-address">
              <description>
                <text>Uses management interface MAC Address for the engine ID</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
          <case name="case_2">
            <leaf name="use-default-ip-address">
              <description>
                <text>Use default IP address for the engine ID</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
          <case name="case_3">
            <leaf name="local">
              <description>
                <text>Local engine ID</text>
              </description>
              <type name="string"/>
            </leaf>
          </case>
        </choice>
      </container>
      <container name="access">
        <description>
          <text>SNMPv3 access information</text>
        </description>
        <status value="deprecated"/>
        <uses name="apply-advanced"/>
        <list name="user">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>SNMPv3 USM user information</text>
          </description>
          <leaf name="name">
            <description>
              <text>SNMPv3 USM username</text>
            </description>
            <type name="string"/>
          </leaf>
          <uses name="apply-advanced"/>
          <leaf name="authentication-type">
            <description>
              <text>SNMPv3 USM authentication type</text>
            </description>
            <type name="enumeration">
              <enum name="none">
                <description>
                  <text>No authentication</text>
                </description>
              </enum>
              <enum name="md5">
                <description>
                  <text>MD5 hash algorithm</text>
                </description>
              </enum>
              <enum name="sha">
                <description>
                  <text>SHA hash algorithm</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="authentication-password">
            <description>
              <text>SNMPv3 USM authentication password</text>
            </description>
            <type name="jt:unreadable"/>
          </leaf>
          <leaf name="privacy-type">
            <description>
              <text>SNMPv3 USM privacy type</text>
            </description>
            <type name="enumeration">
              <enum name="none">
                <description>
                  <text>No privacy</text>
                </description>
              </enum>
              <enum name="des">
                <description>
                  <text>DES algorithm</text>
                </description>
              </enum>
            </type>
          </leaf>
          <leaf name="privacy-password">
            <description>
              <text>SNMPv3 USM privacy password</text>
            </description>
            <type name="jt:unreadable"/>
          </leaf>
          <list name="clients">
            <key value="name"/>
            <ordered-by value="user"/>
            <description>
              <text>List of source address prefix ranges to accept</text>
            </description>
            <leaf name="name">
              <description>
                <text>Address or prefix</text>
              </description>
              <type name="jt:ipprefix"/>
            </leaf>
            <leaf name="restrict">
              <description>
                <text>Deny access</text>
              </description>
              <type name="empty"/>
            </leaf>
          </list>
        </list>
        <list name="group">
