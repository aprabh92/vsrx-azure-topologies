    <leaf name="name">
      <description>
        <text>SNMPv3 VACM security model</text>
      </description>
      <type name="enumeration">
        <enum name="any">
          <description>
            <text>Any security model</text>
          </description>
        </enum>
        <enum name="usm">
          <description>
            <text>User-based security model</text>
          </description>
        </enum>
        <enum name="v1">
          <description>
            <text>SNMPv1 model</text>
          </description>
        </enum>
        <enum name="v2c">
          <description>
            <text>SNMPv2c model</text>
          </description>
        </enum>
      </type>
    </leaf>
    <uses name="apply-advanced"/>
    <list name="security-level">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Security level access configuration</text>
      </description>
      <leaf name="name">
        <description>
          <text>SNMPv3 VACM security level</text>
        </description>
        <type name="enumeration">
          <enum name="none">
            <description>
              <text>None (noAuthNoPriv)</text>
            </description>
          </enum>
          <enum name="authentication">
            <description>
              <text>Authentication (authNoPriv)</text>
            </description>
          </enum>
          <enum name="privacy">
            <description>
              <text>Privacy and authentication (authPriv)</text>
            </description>
          </enum>
        </type>
      </leaf>
      <uses name="apply-advanced"/>
      <leaf name="context-match">
        <description>
          <text>Type of match to perform on context-prefix</text>
        </description>
        <default value="exact"/>
        <type name="enumeration">
          <enum name="exact">
            <description>
              <text>Exact match of prefix and context name</text>
            </description>
          </enum>
          <enum name="prefix">
            <description>
              <text>Only match the context prefix</text>
            </description>
          </enum>
        </type>
      </leaf>
      <leaf name="read-view">
        <description>
          <text>View used for read access</text>
        </description>
        <type name="string"/>
      </leaf>
      <leaf name="write-view">
        <description>
          <text>View used for write access</text>
        </description>
        <type name="string"/>
      </leaf>
      <leaf name="notify-view">
        <description>
          <text>View used to notifications</text>
        </description>
        <type name="string"/>
      </leaf>
    </list>
  </grouping>
  <grouping name="v3-user-config">
    <description>
      <text>SNMPv3 user configuration</text>
    </description>
    <leaf name="name">
      <description>
        <text>User name</text>
      </description>
      <type name="string">
        <length value="1 .. 32"/>
      </type>
    </leaf>
    <uses name="apply-advanced"/>
    <choice name="authentication">
      <case name="case_1">
        <container name="authentication-md5">
          <description>
            <text>Configure MD5 authentication</text>
          </description>
          <uses name="auth-object"/>
        </container>
      </case>
      <case name="case_2">
        <container name="authentication-sha">
          <description>
            <text>Configure SHA authentication</text>
          </description>
          <uses name="auth-object"/>
        </container>
      </case>
      <case name="case_3">
        <container name="authentication-sha224">
          <description>
            <text>Configure SHA224 authentication</text>
          </description>
          <uses name="auth-object"/>
        </container>
      </case>
      <case name="case_4">
        <container name="authentication-sha256">
          <description>
            <text>Configure SHA256 authentication</text>
          </description>
          <uses name="auth-object"/>
        </container>
      </case>
      <case name="case_5">
        <container name="authentication-sha384">
          <description>
            <text>Configure SHA384 authentication</text>
          </description>
          <uses name="auth-object"/>
        </container>
      </case>
      <case name="case_6">
        <container name="authentication-sha512">
          <description>
            <text>Configure SHA512 authentication</text>
          </description>
          <uses name="auth-object"/>
        </container>
      </case>
      <case name="case_7">
        <leaf name="authentication-none">
          <description>
            <text>Set no authentication for the user</text>
          </description>
          <type name="empty"/>
        </leaf>
      </case>
    </choice>
    <choice name="privacy">
      <case name="case_1">
        <container name="privacy-des">
          <description>
            <text>Configure DES privacy</text>
          </description>
          <uses name="priv-object"/>
        </container>
      </case>
      <case name="case_2">
        <container name="privacy-3des">
          <description>
            <text>Configure Triple DES privacy</text>
          </description>
          <uses name="priv-object"/>
        </container>
      </case>
      <case name="case_3">
        <container name="privacy-aes128">
          <description>
            <text>Configure AES128 privacy</text>
          </description>
          <uses name="priv-object"/>
        </container>
      </case>
      <case name="case_4">
        <leaf name="privacy-none">
          <description>
            <text>Set no privacy for the user</text>
          </description>
          <type name="empty"/>
        </leaf>
      </case>
    </choice>
