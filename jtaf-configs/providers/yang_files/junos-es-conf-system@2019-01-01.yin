  <grouping name="jdhcp-local-server-type">
    <uses name="apply-advanced"/>
    <container name="dhcpv6">
      <description>
        <text>DHCPv6 configuration</text>
      </description>
      <uses name="dhcpv6-local-server-type"/>
    </container>
    <list name="pool-match-order">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Define order of attribute matching for pool selection</text>
      </description>
      <leaf name="name">
        <description>
          <text>Match type</text>
        </description>
        <type name="enumeration">
          <enum name="external-authority">
            <description>
              <text>External authority handles address selection</text>
            </description>
          </enum>
          <enum name="ip-address-first">
            <description>
              <text>IP address used first to select a pool</text>
            </description>
          </enum>
          <enum name="option-82">
            <description>
              <text>Option 82 used in matching of pool</text>
            </description>
          </enum>
          <enum name="option-82-strict">
            <description>
              <text>Option 82 used in strict matching of pool</text>
            </description>
          </enum>
        </type>
      </leaf>
      <uses name="apply-advanced"/>
    </list>
    <container name="duplicate-clients-in-subnet">
      <description>
        <text>Allow duplicate clients in a subnet</text>
      </description>
      <uses name="jdhcp-duplicate-clients-in-subnet-type"/>
    </container>
    <container name="forward-snooped-clients">
      <description>
        <text>Forward snooped (unicast) packets</text>
      </description>
      <choice name="interface-type">
        <case name="case_1">
          <leaf name="configured-interfaces">
            <description>
              <text>Forward snooped (unicast) packets on configured interfaces</text>
            </description>
            <type name="empty"/>
          </leaf>
        </case>
        <case name="case_2">
          <leaf name="non-configured-interfaces">
            <description>
              <text>Forward snooped (unicast) packets on non-configured interfaces</text>
            </description>
            <type name="empty"/>
          </leaf>
        </case>
        <case name="case_3">
          <leaf name="all-interfaces">
            <description>
              <text>Forward snooped (unicast) packets on configured and non-configured interfaces</text>
            </description>
            <type name="empty"/>
          </leaf>
        </case>
      </choice>
    </container>
    <container name="overrides">
      <description>
        <text>DHCP override processing</text>
      </description>
      <presence value="enable overrides"/>
      <uses name="override-local-server-type"/>
    </container>
    <leaf name="access-profile">
      <description>
        <text>Access profile to use for AAA services</text>
      </description>
      <type name="string">
        <length value="1 .. 128"/>
      </type>
    </leaf>
    <container name="short-cycle-protection">
      <description>
        <text>Short cycle lockout configuration</text>
      </description>
      <leaf name="lockout-min-time">
        <description>
          <text>Short cycle lockout time in seconds</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 86400"/>
          </type>
        </type>
      </leaf>
      <leaf name="lockout-max-time">
        <description>
          <text>Short cycle lockout time in seconds</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 86400"/>
          </type>
        </type>
      </leaf>
    </container>
    <container name="route-suppression">
      <description>
        <text>Suppress access-internal and/or destination route addition</text>
      </description>
      <uses name="dhcp-route-suppression-type"/>
    </container>
    <list name="group">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Define a DHCP local server group</text>
      </description>
      <uses name="dhcp-local-server-group"/>
    </list>
    <list name="dual-stack-group">
      <key value="name"/>
      <description>
        <text>Define a DHCP dual stack group</text>
      </description>
      <uses name="dhcp-local-server-dual-stack-group"/>
    </list>
    <container name="lease-time-validation">
      <presence value="enable lease-time-validation"/>
      <description>
        <text>Configure lease time violation validation</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="lease-time-threshold">
        <description>
          <text>Threshold for lease time violation seconds</text>
        </description>
        <units name="seconds"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="60 .. 2147483647"/>
          </type>
        </type>
      </leaf>
      <container name="violation-action">
        <description>
          <text>Lease time validation violation action</text>
        </description>
        <choice name="violation-action-choice">
          <case name="case_1">
            <leaf name="strict">
              <description>
                <text>Reject discover and renew</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
          <case name="case_2">
            <leaf name="override-lease">
              <description>
                <text>Override assigned lease time with threshold</text>
              </description>
              <type name="empty"/>
            </leaf>
          </case>
        </choice>
      </container>
    </container>
    <choice name="requested-ip-check">
      <case name="case_1">
        <leaf name="requested-ip-network-match">
          <description>
            <text>Subnet to match server's address for active and giaddr for passive clients</text>
          </description>
          <default value="8"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="0 .. 31"/>
            </type>
          </type>
        </leaf>
      </case>
      <case name="case_2">
        <leaf name="requested-ip-interface-match">
          <description>
            <text>Use incoming-interface's subnet to check</text>
          </description>
          <type name="empty"/>
        </leaf>
      </case>
    </choice>
    <leaf name="no-snoop">
      <description>
        <text>Do not snoop DHCP packets</text>
      </description>
      <type name="empty"/>
    </leaf>
    <container name="allow-leasequery">
      <description>
        <text>Allow DHCP leasequery</text>
      </description>
      <presence value="enable allow-leasequery"/>
      <uses name="server-leasequery-type"/>
    </container>
    <container name="remote-id-mismatch">
      <description>
        <text>DHCP client remote-id mismatch</text>
      </description>
      <presence value="enable remote-id-mismatch"/>
      <uses name="dhcp-remote-id-mismatch-type"/>
    </container>
    <container name="reauthenticate">
      <description>
        <text>DHCP client reauthenticate processing</text>
      </description>
      <leaf name="lease-renewal">
        <description>
          <text>Reauthenticate on each renew, rebind, DISCOVER or SOLICIT</text>
        </description>
