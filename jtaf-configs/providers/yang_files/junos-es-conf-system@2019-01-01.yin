<?xml version="1.0" encoding="UTF-8"?>
<module name="junos-es-conf-system"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:jc-system="http://yang.juniper.net/junos-es/conf/system"
        xmlns:jt="http://yang.juniper.net/junos/common/types"
        xmlns:jc="http://yang.juniper.net/junos-es/conf/root">
  <namespace uri="http://yang.juniper.net/junos-es/conf/system"/>
  <prefix value="jc-system"/>
  <import module="junos-common-types">
    <prefix value="jt"/>
    <revision-date date="2019-01-01"/>
  </import>
  <import module="junos-es-conf-root">
    <prefix value="jc"/>
    <revision-date date="2019-01-01"/>
  </import>
  <organization>
    <text>Juniper Networks, Inc.</text>
  </organization>
  <contact>
    <text>yang-support@juniper.net</text>
  </contact>
  <description>
    <text>Junos system configuration module</text>
  </description>
  <revision date="2019-01-01">
    <description>
      <text>Junos: 21.3R1.9</text>
    </description>
  </revision>
  <augment target-node="/jc:configuration">
    <uses name="system-group"/>
  </augment>
  <augment target-node="/jc:configuration/jc:groups">
    <uses name="system-group"/>
  </augment>
  <grouping name="system-group">
    <container name="system">
      <description>
        <text>System parameters</text>
      </description>
      <uses name="juniper-system"/>
    </container>
  </grouping>
  <grouping name="juniper-system">
    <uses name="apply-advanced"/>
    <leaf name="host-name">
      <description>
        <text>Hostname for this router</text>
      </description>
      <type name="string">
        <length value="0 .. 255"/>
      </type>
    </leaf>
    <container name="root-authentication">
      <description>
        <text>Authentication information for the root login</text>
      </description>
      <uses name="authentication-object"/>
    </container>
    <container name="commit">
      <description>
        <text>Configuration commit management</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="commit-synchronize-server">
        <description>
          <text>Commit synchronize server configuration</text>
        </description>
        <uses name="apply-advanced"/>
        <container name="traceoptions">
          <description>
            <text>Traceoptions for commit synchronize server</text>
          </description>
          <uses name="apply-advanced"/>
          <leaf name="no-remote-trace">
            <description>
              <text>Disable remote tracing</text>
            </description>
            <type name="empty"/>
          </leaf>
          <container name="file">
            <description>
              <text>Trace file information</text>
            </description>
            <leaf name="filename">
              <description>
                <text>Name of file in which to write trace information</text>
              </description>
              <type name="string">
                <length value="1 .. 1024"/>
              </type>
            </leaf>
            <leaf name="size">
              <description>
                <text>Maximum trace file size</text>
              </description>
              <type name="string"/>
            </leaf>
            <leaf name="files">
              <description>
                <text>Maximum number of trace files</text>
              </description>
              <default value="3"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="2 .. 1000"/>
                </type>
              </type>
            </leaf>
            <choice name="world-readable-choice">
              <leaf name="world-readable">
                <description>
                  <text>Allow any user to read the log file</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="no-world-readable">
                <description>
                  <text>Don't allow any user to read the log file</text>
                </description>
                <type name="empty"/>
              </leaf>
            </choice>
            <leaf name="microsecond-stamp">
              <description>
                <text>Timestamp with microsecond granularity</text>
              </description>
              <type name="empty"/>
            </leaf>
          </container>
          <list name="flag">
            <key value="name"/>
            <description>
              <text>Tracing parameters</text>
            </description>
            <leaf name="name">
              <type name="enumeration">
                <enum name="ephemeral-commit">
                  <description>
                    <text>Trace ephemeral database commit synchronize requests</text>
                  </description>
                </enum>
                <enum name="operational-command">
                  <description>
                    <text>Trace operational command requests</text>
                  </description>
                </enum>
                <enum name="debug">
                  <description>
                    <text>Trace debug level information</text>
                  </description>
                </enum>
                <enum name="all">
                  <description>
                    <text>Trace all information</text>
                  </description>
                </enum>
              </type>
            </leaf>
          </list>
        </container>
      </container>
      <choice name="persist-groups-choice">
        <case name="case_1">
          <leaf name="persist-groups-inheritance">
            <description>
              <text>Build configuration groups inheritance path</text>
            </description>
            <type name="empty"/>
          </leaf>
        </case>
        <case name="case_2">
          <leaf name="no-persist-groups-inheritance">
            <description>
              <text>Don't build configuration groups inheritance path</text>
            </description>
            <type name="empty"/>
          </leaf>
        </case>
      </choice>
      <leaf name="ignore-xpath-failure">
        <description>
          <text>Ignore xpath failures during commit</text>
        </description>
        <status value="deprecated"/>
        <type name="empty"/>
      </leaf>
      <container name="server">
        <description>
          <text>Commit server (batch commit)</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="maximum-aggregate-pool">
          <description>
            <text>Maximum number of transactions to aggregate</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="1 .. 4294967295"/>
            </type>
          </type>
        </leaf>
        <leaf name="maximum-entries">
          <description>
            <text>Maximum number of transactions allowed in queue</text>
          </description>
          <type name="union">
            <type name="uint32"/>
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
          </type>
        </leaf>
        <leaf name="commit-interval">
          <description>
            <text>Number of seconds between commits</text>
          </description>
          <type name="union">
            <type name="uint32"/>
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
          </type>
        </leaf>
        <leaf name="retry-attempts">
          <description>
            <text>Retry attempts for commit failure due to db lock error</text>
          </description>
          <default value="5"/>
          <type name="union">
            <type name="uint32"/>
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
          </type>
        </leaf>
        <leaf name="retry-interval">
          <description>
            <text>Retry interval in seconds for commit failure</text>
          </description>
          <default value="20"/>
          <type name="union">
            <type name="uint32"/>
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
          </type>
        </leaf>
        <leaf name="days-to-keep-error-logs">
          <description>
            <text>Number of day to keep error log entries</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="1 .. 366"/>
            </type>
          </type>
        </leaf>
        <leaf name="redirect-completion-status">
          <description>
            <text>Redirect Async commit status to server configured here</text>
          </description>
          <type name="string"/>
        </leaf>
        <list name="commit-schedule-profile">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Scheduling profile for asynchronous low priority commits</text>
          </description>
          <leaf name="name">
            <description>
              <text>Schedule name for low priority commits</text>
            </description>
            <type name="string">
              <length value="1 .. 64"/>
            </type>
          </leaf>
          <uses name="apply-advanced"/>
          <leaf name="start-time">
            <description>
              <text>Time when the schedule starts processing low priority jobs (hh:mm)</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="end-time">
            <description>
              <text>Time when the schedule stops processing low priority jobs (hh:mm)</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="interruptible">
            <description>
              <text>Allow the low priority jobs to be interrupted during the schedule</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="load-average">
            <description>
              <text>Max load average of system at which schedule starts (last 1 min)</text>
            </description>
            <type name="jt:unsigned-float"/>
          </leaf>
        </list>
        <container name="traceoptions">
          <description>
            <text>Trace options for commit server</text>
          </description>
          <uses name="apply-advanced"/>
          <leaf name="no-remote-trace">
            <description>
              <text>Disable remote tracing</text>
            </description>
            <type name="empty"/>
          </leaf>
          <container name="file">
            <description>
              <text>Trace file information</text>
            </description>
            <leaf name="filename">
              <description>
                <text>Name of file in which to write trace information</text>
              </description>
              <type name="string">
                <length value="1 .. 1024"/>
              </type>
            </leaf>
            <leaf name="size">
              <description>
                <text>Maximum trace file size</text>
              </description>
              <type name="string"/>
            </leaf>
            <leaf name="files">
              <description>
                <text>Maximum number of trace files</text>
              </description>
              <default value="3"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="2 .. 1000"/>
                </type>
              </type>
            </leaf>
            <choice name="world-readable-choice">
              <leaf name="world-readable">
                <description>
                  <text>Allow any user to read the log file</text>
                </description>
                <type name="empty"/>
              </leaf>
              <leaf name="no-world-readable">
                <description>
                  <text>Don't allow any user to read the log file</text>
                </description>
                <type name="empty"/>
              </leaf>
            </choice>
            <leaf name="microsecond-stamp">
              <description>
                <text>Timestamp with microsecond granularity</text>
              </description>
              <type name="empty"/>
            </leaf>
          </container>
          <list name="flag">
            <key value="name"/>
            <description>
              <text>Tracing parameters</text>
            </description>
            <leaf name="name">
              <type name="enumeration">
                <enum name="all">
                  <description>
                    <text>Trace all operations</text>
                  </description>
                </enum>
                <enum name="commit-server">
                  <description>
                    <text>Trace commit server</text>
                  </description>
                </enum>
                <enum name="batch">
                  <description>
                    <text>Trace file batching</text>
                  </description>
                </enum>
                <enum name="configuration">
                  <description>
                    <text>Reading of configuration</text>
                  </description>
                </enum>
              </type>
            </leaf>
          </list>
        </container>
      </container>
      <leaf name="fast-synchronize">
        <description>
          <text>Parallelized commit synchronizing multiple routing-engines</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="synchronize">
        <description>
          <text>Synchronize commit on both Routing Engines by default</text>
        </description>
        <type name="empty"/>
      </leaf>
      <leaf name="delta-export">
        <description>
          <text>Export only delta configuration during commit</text>
        </description>
        <type name="empty"/>
      </leaf>
      <container name="notification">
        <presence value="enable notification"/>
        <description>
          <text>Notify applications upon commit complete</text>
        </description>
        <uses name="apply-advanced"/>
      </container>
      <leaf name="peers-synchronize">
        <description>
          <text>Synchronize commit on remote peers by default</text>
        </description>
        <type name="empty"/>
      </leaf>
      <list name="peers">
        <key value="name"/>
        <ordered-by value="user"/>
        <description>
          <text>Commit peers-synchronize details</text>
        </description>
        <max-elements value="3"/>
        <uses name="peers-type"/>
      </list>
    </container>
    <container name="configuration-database">
      <description>
        <text>Configuration database parameters</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="ephemeral">
        <presence value="enable ephemeral"/>
        <description>
          <text>Configure ephemeral database</text>
        </description>
        <uses name="apply-advanced"/>
        <list name="instance">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Configure ephemeral instances</text>
          </description>
          <max-elements value="8"/>
          <leaf name="name">
            <description>
              <text>Name of ephemeral instance</text>
            </description>
            <type name="string">
              <length value="1 .. 32"/>
            </type>
          </leaf>
          <uses name="apply-advanced"/>
        </list>
        <leaf name="commit-synchronize-model">
          <description>
            <text>Select commit synchronize model for ephemeral</text>
          </description>
          <default value="asynchronous"/>
          <type name="enumeration">
            <enum name="asynchronous">
              <description>
                <text>Commit synchronize on backup happens asynchronously</text>
              </description>
            </enum>
            <enum name="synchronous">
              <description>
                <text>Commit synchronize on backup happens synchronously</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="ignore-ephemeral-default">
          <description>
            <text>Ignore ephemeral default database</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="allow-commit-synchronize-with-gres">
          <description>
            <text>Allow ephemeral commit synchronize with GRES</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
      <container name="virtual-memory-mapping">
        <description>
          <text>Virtual memory mapping configuration</text>
        </description>
        <uses name="apply-advanced"/>
        <list name="process">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Per process configuration</text>
          </description>
          <leaf name="name">
            <description>
              <text>Name of the process</text>
            </description>
            <type name="string"/>
          </leaf>
          <uses name="apply-advanced"/>
          <leaf name="fixed-size">
            <description>
              <text>Fixed memory mapped size in kilobytes</text>
            </description>
            <units name="kilobytes"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="512 .. 696320"/>
              </type>
            </type>
          </leaf>
          <leaf name="page-pooling-size">
            <description>
              <text>Page pooling memory mapped size in kilobytes</text>
            </description>
            <units name="kilobytes"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="512 .. 696320"/>
              </type>
            </type>
          </leaf>
          <leaf name="page-leak-debug">
            <description>
              <text>Page leak detection</text>
            </description>
            <type name="empty"/>
          </leaf>
        </list>
        <container name="process-set">
          <description>
            <text>Set of processes using page pool</text>
          </description>
          <uses name="apply-advanced"/>
          <container name="subscriber-management">
            <presence value="enable subscriber-management"/>
            <description>
              <text>Subscriber management processes will use page pooling</text>
            </description>
            <uses name="apply-advanced"/>
            <leaf name="fixed-size">
              <description>
                <text>Fixed memory mapped size</text>
              </description>
              <units name="kilobytes"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="512 .. 696320"/>
                </type>
              </type>
            </leaf>
            <leaf name="page-pooling-size">
              <description>
                <text>Page pooling memory mapped size</text>
              </description>
              <units name="kilobytes"/>
              <type name="union">
                <type name="string">
                  <pattern value="&lt;.*&gt;|$.*"/>
                </type>
                <type name="uint32">
                  <range value="512 .. 696320"/>
                </type>
              </type>
            </leaf>
          </container>
        </container>
      </container>
      <leaf name="extend-size">
        <description>
          <text>Extend configuration database upto 1.5G</text>
        </description>
        <type name="empty"/>
      </leaf>
      <container name="resize">
        <presence value="enable resize"/>
        <description>
          <text>Resize configuration database</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="database-size-on-disk">
          <description>
            <text>Minimum configuration database size on disk</text>
          </description>
          <default value="450"/>
          <units name="Megabytes"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="1 .. 1024"/>
            </type>
          </type>
        </leaf>
        <leaf name="database-size-diff">
          <description>
            <text>Difference between database size and actual usage</text>
          </description>
          <default value="100"/>
          <units name="Megabytes"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="1 .. 1024"/>
            </type>
          </type>
        </leaf>
      </container>
    </container>
    <container name="scripts">
      <description>
        <text>Scripting mechanisms</text>
      </description>
      <uses name="scripts_type"/>
    </container>
    <container name="login">
      <description>
        <text>Names, login classes, and passwords for users</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="retry-options">
        <description>
          <text>Configure password retry options</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="tries-before-disconnect">
          <description>
            <text>Number of times user is allowed to try password</text>
          </description>
          <default value="3"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="2 .. 10"/>
            </type>
          </type>
        </leaf>
        <leaf name="backoff-threshold">
          <description>
            <text>Number of password failures before delay is introduced</text>
          </description>
          <default value="2"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="1 .. 3"/>
            </type>
          </type>
        </leaf>
        <leaf name="backoff-factor">
          <description>
            <text>Delay factor after 'backoff-threshold' password failures</text>
          </description>
          <default value="5"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="5 .. 10"/>
            </type>
          </type>
        </leaf>
        <leaf name="minimum-time">
          <description>
            <text>Minimum total connection time if all attempts fail</text>
          </description>
          <default value="20"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="20 .. 60"/>
            </type>
          </type>
        </leaf>
        <leaf name="maximum-time">
          <description>
            <text>Maximum time the connection will remain for user to enter username and password</text>
          </description>
          <default value="120"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="20 .. 300"/>
            </type>
          </type>
        </leaf>
        <leaf name="lockout-period">
          <description>
            <text>Amount of time user account is locked after 'tries-before-disconnect' failures</text>
          </description>
          <units name="minutes"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="1 .. 43200"/>
            </type>
          </type>
        </leaf>
      </container>
      <leaf name="idle-timeout">
        <description>
          <text>Maximum idle time before logout</text>
        </description>
        <units name="minutes"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 60"/>
          </type>
        </type>
      </leaf>
      <list name="class">
        <key value="name"/>
        <description>
          <text>Login class</text>
        </description>
        <uses name="login-class-object"/>
      </list>
      <list name="user">
        <key value="name"/>
        <description>
          <text>Username</text>
        </description>
        <uses name="login-user-object"/>
      </list>
      <container name="password">
        <description>
          <text>Password configuration</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="minimum-character-changes">
          <description>
            <text>Minimum number of character changes between old and new passwords</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="int32">
              <range value="4 .. 15"/>
            </type>
          </type>
        </leaf>
        <leaf name="minimum-reuse">
          <description>
            <text>Minimum number of old passwords which should not be same as the new password</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="int32">
              <range value="1 .. 20"/>
            </type>
          </type>
        </leaf>
        <leaf name="maximum-lifetime">
          <description>
            <text>Maximum password lifetime in days</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="int32">
              <range value="30 .. 365"/>
            </type>
          </type>
        </leaf>
        <leaf name="minimum-lifetime">
          <description>
            <text>Minimum password lifetime in days</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="int32">
              <range value="1 .. 30"/>
            </type>
          </type>
        </leaf>
        <leaf name="minimum-length">
          <description>
            <text>Minimum password length for all users</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="int32">
              <range value="6 .. 20"/>
            </type>
          </type>
        </leaf>
        <leaf name="maximum-length">
          <description>
            <text>Maximum password length for all users</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="int32">
              <range value="20 .. 128"/>
            </type>
          </type>
        </leaf>
        <leaf name="change-type">
          <description>
            <text>Password change type</text>
          </description>
          <type name="enumeration">
            <enum name="character-sets">
              <description>
                <text>Count number of character type</text>
              </description>
            </enum>
            <enum name="set-transitions">
              <description>
                <text>Count changes in character type</text>
              </description>
