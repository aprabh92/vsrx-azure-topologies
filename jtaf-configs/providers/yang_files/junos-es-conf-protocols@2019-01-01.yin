        </description>
        <leaf name="name">
          <description>
            <text>Root address of the P2MP LSP</text>
          </description>
          <type name="jt:ipaddr"/>
        </leaf>
        <uses name="apply-advanced"/>
        <list name="lsp-id">
          <key value="name"/>
          <description>
            <text>Configure the generic LSP identifier</text>
          </description>
          <leaf name="name">
            <description>
              <text>Generic LSP identifier value</text>
            </description>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="1 .. 65535"/>
              </type>
            </type>
          </leaf>
          <uses name="apply-advanced"/>
        </list>
        <list name="group-address">
          <key value="name"/>
          <description>
            <text>IPv4/Ipv6 group address for mLDP LSP</text>
          </description>
          <leaf name="name">
            <description>
              <text>IPv4/Ipv6 group address</text>
            </description>
            <type name="jt:ipaddr"/>
          </leaf>
          <uses name="apply-advanced"/>
          <list name="source-address">
            <key value="name"/>
            <description>
              <text>IPv4/Ipv6 source address</text>
            </description>
            <leaf name="name">
              <description>
                <text>IPv4/Ipv6 source address for mLDP LSP</text>
              </description>
              <type name="jt:ipaddr"/>
            </leaf>
            <uses name="apply-advanced"/>
          </list>
        </list>
      </list>
      <leaf name="no-rsvp-tunneling">
        <description>
          <text>Do not allow LDP P2MP to use RSVP-TE LSPs for tunneling</text>
        </description>
        <type name="empty"/>
      </leaf>
    </container>
    <container name="sr-mapping-client">
      <presence value="enable sr-mapping-client"/>
      <description>
        <text>Enable LDP to SR mapping-client functionality</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf-list name="policy">
        <ordered-by value="user"/>
        <description>
          <text>SR mapping-client policy</text>
        </description>
        <type name="jt:policy-algebra"/>
      </leaf-list>
    </container>
    <leaf name="upstream-label-assignment">
      <description>
        <text>Allow Upstream Label Assignment capability</text>
      </description>
      <type name="empty"/>
    </leaf>
    <list name="family">
      <key value="name"/>
      <ordered-by value="user"/>
      <description>
        <text>Address family</text>
      </description>
      <leaf name="name">
        <type name="enumeration">
          <enum name="inet">
            <description>
              <text>IPv4 family</text>
            </description>
          </enum>
          <enum name="inet6">
            <description>
              <text>IPv6 family</text>
            </description>
          </enum>
        </type>
      </leaf>
      <uses name="apply-advanced"/>
    </list>
    <leaf name="transport-preference">
      <description>
        <text>TCP transport preference</text>
      </description>
      <type name="enumeration">
        <enum name="ipv4">
          <description>
            <text>Use IPv4 for TCP connections</text>
          </description>
        </enum>
        <enum name="ipv6">
          <description>
            <text>Use IPv6 for TCP connections</text>
          </description>
        </enum>
      </type>
    </leaf>
    <container name="dual-transport">
      <presence value="enable dual-transport"/>
