      </description>
      <uses name="apply-advanced"/>
      <leaf name="inet-lsr-id">
        <description>
          <text>LSR identifier for address family inet</text>
        </description>
        <type name="jt:ipv4addr"/>
      </leaf>
      <leaf name="inet6-lsr-id">
        <description>
          <text>LSR identifier for address family inet6</text>
        </description>
        <type name="jt:ipv4addr"/>
      </leaf>
