          </description>
          <type name="empty"/>
        </leaf>
        <container name="failure-action">
          <description>
            <text>Action to take when BFD session goes down</text>
          </description>
          <choice name="action-choice">
            <case name="case_1">
              <leaf name="remove-route">
                <description>
                  <text>Remove LDP route from the ribs</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
            <case name="case_2">
              <leaf name="remove-nexthop">
                <description>
                  <text>Remove LDP nexthop from the route</text>
                </description>
                <type name="empty"/>
              </leaf>
            </case>
          </choice>
        </container>
        <leaf name="holddown-interval">
          <description>
            <text>Time to hold the session-UP notification to the client</text>
          </description>
          <default value="0"/>
          <units name="milliseconds"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="0 .. 255000"/>
            </type>
          </type>
        </leaf>
        <leaf name="no-router-alert-option">
          <description>
            <text>Do not set Router-Alert options in IP header for MPLS-BFD</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="use-ip-ttl-1">
          <description>
            <text>Set TTL value to 1 in IP header for MPLS-BFD</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
      <container name="periodic-traceroute">
        <presence value="enable periodic-traceroute"/>
        <description>
          <text>Configure periodic traceroute</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="frequency">
          <description>
            <text>Time between traceroute attempts</text>
          </description>
          <units name="minutes"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint16">
              <range value="15 .. 120"/>
            </type>
          </type>
        </leaf>
        <leaf name="ttl">
          <description>
            <text>Maximum time-to-live value</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint16">
              <range value="1 .. 255"/>
            </type>
          </type>
        </leaf>
        <leaf name="retries">
          <description>
            <text>Number of times to resend probe</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint16">
              <range value="1 .. 9"/>
            </type>
          </type>
        </leaf>
        <leaf name="wait">
          <description>
            <text>Time to wait before resending probe</text>
          </description>
          <units name="seconds"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint16">
              <range value="5 .. 15"/>
            </type>
          </type>
        </leaf>
        <leaf name="paths">
          <description>
            <text>Maximum number of paths to traverse</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint16">
              <range value="1 .. 255"/>
            </type>
          </type>
        </leaf>
        <leaf name="source">
          <description>
            <text>Source address to use when sending probes</text>
          </description>
          <type name="jt:ipv4addr"/>
        </leaf>
        <leaf name="exp">
          <description>
            <text>Class-of-service value to use when sending probes</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint16">
              <range value="0 .. 7"/>
            </type>
          </type>
        </leaf>
        <leaf name="fanout">
          <description>
            <text>Maximum number of nexthops to search per node</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint16">
              <range value="1 .. 64"/>
            </type>
          </type>
        </leaf>
      </container>
      <leaf name="lsp-ping-interval">
        <description>
          <text>Time interval between LSP ping messages</text>
        </description>
        <default value="60"/>
        <units name="seconds"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint16">
            <range value="30 .. 3600"/>
          </type>
        </type>
      </leaf>
      <leaf name="lsp-ping-multiplier">
        <description>
          <text>Number of ping reply missed before declaring BFD down</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint8">
            <range value="1 .. 5"/>
          </type>
        </type>
      </leaf>
    </container>
    <container name="targeted-hello">
      <description>
        <text>Configure targeted hello parameters</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="hello-interval">
        <description>
          <text>Hello interval (seconds)</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 65535"/>
          </type>
        </type>
      </leaf>
      <leaf name="hold-time">
        <description>
          <text>Hold interval (seconds)</text>
        </description>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="1 .. 65535"/>
          </type>
        </type>
      </leaf>
    </container>
    <container name="p2mp">
      <presence value="enable p2mp"/>
      <description>
        <text>Advertise P2MP capability to peers</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="recursive">
        <description>
          <text>Configure P2MP recursive parameters</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="route">
          <description>
