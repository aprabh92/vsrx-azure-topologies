<?xml version="1.0" encoding="UTF-8"?>
<module name="junos-es-conf-protocols"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:jc-protocols="http://yang.juniper.net/junos-es/conf/protocols"
        xmlns:jt="http://yang.juniper.net/junos/common/types"
        xmlns:jc="http://yang.juniper.net/junos-es/conf/root">
  <namespace uri="http://yang.juniper.net/junos-es/conf/protocols"/>
  <prefix value="jc-protocols"/>
  <import module="junos-common-types">
    <prefix value="jt"/>
    <revision-date date="2019-01-01"/>
  </import>
  <import module="junos-es-conf-root">
    <prefix value="jc"/>
    <revision-date date="2019-01-01"/>
  </import>
  <organization>
    <text>Juniper Networks, Inc.</text>
  </organization>
  <contact>
    <text>yang-support@juniper.net</text>
  </contact>
  <description>
    <text>Junos protocols configuration module</text>
  </description>
  <revision date="2019-01-01">
    <description>
      <text>Junos: 21.3R1.9</text>
    </description>
  </revision>
  <augment target-node="/jc:configuration">
    <uses name="protocols-group"/>
  </augment>
  <augment target-node="/jc:configuration/jc:groups">
    <uses name="protocols-group"/>
  </augment>
  <grouping name="protocols-group">
    <container name="protocols">
      <description>
        <text>Routing protocol configuration</text>
      </description>
      <uses name="juniper-protocols"/>
    </container>
  </grouping>
  <grouping name="juniper-protocols">
    <uses name="apply-advanced"/>
    <container name="router-advertisement">
      <description>
        <text>IPv6 router advertisement options</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="traceoptions">
        <description>
          <text>Trace options for router advertisement</text>
        </description>
        <uses name="apply-advanced"/>
        <container name="file">
          <description>
            <text>Trace file options</text>
          </description>
          <leaf name="filename">
            <description>
              <text>Name of file in which to write trace information</text>
            </description>
            <type name="string">
              <length value="1 .. 1024"/>
            </type>
          </leaf>
          <leaf name="replace">
            <description>
              <text>Replace trace file rather than appending to it</text>
            </description>
            <status value="deprecated"/>
            <type name="empty"/>
          </leaf>
          <leaf name="size">
            <description>
              <text>Maximum trace file size</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="files">
            <description>
              <text>Maximum number of trace files</text>
            </description>
            <default value="10"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="2 .. 1000"/>
              </type>
            </type>
          </leaf>
          <leaf name="no-stamp">
            <description>
              <text>Do not timestamp trace file</text>
            </description>
            <status value="deprecated"/>
            <type name="empty"/>
          </leaf>
          <choice name="world-readable-choice">
            <leaf name="world-readable">
              <description>
                <text>Allow any user to read the log file</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="no-world-readable">
              <description>
                <text>Don't allow any user to read the log file</text>
              </description>
              <type name="empty"/>
            </leaf>
          </choice>
        </container>
        <list name="flag">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Tracing parameters</text>
          </description>
          <leaf name="name">
            <type name="enumeration">
              <enum name="route">
                <description>
                  <text>Trace routing information</text>
                </description>
              </enum>
              <enum name="normal">
                <description>
                  <text>Trace normal events</text>
                </description>
              </enum>
              <enum name="general">
                <description>
                  <text>Trace general events</text>
                </description>
              </enum>
              <enum name="state">
                <description>
                  <text>Trace state transitions</text>
                </description>
              </enum>
              <enum name="policy">
                <description>
                  <text>Trace policy processing</text>
                </description>
              </enum>
              <enum name="task">
                <description>
                  <text>Trace routing protocol task processing</text>
                </description>
              </enum>
              <enum name="timer">
                <description>
                  <text>Trace routing protocol timer processing</text>
                </description>
              </enum>
              <enum name="all">
                <description>
                  <text>Trace everything</text>
                </description>
              </enum>
            </type>
          </leaf>
        </list>
      </container>
      <list name="interface">
        <key value="name"/>
        <ordered-by value="user"/>
        <description>
          <text>Interfaces on which to configure router advertisement</text>
        </description>
        <leaf name="name">
          <type name="string"/>
        </leaf>
        <uses name="apply-advanced"/>
        <leaf name="preference">
          <description>
            <text>Set the Preference for Router Selection</text>
          </description>
          <type name="enumeration">
            <enum name="medium">
              <description>
                <text>Medium / (Default)</text>
              </description>
            </enum>
            <enum name="high">
              <description>
                <text>High</text>
              </description>
            </enum>
            <enum name="low">
              <description>
                <text>Low</text>
              </description>
            </enum>
          </type>
        </leaf>
        <leaf name="max-advertisement-interval">
          <description>
            <text>Maximum advertisement interval</text>
          </description>
          <units name="seconds"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="4 .. 1800"/>
            </type>
          </type>
        </leaf>
        <leaf name="min-advertisement-interval">
          <description>
            <text>Minimum advertisement interval</text>
          </description>
          <units name="seconds"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="3 .. 1350"/>
            </type>
          </type>
        </leaf>
        <choice name="managed-configuration-choice">
          <leaf name="managed-configuration">
            <description>
              <text>Set managed address configuration</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="no-managed-configuration">
            <description>
              <text>Don't set managed address configuration</text>
            </description>
            <type name="empty"/>
          </leaf>
        </choice>
        <choice name="other-stateful-configuration-choice">
          <leaf name="other-stateful-configuration">
            <description>
              <text>Set other stateful configuration</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="no-other-stateful-configuration">
            <description>
              <text>Don't set other stateful configuration</text>
            </description>
            <type name="empty"/>
          </leaf>
        </choice>
        <choice name="link-mtu-choice">
          <leaf name="link-mtu">
            <description>
              <text>Link MTU</text>
            </description>
            <type name="empty"/>
          </leaf>
          <leaf name="no-link-mtu">
            <description>
              <text>Don't link MTU</text>
            </description>
            <type name="empty"/>
          </leaf>
        </choice>
        <leaf name="solicit-router-advertisement-unicast">
          <description>
            <text>Enbale solicited router advertisement as unicast</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="reachable-time">
          <description>
            <text>Reachable time</text>
          </description>
          <units name="milliseconds"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="0 .. 3600000"/>
            </type>
          </type>
        </leaf>
        <leaf name="retransmit-timer">
          <description>
            <text>Retransmit timer</text>
          </description>
          <units name="milliseconds"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="0 .. 4294967295"/>
            </type>
          </type>
        </leaf>
        <leaf name="virtual-router-only">
          <description>
            <text>Send advertisemnets only for vrrp-inet6-group</text>
          </description>
          <type name="empty"/>
        </leaf>
        <leaf name="current-hop-limit">
          <description>
            <text>Current hop limit</text>
          </description>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="0 .. 255"/>
            </type>
          </type>
        </leaf>
        <leaf name="default-lifetime">
          <description>
            <text>Router lifetime</text>
          </description>
          <units name="seconds"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="0 .. 9000"/>
            </type>
          </type>
        </leaf>
        <list name="dns-server-address">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Recursive DNS address configuration</text>
          </description>
          <max-elements value="3"/>
          <leaf name="name">
            <description>
              <text>DNS address to be advertised</text>
            </description>
            <type name="string"/>
          </leaf>
          <uses name="apply-advanced"/>
          <leaf name="lifetime">
            <description>
              <text>DNS address lifetime</text>
            </description>
            <default value="1800"/>
            <units name="seconds"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="0 .. 4294967295"/>
              </type>
            </type>
          </leaf>
        </list>
        <list name="prefix">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Prefix configuration</text>
          </description>
          <leaf name="name">
            <description>
              <text>Prefix to be advertised</text>
            </description>
            <type name="jt:ipv6prefix"/>
          </leaf>
          <uses name="apply-advanced"/>
          <leaf name="valid-lifetime">
            <description>
              <text>Valid lifetime (fixed)</text>
            </description>
            <units name="seconds"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="0 .. 4294967295"/>
              </type>
            </type>
          </leaf>
          <choice name="on-link-choice">
            <leaf name="on-link">
              <description>
                <text>Set on-link flag</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="no-on-link">
              <description>
                <text>Don't set on-link flag</text>
              </description>
              <type name="empty"/>
            </leaf>
          </choice>
          <leaf name="preferred-lifetime">
            <description>
              <text>Preferred lifetime (fixed)</text>
            </description>
            <units name="seconds"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="0 .. 4294967295"/>
              </type>
            </type>
          </leaf>
          <choice name="autonomous-choice">
            <leaf name="autonomous">
              <description>
                <text>Set autonomous flag</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="no-autonomous">
              <description>
                <text>Don't set autonomous flag</text>
              </description>
              <type name="empty"/>
            </leaf>
          </choice>
        </list>
        <list name="dns-search-list">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>DNS search list configuration</text>
          </description>
          <leaf name="name">
            <description>
              <text>DNS search list name</text>
            </description>
            <type name="string"/>
          </leaf>
          <uses name="apply-advanced"/>
          <leaf name="lifetime">
            <description>
              <text>DNS search list lifetime</text>
            </description>
            <default value="5400"/>
            <units name="seconds"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="0 .. 4294967295"/>
              </type>
            </type>
          </leaf>
        </list>
        <list name="rio-prefix">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Route information option configuration</text>
          </description>
          <leaf name="name">
            <description>
              <text>Prefix to be advertised</text>
            </description>
            <type name="jt:ipv6prefix"/>
          </leaf>
          <uses name="apply-advanced"/>
          <leaf name="rio-lifetime">
            <description>
              <text>Route information lifetime</text>
            </description>
            <units name="seconds"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="0 .. 4294967295"/>
              </type>
            </type>
          </leaf>
          <leaf name="rio-preference">
            <description>
              <text>Set the preference for route information</text>
            </description>
            <type name="enumeration">
              <enum name="medium">
                <description>
                  <text>Medium / Default</text>
                </description>
              </enum>
              <enum name="high">
                <description>
                  <text>High</text>
                </description>
              </enum>
              <enum name="low">
                <description>
                  <text>Low</text>
                </description>
              </enum>
            </type>
          </leaf>
        </list>
      </list>
      <container name="ra-secure">
        <description>
          <text>Protect box against rogue incoming RA messages</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="accept-current-hop-limit-min">
          <description>
            <text>Current hop limit acceptable min for incoming RA</text>
          </description>
          <units name="uint"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="1 .. 255"/>
            </type>
          </type>
        </leaf>
        <leaf name="accept-current-hop-limit-max">
          <description>
            <text>Current hop acceptable min for incoming RA</text>
          </description>
          <units name="uint"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="1 .. 255"/>
            </type>
          </type>
        </leaf>
        <leaf name="accept-reachable-time-min">
          <description>
            <text>Reachable Time acceptable min for incoming RA</text>
          </description>
          <units name="milliseconds"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="1000 .. 3600000"/>
            </type>
          </type>
        </leaf>
        <leaf name="accept-reachable-time-max">
          <description>
            <text>Reachable Time acceptable max for incoming RA</text>
          </description>
          <units name="milliseconds"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="1000 .. 3600000"/>
            </type>
          </type>
        </leaf>
        <leaf name="accept-retransmit-time-min">
          <description>
            <text>Retransmit Time acceptable min for incoming RA</text>
          </description>
          <units name="milliseconds"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="1000 .. 3600000"/>
            </type>
          </type>
        </leaf>
        <leaf name="accept-retransmit-time-max">
          <description>
            <text>Retransmit Time acceptable min for incoming RA</text>
          </description>
          <units name="milliseconds"/>
          <type name="union">
            <type name="string">
              <pattern value="&lt;.*&gt;|$.*"/>
            </type>
            <type name="uint32">
              <range value="1000 .. 3600000"/>
            </type>
          </type>
        </leaf>
      </container>
    </container>
    <container name="neighbor-discovery">
      <presence value="enable neighbor-discovery"/>
      <description>
        <text>IPv6 neighbor discovery</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="onlink-subnet-only">
        <description>
          <text>Onlink subnet only knob</text>
        </description>
        <type name="empty"/>
      </leaf>
      <container name="ndp-proxy">
        <description>
          <text>Configure NDP PROXY behaviour</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="no-proxy-on-resolve">
          <description>
            <text>Disable proxy on unresolved address</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
      <container name="dad-proxy">
        <description>
          <text>Configure DAD PROXY behaviour</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="no-proxy-on-resolve">
          <description>
            <text>Disable proxy on unresolved address</text>
          </description>
          <type name="empty"/>
        </leaf>
      </container>
    </container>
    <container name="iccp">
      <description>
        <text>ICCP options</text>
      </description>
      <uses name="apply-advanced"/>
      <leaf name="local-ip-addr">
        <description>
          <text>Local IP address to use by default for all peers</text>
        </description>
        <type name="jt:ipv4addr"/>
      </leaf>
      <leaf name="session-establishment-hold-time">
        <description>
          <text>Time within which connection must succeed with peers</text>
        </description>
        <default value="300"/>
        <units name="seconds"/>
        <type name="union">
          <type name="string">
            <pattern value="&lt;.*&gt;|$.*"/>
          </type>
          <type name="uint32">
            <range value="45 .. 600"/>
          </type>
        </type>
      </leaf>
      <leaf name="authentication-key">
        <description>
          <text>MD5 authentication key for all peers</text>
        </description>
        <type name="string">
          <length value="1 .. 126"/>
        </type>
      </leaf>
      <list name="peer">
        <key value="name"/>
        <description>
          <text>Redundancy Group Configuration</text>
        </description>
        <uses name="peer-group"/>
      </list>
      <container name="traceoptions">
        <description>
          <text>Trace options for ICCP</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="no-remote-trace">
          <description>
            <text>Disable remote tracing</text>
          </description>
          <type name="empty"/>
        </leaf>
        <container name="file">
          <description>
            <text>Trace file information</text>
          </description>
          <leaf name="filename">
            <description>
              <text>Name of file in which to write trace information</text>
            </description>
            <type name="string">
              <length value="1 .. 1024"/>
            </type>
          </leaf>
          <leaf name="size">
            <description>
              <text>Maximum trace file size</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="files">
            <description>
              <text>Maximum number of trace files</text>
            </description>
            <default value="3"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="2 .. 1000"/>
              </type>
            </type>
          </leaf>
          <choice name="world-readable-choice">
            <leaf name="world-readable">
              <description>
                <text>Allow any user to read the log file</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="no-world-readable">
              <description>
                <text>Don't allow any user to read the log file</text>
              </description>
              <type name="empty"/>
            </leaf>
          </choice>
          <leaf name="match">
            <description>
              <text>Regular expression for lines to be logged</text>
            </description>
            <type name="jt:regular-expression"/>
          </leaf>
        </container>
        <list name="flag">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Trace flag information</text>
          </description>
          <leaf name="name">
            <type name="enumeration">
              <enum name="error">
                <description>
                  <text>Trace all errors</text>
                </description>
              </enum>
              <enum name="event">
                <description>
                  <text>Trace all events</text>
                </description>
              </enum>
              <enum name="packet">
                <description>
                  <text>Trace all packets</text>
                </description>
              </enum>
              <enum name="pipe">
                <description>
                  <text>Trace pipe messages</text>
                </description>
              </enum>
              <enum name="pipe-detail">
                <description>
                  <text>Trace pipe messages in detail</text>
                </description>
              </enum>
              <enum name="all">
                <description>
                  <text>Trace everything</text>
                </description>
              </enum>
            </type>
          </leaf>
        </list>
      </container>
    </container>
    <container name="ilmi">
      <description>
        <text>Interim Local Management Interface Protocol configuration</text>
      </description>
      <uses name="apply-advanced"/>
      <container name="traceoptions">
        <description>
          <text>ILMI trace options</text>
        </description>
        <uses name="apply-advanced"/>
        <leaf name="no-remote-trace">
          <description>
            <text>Disable remote tracing</text>
          </description>
          <type name="empty"/>
        </leaf>
        <container name="file">
          <description>
            <text>Trace file information</text>
          </description>
          <leaf name="filename">
            <description>
              <text>Name of file in which to write trace information</text>
            </description>
            <type name="string">
              <length value="1 .. 1024"/>
            </type>
          </leaf>
          <leaf name="size">
            <description>
              <text>Maximum trace file size</text>
            </description>
            <type name="string"/>
          </leaf>
          <leaf name="files">
            <description>
              <text>Maximum number of trace files</text>
            </description>
            <default value="3"/>
            <type name="union">
              <type name="string">
                <pattern value="&lt;.*&gt;|$.*"/>
              </type>
              <type name="uint32">
                <range value="2 .. 1000"/>
              </type>
            </type>
          </leaf>
          <choice name="world-readable-choice">
            <leaf name="world-readable">
              <description>
                <text>Allow any user to read the log file</text>
              </description>
              <type name="empty"/>
            </leaf>
            <leaf name="no-world-readable">
              <description>
                <text>Don't allow any user to read the log file</text>
              </description>
              <type name="empty"/>
            </leaf>
          </choice>
          <leaf name="match">
            <description>
              <text>Regular expression for lines to be logged</text>
            </description>
            <type name="jt:regular-expression"/>
          </leaf>
        </container>
        <list name="flag">
          <key value="name"/>
          <ordered-by value="user"/>
          <description>
            <text>Tracing parameters</text>
          </description>
          <leaf name="name">
            <type name="enumeration">
              <enum name="database">
                <description>
                  <text>Trace database events</text>
                </description>
              </enum>
              <enum name="routing-socket">
                <description>
                  <text>Trace Routing socket events</text>
                </description>
              </enum>
              <enum name="state">
                <description>
                  <text>Trace state change events</text>
                </description>
              </enum>
              <enum name="debug">
                <description>
                  <text>Trace debug messages</text>
                </description>
              </enum>
              <enum name="event">
                <description>
